;-------------------------------------------------
;26, 弁当
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_KX1_26
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;地の文呼び出し(カラー変更対応)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;最初から弁当を食べる時
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 1
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;売るのをやめて食べる時
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 2
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;主人が弁当を作るとき
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 3
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;食後の運動(性的な意味で)直前
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 4
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;毒を盛られた
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 5
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;毒を見抜いたので弱みを握るイベント（相性のいい子に食べさせるver）
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 6
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;毒を見抜いたので弱みを握るイベント（問い詰めるver）
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 7
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;毒殺された
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 8
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;一般
	PRINTFORMW 
	PRINTFORMW 
	RETURN LOCAL
ENDIF

