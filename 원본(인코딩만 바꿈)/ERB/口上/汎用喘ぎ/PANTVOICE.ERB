;----------------------------------------------------------------------
;汎用喘ぎ別人版ERB
;ver0.02(10/11/28)

;>>118さん版または別人版0.01verでは「PANT_E |= 2とするとみさくら語が表示されなくなる」と説明していましたが、実際にはPANT_E |= 2とするとみさくら語が表示されるようになってました
;以前のバージョンを使っている方でみさくら語を禁止したい方はPANT_E |= 2を消してください。
;絶叫系喘ぎとみさくら語を禁止してた方はPANT_E |= 3をPANT_E |= 1にしてください。
;----------------------------------------------------------------------
;【使い方】
;まずキャラ口上ERBのどこかに以下の文を入れる。
;変数に入れる数字については下記参照。
;------------ここから-------------
;@PANT_VOICE_KX1
;	PANT_A = (1～5の数字)
;	PANT_E = (0～7の数字／特に付けたいオプションがなければ0でよい)
;CALL PANT_VOICE
;RETURN 0
;------------ここまで-------------
;X1にはキャラ番号を入れる。
;あとは喘がせたい場所で
;------------ここから-------------
;CALL PANT_VOICE_KX1
;------------ここまで-------------
;と書くと喘ぎます。
;回数または喘ぎパターンを指定したい場合は
;------------ここから-------------
;PANT_F = (1～10の数字)
;PANT_G = (1～7の数字)
;CALL PANT_VOICE_KX1
;------------ここまで-------------
;と書くと、指定した喘ぎパターンでPANT_F回喘ぎを反復します。
;いちいちPANT_F=なんちゃらとか書くのめんどくさいという方は
;------------ここから-------------
;@PANTVOICE_1_KX1
;	PANT_A = (1～5の数字)
;	PANT_E = (0～7の数字／特に付けたいオプションがなければ0でよい)
;	PANT_F = (1～10の数字)
;	PANT_G = (1～7の数字)
;CALL PANT_VOICE
;RETURN 0
;------------ここまで-------------
;と書いてCALL PANTVOICE_1_KX1とかすると良いと思います。
;記述例はreadme別人版に書いてあるのでそちらも参照ください。

;----------------------------------------------------------------------
;変数使用状況
;PANT_A = キャラ口調（現在5種）
;PANT_B = 喘ぎ回数の多さ
;PANT_C = 感度
;PANT_D = 分岐用ビット
;PANT_E = オプション追加
;PANT_F = 喘ぎ回数指定用
;PANT_G = 喘ぎの種類指定用
;PANT_P = ランダム用

;----------------------------------------------------------------------
;変数初期化
;----------------------------------------------------------------------
@PANT_VOICE
PANT_B = 0
PANT_C = 0
PANT_D = 0
PANT_P:1 = (-1)
PANT_B_CALC = 0
PANT_C_CALC = 0

;----------------------------------------------------------------------
;変数と対応数字一覧
;----------------------------------------------------------------------
;PANT_Aに入れるもの
;乱暴=1
;標準=2
;柔らかめ=3
;敬語=4
;関西弁=5

;PANT_Bに入ってるもの
;PANT_F=0のときの喘ぎ反復回数

;PANT_Cに入ってるもの
;PANT_G=0のときの喘ぎパターンを決定する数値

;PANT_Dに入ってるもの
;崩壊=1=0
;反発1=2=1
;反発2=4=2
;反発3=8=3
;屈服1=16=4
;屈服2=32=5
;屈服3=64=6
;恋慕=128=7
;淫乱=256=8
;服従=512=9
;親愛=1024=10
;娼婦=2048=11
;隷属=4096=12
;口=8192=13
;B=16384=14
;C=32768=15
;A=65536=16
;V=131072=17
;その他=262144=18
;本番=524288=19
;触手=1048576=20
;道具=2097152=21
;苦痛1=4194304=22
;苦痛2=8388608=23
;奉仕系=16777216=24
;奴隷性別男=33554432=25
;慣らさず濡らさず突っこんだ==26

;PANT_Eに入れるもの
;絶叫系喘ぎ禁止=1
;(↑を入れると苦痛LV2になる状況でも苦痛LV1の喘ぎが、快楽LV4になる状況でも快楽LV3の喘ぎが出るようになる)
;みさくら語で喘ぐ(現状らめぇくらいしかセリフが増えない)=2
;淫語で喘ぐ(ちんぽ汁出るうううとか)=4
;淫語で喘ぐモードは一つひとつのセリフが長めなので、PANT_F = 4以下くらいで喘ぎ回数を指定しておいたほうがいいかもしれません。
;PANT_E = 0 :オプションなし
;PANT_E = 1 :絶叫系喘ぎ禁止
;PANT_E = 2 :みさくら語モード
;PANT_E = 3 :絶叫系喘ぎ禁止＋みさくら語モード
;PANT_E = 4 :淫語モード
;PANT_E = 5 :絶叫系喘ぎ禁止＋淫語モード
;PANT_E = 6 :みさくら語モード＋淫語モード
;PANT_E = 7 :絶叫系喘ぎ禁止＋みさくら語モード＋淫語モード

;PANT_Fに入れるもの
;1～10までの数字
;入れた数字の分だけ喘ぎを反復するようになる
;0または負の数を入れるとPANT_Fに何も入れなかったときと同じ動きをし、
;10より大きい数字を入れると喘ぎ反復回数は10回になる

;PANT_Gに入れるもの
;1～7までの数字
;入れた数字に対応する喘ぎパターンで喘ぐようになる
;1～7以外の数字を入れると何も入れなかったのと同じ動きをする
;弱快感=1
;快感LV2=2
;快感LV3=3
;快感LV4=4
;苦痛LV1=5
;苦痛LV2=6
;口が塞がってる(ボールギャグ・触手口虐中以外)=7

;----------------------------------------------------------------------
;素質によってB喘ぐ回数とC感度にプラスマイナス。
;上がり下がりは自分の想像です。
;感度や苦痛に関わる素質は最後に。
;----------------------------------------------------------------------
;男である
SIF TALENT:オトコ == 1
	PANT_D |= 33554432
;反抗的
IF TALENT:反抗的 == 1
	PANT_B -= 1
	PANT_C -= 1
ENDIF
;気丈
SIF TALENT:気丈 == 1
	PANT_B -= 1
;素直
SIF TALENT:素直 == 1
	PANT_B += 1
;プライド高い
IF TALENT:プライド高い == 1
	PANT_B -= 2
	PANT_C -= 1
ENDIF
;プライド低い
SIF TALENT:プライド低い == 1
	PANT_B += 1
;ツンデレは従順4以上ならPANT_B＋以下ならPANT_B－
SIF TALENT:ツンデレ == 1 && ABL:従順 >= 4
	PANT_B += 1
SIF TALENT:ツンデレ == 1 && ABL:従順 < 4
	PANT_B -= 1
;自制心
IF TALENT:自制心 == 1
	PANT_B -= 1
	PANT_C -= 2
ENDIF
;無関心
IF TALENT:無関心 == 1
	PANT_B -= 2
	PANT_C -= 2
ENDIF
;感情乏しい
IF TALENT:感情乏しい == 1
	PANT_B -= 3
	PANT_C -= 1
ENDIF
;保守的
SIF TALENT:保守的 == 1
	PANT_B -= 1
;一線越えないは陥落前ならPANT_B PANT_C-
IF TALENT:一線越えない == 1 && (TALENT:恋慕 == 1 || TALENT:淫乱 == 1 || TALENT:服従 == 1 || TALENT:親愛 == 1 || TALENT:娼婦 == 1 || TALENT:隷属 == 1)
	PANT_B -= 1
	PANT_C -= 2
ENDIF
;貞操観念
IF TALENT:貞操観念 == 1
	PANT_B -= 1
	PANT_C -= 2
ENDIF
;抑圧
SIF TALENT:抑圧 == 1
	PANT_C -= 1
;解放
IF TALENT:解放 == 1
	PANT_B += 2
	PANT_C += 1
ENDIF
;抵抗
SIF TALENT:抵抗 == 1
	PANT_C -= 1
;恥じらい
SIF TALENT:恥じらい == 1
	PANT_B -= 1
;恥薄い
SIF TALENT:恥薄い == 1
	PANT_B += 1
;自慰しやすいまたは自慰狂いは自慰系COMで
SIF (TALENT:自慰しやすい == 1 || TALENT:自慰狂い == 1) && (SELECTCOM == 4 || SELECTCOM == 5 || SELECTCOM == 6 || SELECTCOM == 7 || SELECTCOM == 509)
	PANT_C += 1
;快感に素直
IF TALENT:快感に素直 == 1
	PANT_B += 2
	PANT_C += 2
ENDIF
;快感の否定
IF TALENT:快感の否定 == 1
	PANT_B -= 2
	PANT_C -= 2
ENDIF
;発情期
SIF TALENT:発情期 == 1
	PANT_B += 1

;一度BCの回数異常阻止
;真面目で大人しい性格だと感度上がってもどうしようもないので。
SIF PANT_B < 1
	PANT_B = 1
SIF PANT_C < -2
	PANT_C = -2
;快感が入ってなくても喘ぎだすので素質補正は２まで
SIF PANT_C > 2
	PANT_C = 2

;----------------------------------------------------------------------
;行為箇所＋感度計算
;----------------------------------------------------------------------
;感覚が高いとPANT_B PANT_Cにプラス。
;またこの時点でどこを使った行為かPANT_Dに付ける
;泡踊りってなんだろう…。
;----------------------------------------------------------------------
;B
SELECTCASE SELECTCOM
	CASE 0,5,10 TO 18,73 TO 75,84,141,163,243,317,318,322,341,360,361,373,374,382,402,421,502,504,509,528
		SETBIT PANT_D,14
		;感覚低い～高いでC0～3
		IF ABL:Ｂ感覚 >= 7
			PANT_B_CALC += 2
			PANT_C_CALC += 3
		ELSEIF ABL:Ｂ感覚 >= 5
			PANT_B_CALC += 1
			PANT_C_CALC += 2
		ELSEIF ABL:Ｂ感覚 >= 3
			PANT_C_CALC += 1
		ELSE
			PANT_B_CALC -= 1
		ENDIF
		;調教者の技巧により
		IF ABL:PLAYER:技巧 == 0
			PANT_C_CALC += 2
		ELSEIF ABL:PLAYER:技巧 >= 5
			PANT_C_CALC += 1
		ELSEIF ABL:PLAYER:技巧 < 3
			PANT_C_CALC -= 1
		ENDIF
ENDSELECT
;アイテムがついているときは最大+1
IF BCHECK(TARGET)
	SETBIT PANT_D,14
	SIF ABL:Ｂ感覚 >= 5
		PANT_B_CALC += 1
	SIF ABL:Ｂ感覚 >= 3
		PANT_C_CALC += 1
ENDIF
;C
SELECTCASE SELECTCOM
	CASE 0,3,4,20 TO 40,60 TO 62,76,145,163,182,240 TO 242,244 TO 246,260 TO 265,280,285,317,318,322,342,343,365,373,374,380 TO 382,400,401,422,430,431,503,504,505,515,519 TO 525
		;愛撫のとき、貞操帯があるとＣは気持ちよくない
		IF SELECTCOM == 0 && TEQUIP:貞操帯
		;輪姦のとき、逆セックスがないとＣは気持ちよくない
		ELSEIF SELECTCOM == 322 && (TFLAG:705 & 4) == 0
		;獣姦のとき、ＶかＡどっちかに突っ込まれてると、Ｃは気持ちよくない
		ELSEIF (SELECTCOM == 322 || SELECTCOM == 381) && TFLAG:705 != 4
		;飲尿するのとき、その場の尿を飲むんだとＣは気持ちよくない
		ELSEIF SELECTCOM == 285 && BASE:尿意 < 60
		;サンドイッチのとき、Ｃ愛撫されてないとＣは気持ちよくない
		ELSEIF SELECTCOM == 515 && TFLAG:40 != 3 && TFLAG:40 != 4 && TFLAG:40 != 5
		;サンドイッチさせるのとき、愛撫する側だとＣは気持ちよくない
		ELSEIF SELECTCOM == 520 && (TFLAG:49 == 8 || TFLAG:49 == 9 || TFLAG:49 == 10)
		;連結のとき、受けだとＣは気持ちよくない
		ELSEIF SELECTCOM == 519 && TFLAG:49 != 6 && TFLAG:49 != 7 && TFLAG:49 != 11 && TFLAG:49 != 12
		ELSE
			SETBIT PANT_D,15
			;感覚低い～高いでC0～3
			IF ABL:Ｃ感覚 >= 7
				PANT_B_CALC += 2
				PANT_C_CALC += 3
			ELSEIF ABL:Ｃ感覚 >= 5
				PANT_B_CALC += 1
				PANT_C_CALC += 2
			ELSEIF ABL:Ｃ感覚 >= 3
				PANT_C_CALC += 1
			ELSE
				PANT_B_CALC += 1
			ENDIF
			;調教者の技巧により
			IF ABL:PLAYER:技巧 == 0
				PANT_C_CALC += 2
			ELSEIF ABL:PLAYER:技巧 >= 5
				PANT_C_CALC += 1
			ELSEIF ABL:PLAYER:技巧 < 3
				PANT_C_CALC -= 1
			ENDIF
		ENDIF
ENDSELECT
;アイテムがついているときは最大+1
IF CCHECK(TARGET)
	SETBIT PANT_D,15
	SIF ABL:Ｃ感覚 >= 5
		PANT_B_CALC += 1
	SIF ABL:Ｃ感覚 >= 3
		PANT_C_CALC += 1
ENDIF
;A
SELECTCASE SELECTCOM
	CASE 6,7,8,50  TO 56,110 TO 116,161,162,163,180,181,183,286,322,345,352,364,367,368,370,371,373,374,381,382,404,406,501,509,510,512,514,515,518,519,526
		;アナル舐めのとき、アナル舐めからの派生じゃないとＡは気持ちよくない
		IF SELECTCOM == 286 && TFLAG:56 != 1
		;輪姦のとき、Ａに挿入されてないとＡは気持ちよくない
		ELSEIF SELECTCOM == 322 && (TFLAG:705 & 1) == 0
		;獣姦のとき、Ａに挿入されてないとＡは気持ちよくない
		;バイブも貞操帯もないとＶに挿入してＡは使用しない
		ELSEIF SELECTCOM == 381 && !TEQUIP:バイブ && !TEQUIP:貞操帯
		;騎乗位乳房自慰、駅弁のとき、Ａに挿入されてないとＡは気持ちよくない
		ELSEIF (SELECTCOM == 509 || SELECTCOM == 510) && TFLAG:705 != 1
		;サンドイッチ、連結のとき、Ａに挿入されてないとＡは気持ちよくない
		ELSEIF (SELECTCOM == 515 || SELECTCOM == 519) && TFLAG:40 != 2 && TFLAG:41 != 2
		ELSE
			SETBIT PANT_D,16
			;感覚低い～高いでC0～3
			IF ABL:Ａ感覚 >= 7
				PANT_B_CALC += 2
				PANT_C_CALC += 3
			ELSEIF ABL:Ａ感覚 >= 5
				PANT_B_CALC += 1
				PANT_C_CALC += 2
			ELSEIF ABL:Ａ感覚 >= 3
				PANT_C_CALC += 1
			;慣れてなかったら痛いと思うので苦痛＋
			ELSEIF SELECTCOM != 50
				PANT_B_CALC -= 1
				;アナル愛撫、アナニー、アナルバイブ、アナルビーズ
				IF SELECTCOM == 6 || SELECTCOM == 7 || SELECTCOM == 51 || SELECTCOM == 53 || SELECTCOM == 54
					IF PALAM:潤滑 < PALAMLV:2
						SETBIT PANT_D, 22
						SETBIT PANT_D, 26
					ENDIF
				ELSE
					;潤滑により苦痛１or苦痛２
					IF PALAM:潤滑 < PALAMLV:2
						SETBIT PANT_D, 23
					ELSE
						SETBIT PANT_D, 22
					ENDIF
					SETBIT PANT_D, 26
				ENDIF
			ENDIF
			;調教者の技巧により
			IF ABL:PLAYER:技巧 == 0
				PANT_C_CALC += 2
			ELSEIF ABL:PLAYER:技巧 >= 5
				PANT_C_CALC += 1
			ELSEIF ABL:PLAYER:技巧 < 3
				PANT_C_CALC -= 1
			ENDIF
		ENDIF
ENDSELECT
;アイテムがついているときは最大+1
IF ACHECK(TARGET)
	SETBIT PANT_D,16
	SIF ABL:Ａ感覚 >= 5
		PANT_B_CALC += 1
	IF ABL:Ａ感覚 >= 3
		PANT_C_CALC += 1
	;慣れてない＆濡れてないと痛い
	ELSE
		IF PALAM:潤滑 < PALAMLV:2
			SETBIT PANT_D, 22
			SETBIT PANT_D, 26
		ENDIF
	ENDIF
ENDIF
;V
IF !GETBIT(PANT_D,25)
	SELECTCASE SELECTCOM
		CASE 4,7,41  TO 44,100 TO 106,160,162,163,246,322,344,362,363,366,368,369,373,374,381,382,403,405,500,506 TO 511,513,515,517,519
			;自慰のとき、Ｖを使わないとＶは気持ちよくない
			IF SELECTCOM == 4 && TFLAG:56 != 1
			;輪姦のとき、Ｖに挿入されてないとＡは気持ちよくない
			ELSEIF SELECTCOM == 322 && (TFLAG:705 & 2) == 0
			;獣姦のとき、Ｖに挿入されてないとＶは気持ちよくない
			;貞操帯かオトコだと駄目
			ELSEIF SELECTCOM == 381 && (TEQUIP:貞操帯 || !VAGINA(TARGET))
			;騎乗位乳房自慰、駅弁のとき、Ｖに挿入されてないとＶは気持ちよくない
			ELSEIF (SELECTCOM == 509 || SELECTCOM == 510) && TFLAG:705 != 0
			;サンドイッチ、連結のとき、Ｖに挿入されてないとＡは気持ちよくない
			ELSEIF (SELECTCOM == 515 || SELECTCOM == 519) && TFLAG:40 != 1 && TFLAG:41 != 1
			;電気あんまのとき、下着か下衣を着用しているとＶは気持ちよくない
			ELSEIF SELECTCOM == 246 && (TEQUIP:下着（下） || TEQUIP:下衣)
			ELSE
				SETBIT PANT_D,17
				;感覚低い～高いでC0～3
				IF ABL:Ｖ感覚 >= 7
					PANT_B_CALC += 2
					PANT_C_CALC += 3
				ELSEIF ABL:Ｖ感覚 >= 5
					PANT_B_CALC += 1
					PANT_C_CALC += 2
				ELSEIF ABL:Ｖ感覚 >= 3
					PANT_C_CALC += 1
				ELSE
					;慣れてない＆濡れてなかったら苦痛
					PANT_B_CALC -= 1
					IF PALAM:潤滑 < PALAMLV:2
						SETBIT PANT_D, 22
						SETBIT PANT_D, 26
					ENDIF
				ENDIF
				;調教者の技巧により
				IF ABL:PLAYER:技巧 == 0
					PANT_C_CALC += 2
				ELSEIF ABL:PLAYER:技巧 >= 5
					PANT_C_CALC += 1
				ELSEIF ABL:PLAYER:技巧 < 3
					PANT_C_CALC -= 1
				ENDIF
			ENDIF
	ENDSELECT
ENDIF
IF VCHECK(TARGET)
	SETBIT PANT_D,17
	SIF ABL:Ｖ感覚 >= 5
		PANT_B_CALC += 1
	IF ABL:Ｖ感覚 >= 3
		PANT_C_CALC += 1
	;慣れてない＆濡れてないと痛い
	ELSE
		IF PALAM:潤滑 < PALAMLV:2
			SETBIT PANT_D, 22
			SETBIT PANT_D, 26
		ENDIF
	ENDIF
ENDIF

;２か所以上責めるコマンドは、行為箇所による感度を半分にする
SELECTCASE SELECTCOM
	CASE 0,7,162,163,317,318,322,368,373,374,381,382,504,509,515,519
		;愛撫のとき、貞操帯がついてるとＢしか触らない
		IF SELECTCOM == 0 && TEQUIP:貞操帯
		ELSE
			PANT_B_CALC = PANT_B_CALC / 2
			PANT_C_CALC = PANT_C_CALC / 2
		ENDIF
ENDSELECT
PANT_B += PANT_B_CALC
PANT_C += PANT_C_CALC

;口
SELECTCASE SELECTCOM
	CASE 1,71,72,75,77,85,87,184,185,222,224,322,407,410,412,420,515,516,520,521,527
		;輪姦のとき、口は使わない
		IF SELECTCOM == 322 && (TFLAG:705 & 8) == 0
		;サンドイッチ、サンドイッチさせる、助手をサンドイッチのとき、口は使わない
		ELSEIF (SELECTCOM == 515 || SELECTCOM == 520 || SELECTCOM == 521) && TFLAG:49 != 8 && TFLAG:49 != 9 && TFLAG:49 != 10
		ELSE
			SETBIT PANT_D,13
		ENDIF
ENDSELECT

;継続アイテムによる苦痛
;重度
IF TEQUIP:拡張バルーン || TEQUIP:尿道バルーン || BASE:尿意 >= 90 || BASE:便意 >= 90 || TEQUIP:三角木馬
	SETBIT PANT_D, 23
;軽度
ELSEIF TEQUIP:縄 || TEQUIP:カテーテル || TEQUIP:触手膣内産卵 || TEQUIP:触手腸内産卵 || TEQUIP:触手乳腺挿入 || BASE:尿意 >= 60 || BASE:便意 >= 60 || BASE:吐き気 >= 60
	SETBIT PANT_D, 22
ENDIF
;----------------------------------------------------------------------
;陥落資質
;----------------------------------------------------------------------
;恋慕
IF TALENT:恋慕 == 1
	PANT_B += 1
	PANT_D |= 128
ENDIF
;親愛
IF TALENT:親愛 == 1
	PANT_B += 1
	PANT_D |= 1024
ENDIF
;淫乱
IF TALENT:淫乱 == 1
	PANT_B += 2
	PANT_C += 1
	PANT_D |= 256
ENDIF
;娼婦
IF TALENT:娼婦 == 1
	PANT_B += 2
	PANT_C += 2
	PANT_D |= 2048
ENDIF
;服従
IF TALENT:服従 == 1
	PANT_B -= 1
	PANT_D |= 512
ENDIF
;隷属
IF TALENT:隷属== 1
	PANT_B -= 1
	PANT_D |= 4096
ENDIF
;反発
IF MARK:反発刻印 > 0
	;認めたくないってことでPANT_C - 
	PANT_B -= 2
	PANT_C -= 2
	;反発Lv1
	IF MARK:反発刻印 == 1
		PANT_D |= 2
	;反発Lv2
