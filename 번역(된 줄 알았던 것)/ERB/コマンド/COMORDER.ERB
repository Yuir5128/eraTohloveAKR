;--------------------------------------------------------
;레즈비언 호모 조교 XX
;--------------------------------------------------------
L = 0
S = 0

;레즈비언 조교
IF (!MALE(PLAYER) && !MALE(TARGET)) || (MALE(PLAYER) && MALE(TARGET))
	IF !MALE(PLAYER) && !MALE(TARGET)
		;ABL:레즈끼
		IF ABL:레즈끼 > 3
			L += ABL:레즈끼
		ELSEIF ABL:레즈끼 < 3
			S += ABL:레즈끼
		ENDIF
		;ABL:레즈중독
		SIF ABL:레즈중독
			L += ABL:레즈중독 * 3
	ELSE
		;ABL:게이끼
		IF ABL:게이끼 > 3
			L += ABL:게이끼
		ELSEIF ABL:게이끼 < 3
			S += ABL:게이끼
		ENDIF
		;ABL:게이중독
		SIF ABL:게이중독
			L += ABL:게이중독*3
	ENDIF
	
	;TALENT:바이
	SIF TALENT:바이
		L += 10
	;호기심
	SIF TALENT:호기심
		L += 7
	;보수적
	SIF TALENT:보수적
		S += 13
	
	IF L
		PRINTFORM  + 
		A += L
		IF !MALE(PLAYER) && !MALE(TARGET)
			PRINTFORM 레즈끼 있어({L})
		ELSE
			PRINTFORM 게이끼 있어({L})
		ENDIF
	ELSEIF S
		PRINTFORM  - 
		A -= S
		IF !MALE(PLAYER) && !MALE(TARGET)
			PRINTFORM 레즈끼없음({S})
		ELSE
			PRINTFORM 게이끼없음({S})
		ENDIF
	ENDIF
ELSE
	SIF ABL:레즈중독
		S += ABL:레즈중독*2
	SIF ABL:게이중독
		S += ABL:게이중독*2
	
	;호기심
	SIF TALENT:호기심
		L += 5
	;보수적
	SIF TALENT:보수적
		S += 10
	
	IF L
		A += L
		PRINTFORM  + 
		PRINTFORM 호기심({L})
	ELSEIF S
		A -= S
		PRINTFORM  - 
		IF ABL:레즈중독
			PRINTFORM 레즈중독({S})
		ELSEIF ABL:게이중독
			PRINTFORM 게이중독({S})
		ELSEIF TALENT:보수적
			PRINTFORM 보수적({S})
		ENDIF
	ENDIF
ENDIF


