;-------------------------------------------------
;파이즈리한다
;봉사하는 계 커맨드
;-------------------------------------------------
@COM240

;전회와 이번 조교자가 같음
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;전회의 조교가파이즈리, 69파이즈리 때, 69파이즈리에
	IF PREVCOM == 73 || PREVCOM == 504
		CALL COM_ABLE504
		SIF RESULT == 1
			JUMP COM504
	ENDIF
ENDIF

PRINTL 파이즈리한다
STR:0 = 파이즈리한다
CALL KOJO_MESSAGE_COM

;스타킹 찌릿찌릿 플래그
IF TEQUIP:특수 2 == 5 || TEQUIP:특수 2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;사정게이지 체크
;-------------------------------------------------
B = 50

CALL SHOOT_GAUGE_CHECK_MT

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:사정 += B


;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 30

SOURCE:쾌C += 80

SOURCE:애정 += 100

;스킨십선호라면애정의 소스 추가
SIF TALENT:스킨십선호
	SOURCE:애정 += 50
;플레이어가거유 혹은폭유라면애정의 소스 추가
SIF TALENT:PLAYER:거유 || TALENT:PLAYER:폭유
	TIMES SOURCE:애정, 1.20


SOURCE:욕정 += 50
SOURCE:노출 += 30
SOURCE:일탈 += 20
SOURCE:반발 += 20

;-------------------------------------------------
;특수 소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 50


;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;게이경험라든지 가산
CALL COM_EXP(2)

;의존도 벡터
TFLAG:33 = 1

;의존도 보정치(음란로 1, 창부로 2)
IF TALENT:음란 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:창부 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


