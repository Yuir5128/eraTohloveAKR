;-------------------------------------------------
;조수를 이륜 꽂아애널
;파생계 커맨드
;
; 1:노예 V에 삽입	 2:노예 A에 삽입
; 3:노예를 페라	 4:노예를커닐링구스		 5:노예를 이라마
; 6:주인 V에 삽입	 7:주인 A에 삽입
; 8:주인을 페라	 9:주인을커닐링구스		10:주인을 이라마
;11:조수V에 삽입	12:조수A에 삽입		13:조수를 페라
;-------------------------------------------------
@COM525

PRINTL 조수를 이륜 꽂아애널
STR:0 =조수를 이륜 꽂아애널

SELECTCOM = 525
CALL KOJO_MESSAGE_COM


;스타킹 찌릿찌릿 플래그
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF

;처녀상실 플래그
SIF TALENT:ASSI:백버진
	TFLAG:161 |= 3

;조교자가동정였던 경우의 상실 플래그
SIF TALENT:동정
	TFLAG:160 |= 1

;조교자가동정였던 경우의 상실 플래그
SIF TALENT:MASTER:동정
	TFLAG:160 |= 2


;-------------------------------------------------
;사정게이지 체크(주인)
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PM

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:MASTER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:MASTER:사정 += B

;-------------------------------------------------
;사정게이지 체크(조수)
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PA

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:ASSI:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:사정 += B


;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
LOSEBASE:체력 += 250
LOSEBASE:기력 += 250

SOURCE:쾌C += 350

SOURCE:애정 += 300

;스킨십선호라면애정의 소스 추가
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

SOURCE:성행동 += 500
SOURCE:통증 += 400
SOURCE:중독충족 += 250
SOURCE:노출 += 100
SOURCE:굴복 += 500
SOURCE:반발 += 250

;-------------------------------------------------
;특수소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 400
	
;정조관념소유로연모, 친애 가져 이외로는 항상반발의 소스를 추가한다(섹스계만)
SIF TALENT:정조관념 && (TALENT:연모 == 0 && TALENT:친애 == 0)
	SOURCE:반발 += 1500

;중독에 의한중독충족의 소스는 먼저 처리해 둔다
;섹스중독를 본다
IF ABL:섹스중독 == 1
	TIMES SOURCE:중독충족, 0.00
ELSEIF ABL:섹스중독 == 2
	TIMES SOURCE:중독충족, 0.30
ELSEIF ABL:섹스중독 == 3
	TIMES SOURCE:중독충족, 0.60
ELSEIF ABL:섹스중독 == 4
	TIMES SOURCE:중독충족, 0.90
ELSEIF ABL:섹스중독 == 5
	TIMES SOURCE:중독충족, 1.20
ELSEIF ABL:섹스중독 == 6
	TIMES SOURCE:중독충족, 1.50
ELSEIF ABL:섹스중독 == 7
	TIMES SOURCE:중독충족, 1.80
ELSEIF ABL:섹스중독 == 8
	TIMES SOURCE:중독충족, 2.30
ELSEIF ABL:섹스중독 == 9
	TIMES SOURCE:중독충족, 3.00
ELSEIF ABL:섹스중독 >= 10
	TIMES SOURCE:중독충족, 5.00
ENDIF

;말자지소유로, 조교자가소인체형였던 경우, 일탈소스 추가
SIF TALENT:ASSI:소인체형 && TALENT:말자지
	SOURCE:일탈 += 500

;-------------------------------------------------
;사정체크 1(주인)
;-------------------------------------------------
CALL SAMEN_CHECK(MASTER, 1)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 1
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;조수를범한다로사정 한 플래그
SIF E == 2
	TFLAG:7 = 2
SIF E == 1
	TFLAG:7 = 1

;-------------------------------------------------
;사정체크 2(조수)
;-------------------------------------------------
CALL SAMEN_CHECK(ASSI)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 6
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;노예의 P⇔조교자의 V의 더러움이 이동
;페니스밴드를 사용하는 경우, 더러움은 이동하지 않는다
IF PENIS(TARGET)
	STAIN:MASTER:페니스 |= STAIN:ASSI:애널
	STAIN:ASSI:애널 |= STAIN:MASTER:페니스
	STAIN:MASTER:페니스 |= STAIN:ASSI:장내
	STAIN:ASSI:장내 |= STAIN:MASTER:페니스
ENDIF

;노예의 P⇔조교자의 V의 더러움이 이동
;페니스밴드를 사용하는 경우, 더러움은 이동하지 않는다
IF PENIS(TARGET)
	STAIN:페니스 |= STAIN:ASSI:애널
	STAIN:ASSI:애널 |= STAIN:페니스
	STAIN:페니스 |= STAIN:ASSI:장내
	STAIN:ASSI:장내 |= STAIN:페니스
ENDIF


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;A경험
EXP:ASSI:A경험 += 10
PRINTFORML %CALLNAME:ASSI%의 A경험＋10

;A확장경험
IF EXP:ASSI:A확장경험 == 0
	EXP:ASSI:이상경험 += 1
	PRINTFORML A확장 첫경험으로 %CALLNAME:ASSI%의이상경험＋1
ENDIF
EXP:ASSI:A확장경험 += 2
PRINTFORML %CALLNAME:ASSI%의 A확장경험＋2

;성교경험
EXP:PLAYER:성교경험 += 1
EXP:성교경험 += 1
EXP:ASSI:성교경험 += 2
PRINTFORML %CALLNAME:ASSI%의성교경험＋2

;삽입경험
EXP:PLAYER:삽입경험 += 1
EXP:삽입경험 += 1

;레즈경험
A = 5
B = NO:MASTER + 1099
C = NO:ASSI + 1099
D = 8

IF MALE(TARGET) == 0 && MALE(MASTER) == 0 && MALE(ASSI) == 0
	PRINTFORML %EXPNAME:40%+{D}
	EXP:레즈경험 += D
ELSEIF MALE(TARGET) == 0 && MALE(MASTER) == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:레즈경험 += A
ELSEIF MALE(TARGET) == 0 && MALE(ASSI) == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:레즈경험 += A
ELSEIF MALE(TARGET) && MALE(MASTER) && MALE(ASSI)
	PRINTFORML %EXPNAME:41%+{D}
	EXP:게이경험 += D
ELSEIF MALE(TARGET) && MALE(MASTER)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:게이경험 += A
ELSEIF MALE(TARGET) && MALE(ASSI)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:게이경험 += A
ENDIF
;조교자를 기록
CFLAG:B += A
CFLAG:C += A

;의존도 벡터
TFLAG:33 = 1

;봉사 쾌락 플래그
TFLAG:100 = 1

RETURN 1


