;-------------------------------------------------
;조수를범하게시킨다
;노예×조수계 커맨드
;-------------------------------------------------
@COM430

;-------------------------------------------------
;특수 파생 처리
;-------------------------------------------------
;전회와 이번 조교자가 같음
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;양쪽 모두 삽입하고 있으면 「조수를 이륜 꽂아」
	IF PREVCOM == 440
		CALL COM_ABLE524
		IF RESULT == 1
			TFLAG:40 = 11
			TFLAG:41 = 0
			TFLAG:49 = 11
			JUMP COM524
		ENDIF
	;주인이 조수A 삽입하고 있으면 「조수를 샌드위치(두 구멍 삽입)」
	ELSEIF PREVCOM == 441
		CALL COM_ABLE521
		IF RESULT == 1
			TFLAG:40 = 12
			TFLAG:41 = 0
			TFLAG:49 = 11
			JUMP COM521
		ENDIF
	;조수가 주인에게 삽입하고 있으면 「연결(주인←조수←노예)」
	ELSEIF PREVCOM == 270 || PREVCOM == 271
		CALL COM_ABLE519
		IF RESULT == 1
			TFLAG:40 = 0
			IF PREVCOM == 270
				TFLAG:41 = 6
			ELSE
				TFLAG:41 = 7
			ENDIF
			TFLAG:49 = 11
			JUMP COM519
		ENDIF
	;주인이 노예에게 삽입하고 있으면 「연결(조수←노예←주인)」
	ELSEIF PREVCOM == 101 || PREVCOM == 111 || PREVCOM == 105 || PREVCOM == 115
		CALL COM_ABLE519
		IF RESULT == 1
			IF PREVCOM == 101 || PREVCOM == 105
				TFLAG:40 = 1
			ELSE
				TFLAG:40 = 2
			ENDIF
			TFLAG:41 = 0
			TFLAG:49 = 11
			JUMP COM519
		ENDIF
	;전회 커맨드가 「조수를 이륜 꽂아」 「조수를 샌드위치」 「연결」
	ELSEIF PREVCOM == 524 || PREVCOM == 521 || PREVCOM == 519
		;양쪽 모두 삽입하고 있으면 「조수를 이륜 꽂아」
		IF TFLAG:40 == 11
			CALL COM_ABLE524
			IF RESULT == 1
				TFLAG:41 = 0
				TFLAG:49 = 11
				JUMP COM524
			ENDIF
		;주인이 조수A 삽입하고 있으면 「조수를 샌드위치(두 구멍 삽입)」
		ELSEIF TFLAG:40 == 12
			CALL COM_ABLE521
			IF RESULT == 1
				TFLAG:41 = 0
				TFLAG:49 = 11
				JUMP COM521
			ENDIF
		;주인이 노예에게 삽입하고 있으면 「연결(조수←노예←주인)」
		ELSEIF TFLAG:40 == 1 || TFLAG:40 == 2
			CALL COM_ABLE519
			IF RESULT == 1
				TFLAG:41 = 0
				TFLAG:49 = 11
				JUMP COM519
			ENDIF
		;조수가 주인에게 삽입하고 있으면 「연결(주인←조수←노예)」
		ELSEIF TFLAG:41 == 6 || TFLAG:41 == 7
			CALL COM_ABLE519
			IF RESULT == 1
				TFLAG:40 = 0
				TFLAG:49 = 11
				JUMP COM519
			ENDIF
		ENDIF
	ENDIF
ENDIF

;아무것도 읽지 않고 여기에 왔을 때는 플래그 해제
TFLAG:40 = 0
TFLAG:41 = 0
TFLAG:49 = 0
TFLAG:42 = 0

PRINTL 조수를범하게시킨다
STR:0 = 조수를범하게시킨다

;-------------------------------------------------
;실행 판정 처리
;-------------------------------------------------
;모든 명령에 공통의 요소를 고려
;(순종가 높다고 명령에 따르기 쉬운 등)
CALL COM_ORDER

;실행 판정 조작
CALL KOJO_DENIAL_PLUS

;합계를 표시(40이상으로 실행)
PRINT  = 
PRINTV A

V = 40
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행치
PRINTV V

;실행 불가대사/실행 판정 조작
SIF A < V
	CALL KOJO_DENIAL

WAIT

;실행 할 수 없다
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM

;스타킹 벗게 한다
IF TEQUIP:특수 2 == 5 || TEQUIP:특수 2 == 6
	TEQUIP:특수 2 = 0
	TEQUIP:특수 2 = 0
ENDIF

;처녀상실 플래그
SIF TALENT:ASSI:처녀 || TALENT:ASSI:재생처녀
	TFLAG:19 |= 3

;조교자가동정였던 경우의 상실 플래그
SIF TALENT:동정
	TFLAG:160 |= 1

;삽입 개소 판정 플래그
TFLAG:705 = 0

;-------------------------------------------------
;사정게이지 체크(조수)
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_AT

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:ASSI:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:사정 += B

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 100
LOSEBASE:기력 += 80

SIF PENIS(TARGET)
	SOURCE:쾌C += 300

SOURCE:성행동 += 500
SOURCE:성취감 += 400
SOURCE:굴복 += 700
SOURCE:일탈 += 500
SOURCE:반발 += 500


;-------------------------------------------------
;특수 소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 500


;-------------------------------------------------
;사정체크(조수)
;-------------------------------------------------
CALL SAMEN_CHECK(ASSI)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 6
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF


;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;노예의 P⇔조교자의 V의 더러움이 이동
;페니스밴드를 사용하는 경우, 더러움은 이동하지 않는다
IF PENIS(TARGET)
	CALL COM_STAIN
ENDIF


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;움직이는지 모르지만 궁합으로이상경험를 가산
IF TALENT:ASSI:처녀
	IF RELATION:(NO:ASSI) > 100
		EXP:이상경험 += 1
		PRINTFORML %CALLNAME%에 %EXPNAME:50%＋1
	ENDIF

	IF RELATION:ASSI:(NO:TARGET) > 100
		EXP:ASSI:이상경험 += 1
		PRINTFORML %CALLNAME:ASSI%에 %EXPNAME:50%＋1
	ENDIF
ENDIF

;조수에 V경험
EXP:ASSI:V경험 += 1

;조수가소인체형로 노예가말자지였던 경우
IF TALENT:ASSI:소인체형 && TALENT:말자지
	IF EXP:ASSI:V확장경험 == 0
		EXP:이상경험 += 1
		PRINTL V확장 첫경험으로이상경험＋1
		EXP:ASSI:이상경험 += 1
		PRINTFORML %CALLNAME:ASSI%의이상경험＋1
	ENDIF
	EXP:ASSI:V확장경험 += 1
	PRINTFORML %CALLNAME:ASSI%의 V확장경험＋1
ENDIF

;성교경험
EXP:성교경험 += 1
EXP:ASSI:성교경험 += 1
PRINTS EXPNAME:4
PRINTL +1

;삽입경험
EXP:삽입경험 += 1
PRINTS EXPNAME:17
PRINTL +1

;게이경험라든지 가산
CALL COM_EXP(10)

;의존도 벡터
P = PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;의존도 보정치(궁합 100↑또한음란로 1, 창부로 2)
P = PLAYER
IF TALENT:음란 && RELATION:P > 100
	TFLAG:30 += 1
ELSEIF TALENT:창부 && RELATION:P > 100
	TFLAG:30 += 2
ENDIF


RETURN 1




