;-------------------------------------------------
;이륜 꽂아애널
;파생계 커맨드
;-------------------------------------------------
@COM518

PRINTL 이륜 꽂아애널
STR:0 =이륜 꽂아애널

SELECTCOM = 518
CALL KOJO_MESSAGE_COM

;스타킹 찌릿찌릿 플래그
IF TEQUIP:특수 2 == 5 || TEQUIP:특수 2 == 6
	TFLAG:55 = 1
ENDIF

;처녀상실 플래그
SIF TALENT:백버진
	TFLAG:161 |= 1

;조교자가동정였던 경우의 상실 플래그
SIF TALENT:PLAYER:동정
	TFLAG:160 |= 2

;조교자가동정였던 경우의 상실 플래그
SIF TALENT:ASSI:동정
	TFLAG:160 |= 3


;-------------------------------------------------
;사정게이지 체크(주인)
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PM

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:MASTER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:MASTER:사정 += B

;-------------------------------------------------
;사정게이지 체크(조수)
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PA

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:ASSI:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:사정 += B

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 300
LOSEBASE:기력 += 200

SOURCE:쾌A += 500

;A확장경험를 본다
IF EXP:A확장경험 >= 50
	TIMES SOURCE:쾌A , 2.00
ELSEIF EXP:A확장경험 > 40
	TIMES SOURCE:쾌A , 1.80
ELSEIF EXP:A확장경험 > 30
	TIMES SOURCE:쾌A , 1.50
ELSEIF EXP:A확장경험 > 20
	TIMES SOURCE:쾌A , 1.20
ENDIF

SOURCE:애정 += 300
;스킨십선호라면애정의 소스 추가
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

SOURCE:성행동 += 250
SOURCE:통증 += 1200
SOURCE:중독충족 += 500
SOURCE:노출 += 500
SOURCE:굴복 += 500
SOURCE:일탈 += 800
SOURCE:반발 += 500

;-------------------------------------------------
;특수 소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 500

;정조관념소유로연모, 친애 가져 이외로는 항상반발의 소스를 추가한다(섹스계만)
SIF TALENT:정조관념 && (TALENT:연모 == 0 && TALENT:친애 == 0)
	SOURCE:반발 += 1000

;A에의 큰고통는 먼저 처리해 둔다
;EXP:A경험를 본다
IF EXP:A경험 < EXPLV:1
	TIMES SOURCE:통증, 3.00
	TIMES SOURCE:반발, 4.00
ELSEIF EXP:A경험 < EXPLV:2
	TIMES SOURCE:통증, 2.50
	TIMES SOURCE:반발, 3.50
ELSEIF EXP:A경험 < EXPLV:3
	TIMES SOURCE:통증, 2.40
	TIMES SOURCE:반발, 3.00
ELSEIF EXP:A경험 < EXPLV:4
	TIMES SOURCE:통증, 2.00
	TIMES SOURCE:반발, 2.50
ELSEIF EXP:A경험 < EXPLV:5
	TIMES SOURCE:통증, 1.80
	TIMES SOURCE:반발, 2.00
ELSEIF EXP:A경험 >= EXPLV:5
	TIMES SOURCE:통증, 1.60
	TIMES SOURCE:반발, 1.50
ENDIF

;PALAM:윤활를 본다
IF PALAM:윤활 < PALAMLV:1
	TIMES SOURCE:통증, 3.00
	TIMES SOURCE:반발, 2.00
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES SOURCE:통증, 2.50
	TIMES SOURCE:반발, 1.50
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES SOURCE:통증, 2.00
	TIMES SOURCE:반발, 1.20
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES SOURCE:통증, 1.50
ELSEIF PALAM:윤활 >= PALAMLV:4
	TIMES SOURCE:통증, 1.00
ENDIF


;섹스중독를 본다
IF ABL:섹스중독 == 1
	TIMES SOURCE:중독충족, 0.00
ELSEIF ABL:섹스중독 == 2
	TIMES SOURCE:중독충족, 0.30
ELSEIF ABL:섹스중독 == 3
	TIMES SOURCE:중독충족, 0.60
ELSEIF ABL:섹스중독 == 4
	TIMES SOURCE:중독충족, 0.90
ELSEIF ABL:섹스중독 == 5
	TIMES SOURCE:중독충족, 1.20
ELSEIF ABL:섹스중독 == 6
	TIMES SOURCE:중독충족, 1.50
ELSEIF ABL:섹스중독 == 7
	TIMES SOURCE:중독충족, 1.80
ELSEIF ABL:섹스중독 == 8
	TIMES SOURCE:중독충족, 2.30
ELSEIF ABL:섹스중독 == 9
	TIMES SOURCE:중독충족, 3.00
ELSEIF ABL:섹스중독 >= 10
	TIMES SOURCE:중독충족, 5.00
ENDIF

;-------------------------------------------------
;사정체크 1(주인)
;-------------------------------------------------
CALL SAMEN_CHECK(MASTER, 1)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 2
	;사정처 선택의 가능·불가
	TFLAG:704 = 1
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;사정체크 2(조수)
;-------------------------------------------------
CALL SAMEN_CHECK(ASSI)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 2
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;노예의 A와장내⇔조교자의 C의 더러움이 이동
;페니스밴드를 사용하는 경우, 더러움은 이동하지 않는다
IF PENIS(PLAYER)
	STAIN:장내 |= STAIN:MASTER:페니스
	STAIN:MASTER:페니스 |= STAIN:장내
	STAIN:애널 |= STAIN:MASTER:페니스
	STAIN:MASTER:페니스 |= STAIN:애널
ENDIF

;노예의 A⇔조수의페니스의 더러움이 이동
;페니스밴드를 사용하는 경우, 더러움은 이동하지 않는다
IF PENIS(ASSI)
	STAIN:애널 |= STAIN:ASSI:페니스
	STAIN:ASSI:페니스 |= STAIN:애널
	STAIN:장내 |= STAIN:ASSI:페니스
	STAIN:ASSI:페니스 |= STAIN:장내
ENDIF

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;A경험
EXP:A경험 += 10
PRINTL A경험＋10

;A확장경험
IF EXP:A확장경험 == 0
	EXP:이상경험 += 1
	PRINTL A확장 첫경험으로이상경험＋1
ENDIF
EXP:A확장경험 += 2
PRINTL A확장경험＋2

;성교경험
EXP:MASTER:성교경험 += 1
EXP:ASSI:성교경험 += 1
EXP:성교경험 += 2
PRINTS EXPNAME:4
PRINTL +2

;삽입경험
EXP:MASTER:삽입경험 += 1
EXP:ASSI:삽입경험 += 1

;레즈경험
A = 5
B = NO:MASTER + 1099
C = NO:ASSI + 1099
D = 8

IF MALE(ASSI) == 0 && MALE(MASTER) == 0 && MALE(ASSI) == 0
	PRINTFORML %EXPNAME:40%+{D}
	EXP:레즈경험 += D
ELSEIF MALE(ASSI) == 0 && MALE(MASTER) == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:레즈경험 += A
ELSEIF MALE(ASSI) == 0 && MALE(ASSI) == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:레즈경험 += A
ELSEIF MALE(TARGET) && MALE(MASTER) && MALE(ASSI)
	PRINTFORML %EXPNAME:41%+{D}
	EXP:게이경험 += D
ELSEIF MALE(TARGET) && MALE(MASTER)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:게이경험 += A
ELSEIF MALE(TARGET) && MALE(ASSI)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:게이경험 += A
ENDIF
;조교자를 기록
CFLAG:B += A
CFLAG:C += A

;의존도 벡터
TFLAG:33 = 1

RETURN 1



