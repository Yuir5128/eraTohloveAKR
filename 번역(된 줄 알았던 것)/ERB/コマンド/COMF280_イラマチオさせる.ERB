;-------------------------------------------------
;이라마치오시킨다
;주인마조계 커맨드
;-------------------------------------------------
@COM280

;-------------------------------------------------
;특수 파생 처리
;-------------------------------------------------
;전회와 이번 조교자가 같음
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;벌써삽입명령중은 「샌드위치」
	IF PREVCOM == 405 || PREVCOM == 406
		CALL COM_ABLE515
		IF RESULT == 1
			TFLAG:40 = 5
			IF PREVCOM == 405
				TFLAG:41 = 1
			ELSE
				TFLAG:41 = 2
			ENDIF
			TFLAG:49 = 0
			JUMP COM515
		ENDIF
	;전회 커맨드가촉수에범하게시킨다라고 해 「샌드위치 시킨다」
	ELSEIF PREVCOM == 270 || PREVCOM == 271
		CALL COM_ABLE520
		IF RESULT == 1
			TFLAG:40 = 5
			IF PREVCOM == 270
				TFLAG:41 = 6
			ELSE
				TFLAG:41 = 7
			ENDIF
			TFLAG:49 = 0
			JUMP COM520
		ENDIF
	;전회가 샌드위치인가, 샌드위치 시킨다
	ELSEIF PREVCOM == 515 || PREVCOM == 520
		;조수가 노예에게 삽입하고 있으면 다시 「샌드위치」에
		IF TFLAG:41 == 1 || TFLAG:41 == 2
			CALL COM_ABLE515
			IF RESULT == 1
				TFLAG:40 = 5
				IF PREVCOM == 405
					TFLAG:41 = 1
				ELSE
					TFLAG:41 = 2
				ENDIF
				TFLAG:49 = 0
				JUMP COM515
			ENDIF
		;조수가 주인에게 삽입하고 있으면 「샌드위치 시킨다」에
		ELSEIF TFLAG:41 == 6 || TFLAG:41 == 7
			CALL COM_ABLE520
			IF RESULT == 1
				TFLAG:40 = 5
				TFLAG:49 = 0
				JUMP COM520
			ENDIF
		ENDIF
	ENDIF
ENDIF

;아무것도 읽지 않고 여기에 왔을 때는 플래그 해제
TFLAG:40 = 0
TFLAG:41 = 0
TFLAG:49 = 0
TFLAG:42 = 0

PRINTL 이라마치오시킨다
STR:0 = 이라마치오시킨다

;-------------------------------------------------
;실행 판정 처리
;-------------------------------------------------
;모든 명령에 공통의 요소를 고려
;(순종가 높다고 명령에 따르기 쉬운 등)
CALL COM_ORDER

;실행 판정 조작
CALL KOJO_DENIAL_PLUS

;합계를 표시(26이상으로 실행)
PRINT  = 
PRINTV A

V = 26
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행치
PRINTV V

;실행 불가입상/실행 판정 조작
SIF A < V
	CALL KOJO_DENIAL

WAIT

;실행 할 수 없다
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM


;스타킹 찌릿찌릿 플래그
IF TEQUIP:특수 2 == 5 || TEQUIP:특수 2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 20
LOSEBASE:기력 += 50

SOURCE:쾌C += 200

;조교자의 TALENT:혀놀림를 본다
IF TALENT:PLAYER:혀놀림
	TIMES SOURCE:쾌C , 1.50
ELSEIF TALENT:PLAYER:고양이혀
	TIMES SOURCE:쾌C , 0.75
ENDIF

SOURCE:노출 += 50
SOURCE:일탈 += 50
SOURCE:반발 += 50


;-------------------------------------------------
;특수 소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 40


;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;게이경험라든지 가산
CALL COM_EXP(3)

;새드끼에 응해가학쾌락경험
E = 1
L = 0
IF ABL:새드끼 == 0
	E += 0
	L += 0
ELSEIF ABL:새드끼 == 1
	E += 1
	L += 1
ELSEIF ABL:새드끼 == 2
	E += 3
	L += 2
ELSEIF ABL:새드끼 == 3
	E += 5
	L += 3
ELSEIF ABL:새드끼 == 4
	E += 7
	L += 4
ELSEIF ABL:새드끼 >= 5
	E += 9
	L += 5
ENDIF
;조교자의마조끼로가학쾌락경험
IF ABL:PLAYER:9 == 0
	E += 0
	L += 0
ELSEIF ABL:PLAYER:9 == 1
	E += 1
	L += 1
ELSEIF ABL:PLAYER:9 == 2
	E += 2
	L += 3
ELSEIF ABL:PLAYER:9 == 3
	E += 3
	L += 5
ELSEIF ABL:PLAYER:9 == 4
	E += 4
	L += 7
ELSEIF ABL:PLAYER:9 >= 5
	E += 5
	L += 9
ENDIF
PRINT 가학쾌락경험+
PRINTVL E
EXP:가학쾌락경험 += E
IF L > 0
	PRINTFORM %CALLNAME:PLAYER%의
	PRINT 고통쾌락경험+
	PRINTVL L
	EXP:PLAYER:고통쾌락경험 += L
	TFLAG:27 = L
ENDIF

;조교자가고양이혀소유의 경우는, 조교자에게가학쾌락경험를 추가
IF TALENT:PLAYER:고양이혀
	EXP:PLAYER:가학쾌락경험 += 1
	PRINTFORML %CALLNAME:PLAYER%의 %EXPNAME:32%＋1
ENDIF

;의존도 벡터
TFLAG:33 = 3


RETURN 1


