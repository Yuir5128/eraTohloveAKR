;-------------------------------------------------
;애널바이브
;A계 조교 커맨드
;-------------------------------------------------
@COM54

IF TEQUIP:촉수
	PRINTL 촉수애널삽입
	STR:0 = 촉수애널삽입
	SELECTCOM = 345
ELSE
	PRINTL 애널바이브
	STR:0 = 애널바이브
ENDIF

CALL KOJO_MESSAGE_COM

;스타킹 찌릿찌릿 플래그
IF TEQUIP:특수 2 == 5 || TEQUIP:특수 2 == 6
	TFLAG:55 = 1
ENDIF

;착탈의 처리
IF TEQUIP:애널바이브 == 0
	TEQUIP:애널바이브 = 1
ELSE
	TEQUIP:애널바이브 = 0
ENDIF


;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 20
LOSEBASE:기력 += 80

SOURCE:쾌A += 200

SOURCE:통증 += 250
;지하실에 있으면고통의 소스 추가
SIF TEQUIP:지하실
	SOURCE:통증 += 250

SOURCE:노출 += 50
SOURCE:굴복 += 50
SOURCE:일탈 += 50

;-------------------------------------------------
;특수 소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 70

;A에의고통는 먼저 처리해 둔다
;EXP:A경험를 본다
IF EXP:A경험 < EXPLV:1
	TIMES SOURCE:통증, 3.00
	TIMES SOURCE:반발, 3.00
ELSEIF EXP:A경험 < EXPLV:2
	TIMES SOURCE:통증, 1.20
	TIMES SOURCE:반발, 2.00
ELSEIF EXP:A경험 < EXPLV:3
	TIMES SOURCE:통증, 0.80
	TIMES SOURCE:반발, 1.50
ELSEIF EXP:A경험 < EXPLV:4
	TIMES SOURCE:통증, 0.60
	TIMES SOURCE:반발, 1.20
ELSEIF EXP:A경험 < EXPLV:5
	TIMES SOURCE:통증, 0.40
ELSEIF EXP:A경험 >= EXPLV:5
	TIMES SOURCE:통증, 0.20
ENDIF

;PALAM:윤활를 본다
IF PALAM:윤활 < PALAMLV:1
	TIMES SOURCE:통증, 1.20
	TIMES SOURCE:반발, 2.00
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES SOURCE:통증, 1.00
	TIMES SOURCE:반발, 1.50
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES SOURCE:통증, 0.60
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES SOURCE:통증, 0.30
ELSEIF PALAM:윤활 >= PALAMLV:4
	TIMES SOURCE:통증, 0.20
ENDIF


;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;A경험
EXP:A경험 += 2
PRINTL A경험＋2

;게이경험라든지 가산
CALL COM_EXP(1)

;촉수소환시, 촉수경험의 리셋트를 한다
SIF TEQUIP:촉수 == 1
	T = 0

;의존도 벡터
TFLAG:33 = 3


RETURN 1



;-------------------------------------------------
;장착 계속의 처리
;-------------------------------------------------
@EQUIP_COM54
;애널바이브삽입중
IF TEQUIP:촉수
	PRINTL ＜애널촉수삽입중＞
ELSE
	PRINTL ＜애널바이브삽입중＞
ENDIF

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
LOSEBASE:체력 += 10
LOSEBASE:기력 += 30

SOURCE:쾌A += 125
SOURCE:통증 += 100
SOURCE:노출 += 25
SOURCE:일탈 += 25
SOURCE:반발 += 40

;-------------------------------------------------
;특수 소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 35

;A에의고통는 먼저 처리해 둔다
;EXP:A경험를 본다
A = 100
B = 40
IF EXP:A경험 < EXPLV:1
	TIMES A , 3.00
	TIMES B , 3.00
ELSEIF EXP:A경험 < EXPLV:2
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF EXP:A경험 < EXPLV:3
	TIMES A , 0.80
	TIMES B , 1.50
ELSEIF EXP:A경험 < EXPLV:4
	TIMES A , 0.60
	TIMES B , 1.20
ELSEIF EXP:A경험 < EXPLV:5
	TIMES A , 0.40
ELSEIF EXP:A경험 >= EXPLV:5
	TIMES A , 0.20
ENDIF

;PALAM:윤활를 본다
IF PALAM:윤활 < PALAMLV:1
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES A , 1.00
	TIMES B , 1.50
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES A , 0.60
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES A , 0.30
ELSEIF PALAM:윤활 >= PALAMLV:4
	TIMES A , 0.20
ENDIF
SOURCE:통증 += A
SOURCE:반발 += B

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
EXP:A경험 += 1
PRINTL A경험＋1

;촉수경험가산
SIF TEQUIP:촉수 == 1
	T += 1


RETURN 1


