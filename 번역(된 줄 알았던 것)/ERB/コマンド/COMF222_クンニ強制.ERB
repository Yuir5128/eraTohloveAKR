;-------------------------------------------------
;커닐링구스강제
;애무시킨다계 커맨드
;-------------------------------------------------
@COM222

;-------------------------------------------------
;특수 파생 처리
;-------------------------------------------------
;전회와 이번 조교자가 같음
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;조수삽입중, 전회 커맨드가 조수삽입 때= 「샌드위치」
	IF PREVCOM == 405 || PREVCOM == 406
		CALL COM_ABLE515
		IF RESULT == 1
			TFLAG:40 = 0
			IF PREVCOM == 405
				TFLAG:41 = 1
			ELSE
				TFLAG:41 = 2
			ENDIF
			TFLAG:49 = 9
			JUMP COM515
		ENDIF
	;전회 커맨드가촉수에범하게시킨다 때, 「샌드위치 시킨다」
	ELSEIF PREVCOM == 270 || PREVCOM == 271
		CALL COM_ABLE520
		IF RESULT == 1
			TFLAG:40 = 0
			IF PREVCOM == 270
				TFLAG:41 = 6
			ELSE
				TFLAG:41 = 7
			ENDIF
			TFLAG:49 = 9
			JUMP COM520
		ENDIF
	;전회의 조교가펠라한다, 커닐링구스, 식스나인 때, 식스나인
	ELSEIF PREVCOM == 31 || PREVCOM == 40 || PREVCOM == 503
		CALL COM_ABLE503
		SIF RESULT == 1
			JUMP COM503
	;전회 커맨드가 샌드위치 때
	ELSEIF PREVCOM == 515
		;조수가 노예에게 삽입하고 있으면 다시 「샌드위치」에
		IF TFLAG:41 == 1 || TFLAG:41 == 2
			CALL COM_ABLE515
			IF RESULT == 1
				TFLAG:40 = 0
				IF PREVCOM == 405
					TFLAG:41 = 1
				ELSE
					TFLAG:41 = 2
				ENDIF
				TFLAG:49 = 9
				JUMP COM515
			ENDIF
		;조수가 주인에게 삽입하고 있으면 「샌드위치 시킨다」에
		ELSEIF TFLAG:41 == 6 || TFLAG:41 == 7
			CALL COM_ABLE520
			IF RESULT == 1
				TFLAG:40 = 0
				TFLAG:49 = 9
				JUMP COM520
			ENDIF
		ENDIF
	ENDIF
ENDIF

;아무것도 읽지 않고 여기에 왔을 때는 플래그 해제
TFLAG:40 = 0
TFLAG:41 = 0
TFLAG:49 = 0
TFLAG:42 = 0

PRINTL 커닐링구스 시킨다
STR:0 = 커닐링구스 시킨다

;-------------------------------------------------
;실행 판정 처리
;-------------------------------------------------
;모든 명령에 공통의 요소를 고려
;(순종가 높다고 명령에 따르기 쉬운 등)
CALL COM_ORDER

;실행 판정 조작
CALL KOJO_DENIAL_PLUS

;합계를 표시(23이상으로 실행)
PRINT  = 
PRINTV A

V = 23
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행치
PRINTV V

;실행 불가입상/실행 판정 조작
SIF A < V
	CALL KOJO_DENIAL

WAIT

;실행 할 수 없다
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM


;실행자가 노예인 플래그
TFLAG:28 = 1


;-------------------------------------------------
;사정게이지 체크(후타나리의 경우)
;-------------------------------------------------
B = 300

CALL SHOOT_GAUGE_CHECK_MT

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:사정 += B

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 50

SOURCE:애정 += 50

;스킨십선호라면애정의 소스 추가
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

SOURCE:성행동 += 300
SOURCE:성취감 += 150
SOURCE:굴복 += 500
SOURCE:일탈 += 200
SOURCE:반발 += 200

;위 쪽으로 계산한 더러운 데이터
SOURCE:불결 += Y*80 + 50


;-------------------------------------------------
;특수 소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 400


;-------------------------------------------------
;사정체크
;-------------------------------------------------
CALL SAMEN_CHECK(PLAYER, 1)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 7
	;사정처 선택의 가능·불가
	;노예가공, 한편 주인이수나오메가라면 선택 불가
	IF TALENT:공 && (TALENT:PLAYER:수 || TALENT:MASTER:오메가)
		TFLAG:704 = 0
	ELSE
		TFLAG:704 = 1
	ENDIF
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
CALL COM_STAIN

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;게이경험라든지 가산
CALL COM_EXP(6)

;노예가고양이혀소유의 경우는, 조교자에게가학쾌락경험를 추가
IF TALENT:고양이혀
	EXP:가학쾌락경험 += 1
	PRINTFORML %EXPNAME:32%＋1
ENDIF

;의존도 벡터
P = PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;의존도 보정치(궁합 100↑또한음란로 1, 창부로 2)
P = PLAYER
IF TALENT:음란 && RELATION:P > 100
	TFLAG:30 += 1
ELSEIF TALENT:창부 && RELATION:P > 100
	TFLAG:30 += 2
ENDIF

;봉사 쾌락 플래그
TFLAG:100 = 1


RETURN 1



