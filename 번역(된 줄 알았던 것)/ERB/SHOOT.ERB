@SAMEN_SHOOT_CALC
;사정시의 공통 처리
;정액 더러움의 설정
STAIN:P:페니스 |= 4

;대량사정
IF E == 2
	IF NO:P == 0
		PRINT 주인
	ELSE
		PRINTFORM %CALLNAME:P%
	ENDIF
	PRINTL 　대량사정

	EXP:P:사정경험 += 2
	BASE:P:사정 -= EJAC*2
	SIF BASE:P:사정 >= EJAC
		BASE:P:사정 = EJAC-1
;통상의사정
ELSEIF E == 1
	IF NO:P == 0
		PRINT 주인
	ELSE
		PRINTFORM %CALLNAME:P%
	ENDIF
	PRINTL 　사정

	EXP:P:사정경험 += 1
	BASE:P:사정 -= EJAC
	SIF BASE:P:사정 >= EJAC
		BASE:P:사정 = EJAC-1
ENDIF

;사정시에중독충족의 소스를 추가
;통상사정시
SIF E == 1
	A = 200
;대량사정시
SIF E == 2
	A = 500

IF E > 0
	;정액중독를 본다
	IF ABL:정액중독 == 1
		TIMES A , 0.00
	ELSEIF ABL:정액중독 == 2
		TIMES A , 0.30
	ELSEIF ABL:정액중독 == 3
		TIMES A , 0.60
	ELSEIF ABL:정액중독 == 4
		TIMES A , 0.90
	ELSEIF ABL:정액중독 == 5
		TIMES A , 1.20
	ELSEIF ABL:정액중독 == 6
		TIMES A , 1.50
	ELSEIF ABL:정액중독 == 7
		TIMES A , 1.80
	ELSEIF ABL:정액중독 == 8
		TIMES A , 2.30
	ELSEIF ABL:정액중독 == 9
		TIMES A , 3.00
	ELSEIF ABL:정액중독 == 10
		TIMES A , 5.00
	ENDIF
ENDIF

;고무 쏘아 맞히고 플래그가 있으면중독충족 하지 않는다
SIF CFLAG:T:106!= 9
	SOURCE:중독충족 += A

;사정후는 재장전의 시간을 마련한다
;통상사정시
SIF E == 1
	CFLAG:P:9 = 3
;대량사정시
SIF E == 2
	CFLAG:P:9 = 5
CFLAG:P:11 -= 1


