@DAILY_LIFE_30
;체력이 반이하의 캐릭터를 검색
VARSET LOCAL
FOR LOCAL, 0, CHARANUM
	;주인은 제외하다
	SIF LOCAL == MASTER
		CONTINUE
	;일상 이벤트에 등장 불가
	SIF !DAILYLIFE_CHARA(LOCAL)
		CONTINUE
	;이미 등장하고 있는 캐릭터와 감싸고 있으면 불가
	SIF LOCAL == A
		CONTINUE
	;체력이 반이하가 아니면 불가
	SIF (BASE:LOCAL:체력 * 2) > MAXBASE:LOCAL:체력
		CONTINUE
	LOCAL:(10 + LOCAL:1) = LOCAL
	LOCAL:1++
NEXT
;조건에 들어맞는 캐릭터로부터 랜덤으로 1 인선택 한다
IF LOCAL:1
	LOCAL:2 = RAND:(LOCAL:1)
	B = LOCAL:(10 + LOCAL:2)
ENDIF
;B체력회복
IF B > -1 && RAND:2
	PRINTFORMW %CALLNAME:MASTER%가 %CALLNAME:A%의 방의 근처를 지나면、방 안으로부터 이야기 소리가 들린다
