;====================================================================
;플레이 내용의 판정
;====================================================================
@PROSTITUTION_PLAY_CHECK

;--------------------------------------------------------
;인원수와 플레이 내용의 계산
;최대 인원수 =아침에 냈다면 12명, 밤에 냈다면 8명
B = 0

;성적 봉사(수)	CFLAG:A:160
;V섹스		CFLAG:A:161
;A섹스		CFLAG:A:162
;소프트 SM(수)	CFLAG:A:163
;하도SM(수)	CFLAG:A:164
;수치심			CFLAG:A:165
;방뇨 감상		CFLAG:A:166
;음뇨플레이		CFLAG:A:167
;관장플레이		CFLAG:A:168
;성적 봉사(공)	CFLAG:A:169
;역섹스		CFLAG:A:170
;소프트 SM(공)	CFLAG:A:171
;하도SM(공)	CFLAG:A:172
;처녀관통		CFLAG:A:173
;A처녀관통		CFLAG:A:174
;동정 먹어		CFLAG:A:175

CFLAG:A:160 = 0
CFLAG:A:161 = 0
CFLAG:A:162 = 0
CFLAG:A:163 = 0
CFLAG:A:164 = 0
CFLAG:A:165 = 0
CFLAG:A:166 = 0
CFLAG:A:167 = 0
CFLAG:A:168 = 0
CFLAG:A:169 = 0
CFLAG:A:170 = 0
CFLAG:A:171 = 0
CFLAG:A:172 = 0
CFLAG:A:173 = 0
CFLAG:A:174 = 0
CFLAG:A:175 = 0
;--------------------------------------------------------

REPEAT 12
	;최대 인원수 넘으면 빠진다
	IF (B == 12 && TIME == 0) || (B == 8 && TIME != 0)
		BREAK
	;체력 없다고 손님이 붙지 않는다. 손님이 붙지 않으면체력회복
	ELSEIF BASE:A:체력 < 500
		IF TALENT:A:회복빠름 || TALENT:A:창부 || TALENT:A:예속 || ABL:A:마조끼 >= 5
			BASE:A:체력 += 300
		ELSEIF TALENT:A:회복느림 || ABL:A:마조끼 < 3
			BASE:A:체력 += 100
		ELSE
			BASE:A:체력 += 200
		ENDIF
	;도 없으면 손님을 선출
	ELSE
		;-----------------------
		;인원수를 늘린다
		;-----------------------
		B += 1
		
		LOCAL:1 = 0
		LOCAL:2 = 0
		;-----------------------
		;노예의수(LOCAL:1)공(LOCAL:2) 보존
		;-----------------------
		IF TALENT:A:공
			LOCAL:2 = 1
		ELSEIF TALENT:A:수 || TALENT:A:오메가
			LOCAL:1 = 1
		ELSE
			IF !RAND:5
				LOCAL:2 = 1
			ELSE
				LOCAL:1 = 1
			ENDIF
		ENDIF
		
		;-----------------------
		;손님의 도S(LOCAL:3), S(LOCAL:4), 보통(LOCAL:5), M(LOCAL:6)
		;-----------------------
		;노예가공
		IF LOCAL:2
			;M의 손님이 증가한다
			IF RAND:10 < ABL:A:새드끼
				LOCAL:6 = 1
			;(이)가 아니면 보통 손님
			ELSE
				LOCAL:5 = 1
			ENDIF
		;노예가수
		ELSE
			K:1 = RAND:10
			;도S
			IF K:1 < 2
				LOCAL:3 = 1
			;S의 손님이 증가한다
			ELSEIF K:1 < ABL:A:마조끼
				LOCAL:4 = 1
			;(이)가 아니면 보통 손님
			ELSE
				LOCAL:5 = 1
			ENDIF
		ENDIF
		
		;각각의 플레이가 있었는가 어떤가 판정
		;-----------------------
		;성적 봉사는 절대로 있다
		;-----------------------
		;노예공
		IF LOCAL:2
			CFLAG:A:169 += 1
			BASE:A:체력 -= 150
		;노예수
		ELSE
			CFLAG:A:160 += 1
			BASE:A:체력 -= 100
		ENDIF
		
		;-----------------------
		;하도SM(옵션유, 랜덤 1/3)
		;-----------------------
		IF (FLAG:160 & 8) && RAND:2 == 0
			;노예공
			IF LOCAL:2
				CFLAG:A:172 += 1
			;노예수
			ELSE
				CFLAG:A:164 += 1
			ENDIF
			;여유
			IF (LOCAL:1 && (ABL:A:마조끼 >= 5 || TALENT:A:마조)) || (LOCAL:2 && (ABL:A:새드끼 >= 5 || TALENT:A:새드))
				BASE:A:체력 -= 300
			;지친다
			ELSEIF (LOCAL:1 && (ABL:A:마조끼 < 3 || !TALENT:A:도착적)) || (LOCAL:2 && (ABL:A:새드끼 < 3 || TALENT:A:소악마))
				BASE:A:체력 -= 800
			ELSE
				BASE:A:체력 -= 500
			ENDIF
			
			;-----------------------
			;관장플레이(옵션유, 랜덤 1/5, 노예수)
			;수치(CFLAG:A:165)도 느낀다
			;-----------------------
			IF (FLAG:160 & 768) && RAND:4 == 0 && LOCAL:1
				CFLAG:A:168 += 1
				;여유
				IF ABL:A:마조끼 >= 5 || TALENT:A:마조 || EXP:A:관장경험 >= 50 || ABL:A:스캇중독 >= 3
					BASE:A:체력 -= 200
					CFLAG:A:165 += 1
				;지친다
				ELSEIF ABL:A:마조끼 < 3 || EXP:A:관장경험 < 20
					BASE:A:체력 -= 500
					CFLAG:A:165 += 5
				ELSE
					BASE:A:체력 -= 350
					CFLAG:A:165 += 3
				ENDIF
			ENDIF
			
			;-----------------------
			;음뇨플레이(옵션유, 랜덤 1/5, 노예수)
			;-----------------------
			IF (FLAG:160 & 128) && RAND:4 == 0 && LOCAL:1
				CFLAG:A:167 += 1
				;여유
				IF ABL:A:마조끼 >= 5 || TALENT:A:마조 || EXP:A:음뇨경험 >= 50 || ABL:A:스캇중독 >= 3
					BASE:A:체력 -= 50
				;지친다
				ELSEIF ABL:A:마조끼 < 3 || EXP:A:음뇨경험 < 10
					BASE:A:체력 -= 300
				ELSE
					BASE:A:체력 -= 150
				ENDIF
			ENDIF
			
			;-----------------------
			;방뇨 감상(옵션유, 랜덤 1/5, 노예수)
			;수치(CFLAG:A:165)도 느낀다
			;-----------------------
			IF (FLAG:160 & 192) && RAND:4 == 0 && LOCAL:1
				CFLAG:A:166 += 1
				;여유
				IF ABL:A:마조끼 >= 5 || TALENT:A:마조 || EXP:A:배뇨경험 >= 50 || ABL:A:스캇중독 >= 3
					BASE:A:체력 -= 50
					CFLAG:A:165 += 1
				;지친다
				ELSEIF ABL:A:마조끼 < 3 || EXP:A:배뇨경험 < 20
					BASE:A:체력 -= 200
					CFLAG:A:165 += 3
				ELSE
					BASE:A:체력 -= 100
					CFLAG:A:165 += 2
				ENDIF
			ENDIF
			
			;-----------------------
			;수치플레이(노예수만)
			;-----------------------
			IF LOCAL:1
				IF RAND:3 == 0
					CFLAG:A:165 += 5
				ELSEIF RAND:2 == 0
					CFLAG:A:165 += 3
				ELSE
					CFLAG:A:165 += 1
				ENDIF
			ENDIF
		;-----------------------
		;소프트 SM(랜덤 1/3)
		;-----------------------
		ELSEIF RAND:2 == 0
			;노예공
			IF LOCAL:2
				CFLAG:A:171 += 1
			;노예수
			ELSE
				CFLAG:A:163 += 1
			ENDIF
			;여유
			IF (LOCAL:1 && (ABL:A:마조끼 >= 5 || TALENT:A:마조)) || (LOCAL:2 && (ABL:A:새드끼 >= 5 || TALENT:A:새드))
				BASE:A:체력 -= 100
			;지친다
			ELSEIF (LOCAL:1 && (ABL:A:마조끼 < 3 || TALENT:A:도착적)) || (LOCAL:2 && (ABL:A:새드끼 < 3 || TALENT:A:소악마))
				BASE:A:체력 -= 300
			ELSE
				BASE:A:체력 -= 200
			ENDIF
			
			;-----------------------
			;방뇨 감상(옵션유, 랜덤 1/5, 노예수)
			;수치(CFLAG:A:165)도 느낀다
			;-----------------------
			IF (FLAG:160 & 192) && RAND:4 == 0 && LOCAL:1
				CFLAG:A:166 += 1
				;여유
				IF ABL:A:마조끼 >= 5 || TALENT:A:마조 || EXP:A:배뇨경험 >= 50 || ABL:A:스캇중독 >= 3
					BASE:A:체력 -= 50
					CFLAG:A:165 += 1
				;지친다
				ELSEIF ABL:A:마조끼 < 3 || TALENT:A:도착적 || EXP:A:배뇨경험 < 20
					BASE:A:체력 -= 200
					CFLAG:A:165 += 3
				ELSE
					BASE:A:체력 -= 100
					CFLAG:A:165 += 2
				ENDIF
			ENDIF
			
			;-----------------------
			;수치플레이(노예수만)
			;-----------------------
			IF LOCAL:1
				IF RAND:3 == 0
					CFLAG:A:165 += 5
				ELSEIF RAND:2 == 0
					CFLAG:A:165 += 3
				ELSE
					CFLAG:A:165 += 1
				ENDIF
			ENDIF
		ENDIF
		
		;실전이 있을까 판정
		;노예공
		IF LOCAL:2
			;실전 없음(노예동정로 옵션이 없는, 혹은 랜덤 1/5)
			IF (TALENT:A:동정 && (FLAG:160 & 4) == 0) || RAND:4 == 0
				BASE:A:체력 -= 100
			;-----------------------
			;실전 있어
			;-----------------------
			ELSE
				CFLAG:A:170 += 1
				BASE:A:체력 -= 200
				;-----------------------
				;동정 먹어
				;-----------------------
				IF TALENT:A:동정 && (FLAG:160 & 4)
					CFLAG:A:175 += 1
					BASE:A:체력 -= 200
				ENDIF
			ENDIF
		;노예수
		ELSE
			;V실전 없음(노예처녀로 옵션이 없는, 혹은 랜덤 1/5, 혹은바기나 없다)
			IF !VAGINA(A) || ((TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1) == 0) || RAND:4 == 0
				SIF VAGINA(A)
					BASE:A:체력 -= 100
			;-----------------------
			;V실전 있어
			;-----------------------
			ELSE
				CFLAG:A:161 += 1
				BASE:A:체력 -= 200
				;-----------------------
				;처녀간통
				;-----------------------
				IF (TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1)
					CFLAG:A:173 += 1
					BASE:A:체력 -= 300
				ENDIF
			ENDIF
			
			;A실전 없음(노예처녀로 옵션이 없는, 혹은 랜덤 1/5(여자는 3/4))
			IF (TALENT:A:백버진 && (FLAG:160 & 2) == 0) || (VAGINA(A) && RAND:3 > 0) || (RAND:4 == 0)
				BASE:A:체력 -= 100
			;-----------------------
			;A실전 있어
			;-----------------------
			ELSE
				CFLAG:A:162 += 1
				BASE:A:체력 -= 200
				;-----------------------
				;백버진간통
				;-----------------------
				IF TALENT:A:백버진 && (FLAG:160 & 2)
					CFLAG:A:174 += 1
					BASE:A:체력 -= 300
				ENDIF
			ENDIF
		ENDIF
		
		;수치 보정
		IF RAND:3 == 0
			CFLAG:A:165 += 3
		ELSEIF RAND:2 == 0
			CFLAG:A:165 += 2
		ELSE
			CFLAG:A:165 += 1
		ENDIF
		IF TALENT:A:부끄럼쟁이
			CFLAG:A:165 *= 2
		ELSEIF TALENT:A:수치심없음
			CFLAG:A:165 /= 2
		ENDIF
		SIF CFLAG:A:165 < 1
			CFLAG:A:165 = 1
		
		SIF BASE:A:체력 < 1
			BASE:A:체력 = 1
	ENDIF
REND




;====================================================================
;돌아왔을 때의 처리
;====================================================================
@END_PROSTITUTION
DRAWLINE
PRINTFORML %CALLNAME:A%가 돌아왔다

;보존해 둔 것 호출
B = CFLAG:A:18
S = CFLAG:A:19

;이벤트대사의 표시
T = TARGET
TARGET = A
TFLAG:13 = 69
CALL KOJO_MESSAGE_EVENT
TARGET = T




;--------------------------------------------------------
;경험의 가산 처리와 표시
C = 0
D = 0
E = 0
F = 0
G = 0
L = 0
Q = 0
R = 0
U = 0
;--------------------------------------------------------
PRINTFORML 【%CALLNAME:A%의 경험과 구슬】

;-----------------------------
;창관봉공경험
;-----------------------------
EXP:A:창관봉공경험 += B
PRINTFORML 창관봉공경험＋{B}


;-----------------------------
;성교, 절정계
C = 0
E = 0
F = 0
;-----------------------------
;C =회수와쾌C의 주용
IF !TEQUIP:A:정조대 && (CFLAG:A:160 || CFLAG:A:169 || CFLAG:A:170 || CFLAG:A:175)
	;성적 봉사(수/공), 역섹스
	C += CFLAG:A:160+CFLAG:A:169+CFLAG:A:170
	
	;U =절정 회수 보존
	U += C/3
	
	;습득의 주용
	Q += (CFLAG:A:169+CFLAG:A:170)*(ABL:A:욕망+1)
	;쾌C의 주용
	C += C*(ABL:A:C감각+1)
ENDIF

;E =회수와쾌V의 주용
IF !TEQUIP:A:정조대 && VAGINA(A) && (CFLAG:A:160 || CFLAG:A:161 || CFLAG:A:173)
	;성적 봉사(수), V섹스
	E += CFLAG:A:160+CFLAG:A:161
	
	;U =절정 회수 보존
	U += E/3
	
	EXP:A:V경험 += E
	PRINTFORML %EXPNAME:0%＋{E}
	
	;쾌V의 주용
	E += E*(ABL:A:V감각+1)
ENDIF

;F =회수와쾌A의 주용
IF CFLAG:A:160 || CFLAG:A:162 || CFLAG:A:168 || CFLAG:A:174
	;성적 봉사(수), A섹스, 관장플레이
	F += CFLAG:A:160
	F += CFLAG:A:162
	F += CFLAG:A:168/3
	
	;U =절정 회수 보존
	U += F/3
	
	EXP:A:A경험 += F
	PRINTFORML %EXPNAME:1%＋{F}
	
	;쾌A의 주용
	F += F*(ABL:A:A감각+1)
		
	;고통의 주용
	SIF CFLAG:A:168
		D += CFLAG:A:168
ENDIF

;L = 성교경험
IF CFLAG:A:161 || CFLAG:A:162 || CFLAG:A:170
	L = CFLAG:A:161+CFLAG:A:162+CFLAG:A:170
	
	EXP:A:성교경험 += L
	PRINTFORML 성교경험＋{L}
	
	IF CFLAG:A:170
		EXP:A:삽입경험 += L
		PRINTFORML 삽입경험＋{L}
	ENDIF
	
	;동정상실역섹스때만
	IF CFLAG:A:175
		PRINTFORML 창관봉공으로동정상실
		PRINTFORML 이상경험＋1
		EXP:A:이상경험 += 1
		TALENT:A:동정 = 0
		CSTR:A:11 =창관봉공
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += E+F
		SIF TALENT:A:친애
			G += (E+F)*2
	ENDIF
	
	;처녀상실
	IF CFLAG:A:173 && TALENT:A:처녀
		PRINTFORML 창관봉공으로처녀상실
		PRINTFORML 이상경험＋1
		EXP:A:이상경험 += 1
		TALENT:A:처녀 = 0
		CSTR:A:10 =창관봉공
		
		;고통의 주용
		D += 10
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*3
		SIF TALENT:A:친애
			G += (E+F)*5
	ELSEIF CFLAG:A:173 && TALENT:A:재생처녀
		PRINTFORML 창관봉공으로재생처녀상실
		TALENT:A:재생처녀 = 0
		
		;고통의 주용
		D += 5
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*2
		SIF TALENT:A:친애
			G += (E+F)*3
	ENDIF
	
	;질내사정경험
	IF E && (CFLAG:A:108 == 0 || TEQUIP:A:정조대 == 0)
		EXP:A:질내사정경험 += E/2
		;창관 따위의 손님과 노예 사이의 나카타씨 카운트용
		CFLAG:A:105 += E/2
		PRINTFORML %EXPNAME:5%＋{E/2}
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*2
		SIF TALENT:A:친애
			G += (E+F)*3
	ENDIF
	
	;백버진상실
	IF CFLAG:A:174
		PRINTFORML 창관봉공으로백버진상실
		PRINTFORML 이상경험＋1
		EXP:A:이상경험 += 1
		TALENT:A:백버진 = 0
		CSTR:A:12 =창관봉공
		
		;고통의 주용
		D += 10
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*2
		SIF TALENT:A:친애
			G += (E+F)*3
	ENDIF
	
	;장내사정경험
	IF F && CFLAG:A:108 == 0
		EXP:A:장내사정경험 += F/2
		;창관 따위의 손님과 노예 사이의 장나카타씨 카운트용
		CFLAG:A:705 += F/2
		PRINTFORML %EXPNAME:14%＋{F/2}
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*2
		SIF TALENT:A:친애
			G += (E+F)*3
	ENDIF
	
	;U =절정 회수 보존
	U += L/3
ENDIF

;U =절정 회수
IF U
	EXP:A:절정경험 += U
	PRINTFORML 절정경험＋{U}
	
	IF PENIS(A)
		IF TALENT:A:아이사이즈
			EXP:A:허공사정경험 += U
			PRINTFORML 허공사정경험＋{U}
		ELSEIF TEQUIP:A:정조대
			EXP:A:사정경험 += U/2
			PRINTFORML 사정경험＋{U/2}
		ELSE
			EXP:A:사정경험 += U
			PRINTFORML 사정경험＋{U}
		ENDIF
	ENDIF
ENDIF


;-----------------------------
;봉사계
;-----------------------------
IF CFLAG:A:160 || CFLAG:A:169
	L = CFLAG:A:160+CFLAG:A:169
	
	IF CFLAG:A:160
		EXP:A:펠라경험 += L
		PRINTFORML 펠라경험＋{L}
		EXP:A:정액경험 += L/2
		PRINTFORML 정액경험＋{L/2}
	ENDIF
	
	EXP:A:봉사쾌락경험 += L*ABL:A:봉사정신
	PRINTFORML 봉사쾌락경험＋{L*ABL:A:봉사정신}
	
	;습득의 주용
	Q += L*(ABL:A:봉사정신+1)
	
	;불쾌의 구슬
	SIF TALENT:A:연모
		G += E+F
	SIF TALENT:A:친애
		G += (E+F)*2
ENDIF


;-----------------------------
;SM계
;-----------------------------
;공
L = 0
IF CFLAG:A:171 || CFLAG:A:172
	L += CFLAG:A:171
	L += CFLAG:A:172*2
	
	IF ABL:A:새드끼 >= 8 || TALENT:A:새드 == 2
		L *= 3
	ELSEIF ABL:A:새드끼 >= 5 || TALENT:A:새드
		L *= 2
	ELSEIF ABL:A:새드끼 >= 3
	ELSEIF ABL:A:새드끼 == 2 || TALENT:A:소악마
		L /= 2
	ELSEIF ABL:A:새드끼 == 1
		L /= 3
	ELSE
		L /= 5
	ENDIF
	
	IF L > 0
		EXP:A:가학쾌락경험 += L
		PRINTFORML 가학쾌락경험＋{L}
	ENDIF
	
	;습득의 주용
	Q += L*(ABL:A:새드끼+1)
ENDIF

;수
L = 0
IF CFLAG:A:163 || CFLAG:A:164
	L += CFLAG:A:163
	L += CFLAG:A:164*3
	
	EXP:A:속박경험 += (CFLAG:A:163+CFLAG:A:164)/2
	PRINTFORML 속박경험＋{(CFLAG:A:163+CFLAG:A:164)/2}
	
	IF ABL:A:마조끼 >= 8 || TALENT:A:마조 == 2
		L *= 3
	ELSEIF ABL:A:마조끼 >= 5 || TALENT:A:마조
		L *= 2
	ELSEIF ABL:A:마조끼 >= 3
	ELSEIF ABL:A:마조끼 == 2 || TALENT:A:도착적
		L /= 2
	ELSEIF ABL:A:마조끼 == 1
		L /= 3
	ELSE
		L /= 5
	ENDIF
	
	IF L > 0
		EXP:A:고통쾌락경험 += L
		PRINTFORML 고통쾌락경험＋{L}
	ENDIF
	
	;습득의 주용
	Q += L*(ABL:A:마조끼+1)
	
	;고통, 공포의 주용
	D += L
ENDIF


;-----------------------------
;스카톨로지계
L = 0
;-----------------------------
IF CFLAG:A:166 || CFLAG:A:167 || CFLAG:A:168
	;관장플레이
	IF CFLAG:A:168
		;하도SM는 수가 증가한다
		IF CFLAG:A:164
			L += CFLAG:A:168*2
			
			;고통, 공포의 주용
			D += CFLAG:A:168*2
		ELSE
			L += CFLAG:A:168
			
			;고통, 공포의 주용
			D += CFLAG:A:168
		ENDIF
		
		EXP:A:관장경험 += CFLAG:A:168*2
		PRINTFORML 관장경험＋{CFLAG:A:168*2}
		EXP:A:배변경험 += CFLAG:A:168
		PRINTFORML 배변경험＋{CFLAG:A:168}
		
		;불쾌의 구슬
		G += (10-ABL:A:스캇중독)*2
	ENDIF
	
	;음뇨플레이
	IF CFLAG:A:167
		L += CFLAG:A:167
		
		EXP:A:음뇨경험 += CFLAG:A:167
		PRINTFORML 음뇨경험＋{CFLAG:A:167}
		
		;불쾌의 구슬
		G += (10-ABL:A:스캇중독)*2
	ENDIF
	
	;방뇨 감상
	IF CFLAG:A:166
		L += CFLAG:A:166
		
		EXP:A:배뇨경험 += CFLAG:A:166
		PRINTFORML 배뇨경험＋{CFLAG:A:166}
		
		;불쾌의 구슬
		G += 10-ABL:A:스캇중독
	ENDIF
	
	IF !EXP:A:스캇경험
		PRINTFORML 이상경험＋1
		EXP:A:이상경험 += 1
	ENDIF
	
	EXP:A:스캇경험 += L
	PRINTFORML 스캇경험＋{L}
ENDIF


;-----------------------------
;수치
;-----------------------------
R = CFLAG:A:165


;--------------------------------------------------------
;구슬
;C = 쾌C
;E = 쾌V
;F = 쾌A
;D = 고통, 공포의 구슬
;G = 불쾌의 구슬
;Q = 습득의 구슬
;R = 수치의 구슬
;S =금액
;--------------------------------------------------------
IF C > 0
	JUEL:A:쾌C += C*50
	PRINTFORML 쾌C의 구슬＋{C*50}
ENDIF
IF E > 0
	JUEL:A:쾌V += E*30
	PRINTFORML 쾌V의 구슬＋{E*30}
ENDIF
IF F > 0
	JUEL:A:쾌A += F*30
	PRINTFORML 쾌A의 구슬＋{F*30}
ENDIF

JUEL:A:순종 += B*100
PRINTFORML 순종의 구슬＋{B*100}

JUEL:A:습득 += Q*35
PRINTFORML 습득의 구슬＋{Q*35}

IF R > 0
	JUEL:A:수치 += R*50
	PRINTFORML 수치의 구슬＋{R*50}
ENDIF

IF TEQUIP:A:정조대
	JUEL:A:욕정 += B*200+1000
	PRINTFORML 욕정의 구슬＋{B*200+1000}
ELSE
	JUEL:A:욕정 += B*50
	PRINTFORML 욕정의 구슬＋{B*50}
ENDIF

IF D > 0
	JUEL:A:고통 += D*30
	PRINTFORML 고통의 구슬＋{D*30}
	JUEL:A:공포 += D*15
	PRINTFORML 공포의 구슬＋{D*15}
ENDIF
