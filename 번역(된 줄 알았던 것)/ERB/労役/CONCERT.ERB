		PRINTFORML [500] - 멤버를 리셋트
	SIF A > 0
		PRINTL [800] - 멤버를 확정한다
	PRINTL [999] - 돌아온다
	$INPUT_LOOP
	INPUT
	IF RESULT == 999
		IF A > 0
			CFLAG:A:18 = 0
			A = 0
		ENDIF
		IF A:1 > 0
			CFLAG:(A:1):18 = 0
			A:1 = 0
		ENDIF
		IF A:2 > 0
			CFLAG:(A:2):18 = 0
			A:2 = 0
		ENDIF
		IF A:3 > 0
			CFLAG:(A:3):18 = 0
			A:3 = 0
		ENDIF
		IF A:4 > 0
			CFLAG:(A:4):18 = 0
			A:4 = 0
		ENDIF
		IF A:5 > 0
			CFLAG:(A:5):18 = 0
			A:5 = 0
		ENDIF
		RETURN 0
	ELSEIF RESULT == 800
		BREAK
	ELSEIF RESULT == 500
		IF A > 0
			CFLAG:A:18 = 0
			A = 0
		ENDIF
		IF A:1 > 0
			CFLAG:(A:1):18 = 0
			A:1 = 0
		ENDIF
		IF A:2 > 0
			CFLAG:(A:2):18 = 0
			A:2 = 0
		ENDIF
		IF A:3 > 0
			CFLAG:(A:3):18 = 0
			A:3 = 0
		ENDIF
		IF A:4 > 0
			CFLAG:(A:4):18 = 0
			A:4 = 0
		ENDIF
		IF A:5 > 0
			CFLAG:(A:5):18 = 0
			A:5 = 0
		ENDIF
		G = 0
	ELSEIF RESULT < 0 || RESULT >= CHARANUM
		GOTO INPUT_LOOP
	ELSEIF RESULT == MASTER
		GOTO INPUT_LOOP
	ELSEIF CFLAG:RESULT:1 < 2
		GOTO INPUT_LOOP
	ELSEIF CFLAG:RESULT:12 > 0
		GOTO INPUT_LOOP
	ELSEIF ((CFLAG:RESULT:151 & 2) == 0) || BASE:RESULT:체력 < 800
		GOTO INPUT_LOOP
	ELSE
		;미선택의 캐릭터를 추가했을 경우
		IF CFLAG:RESULT:18 == 0 && G < 6
			LOCAL = RESULT
			CFLAG:LOCAL:18 = 1
			PRINTFORML %CALLNAME:LOCAL%를 어디에 자리잡습니까?
			PRINTFORML [ 0] - 대장
			PRINTFORML [ 1] - 부대장
			PRINTFORML [ 2] - 그 외
			$INPUT_LOOP1
			INPUT
			IF RESULT == 0
				IF A > 0
					CFLAG:A:18 = 0
					A = 0
				ENDIF
				A = LOCAL
				G += 1
			ELSEIF RESULT == 1
				IF A:1 > 0
					CFLAG:(A:1):18 = 0
					A:1 = 0
				ENDIF
				A:1 = LOCAL
				G += 1
			ELSEIF RESULT == 2
				IF A:2 == 0
					A:2 = LOCAL
				ELSEIF A:3 == 0
					A:3 = LOCAL
				ELSEIF A:4 == 0
					A:4 = LOCAL
				ELSEIF A:5 == 0
					A:5 = LOCAL
				ENDIF
				G += 1
			ELSE
				GOTO INPUT_LOOP1
			ENDIF
		;벌써 선택되고 있는 캐릭터를 재선택 했을 경우
		ELSE
			IF RESULT == A
				CFLAG:A:18 = 0
				A = 0
			ELSEIF RESULT == A:1
				CFLAG:(A:1):18 = 0
				A:1 = 0
			ELSEIF RESULT == A:2
				CFLAG:(A:2):18 = 0
				A:2 = 0
			ELSEIF RESULT == A:3
				CFLAG:(A:3):18 = 0
				A:3 = 0
			ELSEIF RESULT == A:4
				CFLAG:(A:4):18 = 0
				A:4 = 0
			ELSE
				CFLAG:(A:5):18 = 0
				A:5 = 0
			ENDIF
			G -= 1
			SIF G < 0
				G = 0
		ENDIF
	ENDIF
WEND
