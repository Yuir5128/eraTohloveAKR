;역섹스		CFLAG:A:164
;처녀관통		CFLAG:A:165
;A처녀관통		CFLAG:A:166
;동정 먹어		CFLAG:A:167
;소프트 SM(수)	CFLAG:A:168
;소프트 SM(공)	CFLAG:A:169
;하도SM(수)	CFLAG:A:170
;하도SM(공)	CFLAG:A:171
;수치심			CFLAG:A:172
;복수인(수)		CFLAG:A:173
;복수인(공)		CFLAG:A:174
;수간			CFLAG:A:175
;방뇨 감상		CFLAG:A:176
;음뇨플레이		CFLAG:A:177
;관장플레이		CFLAG:A:178
;식분			CFLAG:A:179

CFLAG:A:160 = 0
CFLAG:A:161 = 0
CFLAG:A:162 = 0
CFLAG:A:163 = 0
CFLAG:A:164 = 0
CFLAG:A:165 = 0
CFLAG:A:166 = 0
CFLAG:A:167 = 0
CFLAG:A:168 = 0
CFLAG:A:169 = 0
CFLAG:A:170 = 0
CFLAG:A:171 = 0
CFLAG:A:172 = 0
CFLAG:A:173 = 0
CFLAG:A:174 = 0
CFLAG:A:175 = 0
CFLAG:A:176 = 0
CFLAG:A:177 = 0
CFLAG:A:178 = 0
CFLAG:A:179 = 0


;--------------------------------------------------------
REPEAT 30
	;최대 시행 회수 넘으면 빠진다
	IF X == LOCAL
		BREAK
	;체력 없으면 휴게. 쉬면체력회복
	ELSEIF BASE:A:체력 < 500
		IF TALENT:A:회복빠름 || TALENT:A:창부 || TALENT:A:예속 || ABL:A:마조끼 >= 5
			BASE:A:체력 += 300
		ELSEIF TALENT:A:회복느림 || TALENT:A:발정기 || ABL:A:마조끼 < 3
			BASE:A:체력 += 100
		ELSE
			BASE:A:체력 += 200
		ENDIF
	;도 없으면 플레이 판정
	ELSE
		;-----------------------
		;수간
		;-----------------------
		IF CFLAG:A:152 & 1
			CFLAG:A:175 += 1
			;여유
			IF EXP:수간경험 >= EXPLV:5
				BASE:A:체력 -= 100
			;지친다
			ELSEIF EXP:수간경험 < EXPLV:2
				BASE:A:체력 -= 300
			ELSE
				BASE:A:체력 -= 200
			ENDIF
		ENDIF
		
		;-----------------------
		;스카톨로지
		;-----------------------
		IF CFLAG:A:152 & 2
			;-----------------------
			;식분플레이
			;-----------------------
			IF (FLAG:160 & 512) && LOCAL:4 && !RAND:3
				CFLAG:A:179 += 1
				;여유
				IF ABL:A:마조끼 >= 5 || TALENT:A:마조 || EXP:A:식분경험 >= 50 || ABL:A:스캇중독 >= 3
					BASE:A:체력 -= 200
				;지친다
				ELSEIF ABL:A:마조끼 < 3 || EXP:A:식분경험 < 20
					BASE:A:체력 -= 500
				ELSE
					BASE:A:체력 -= 350
				ENDIF
			ENDIF
			
			;-----------------------
			;관장플레이
			;-----------------------
			IF (FLAG:160 & 256) && ((LOCAL:3 && !RAND:2) || LOCAL:4)
				CFLAG:A:178 += 1
				;여유
				IF ABL:A:마조끼 >= 5 || TALENT:A:마조 || EXP:A:관장경험 >= 50 || ABL:A:스캇중독 >= 3
					BASE:A:체력 -= 200
					CFLAG:A:172 += 1
				;지친다
				ELSEIF ABL:A:마조끼 < 3 || EXP:A:관장경험 < 20
					BASE:A:체력 -= 500
					CFLAG:A:172 += 5
				ELSE
					BASE:A:체력 -= 350
					CFLAG:A:172 += 3
				ENDIF
			ENDIF
			
			;-----------------------
			;음뇨플레이
			;-----------------------
			IF (FLAG:160 & 128) && ((LOCAL:3 && !RAND:3) || (LOCAL:4 && !RAND:2))
				CFLAG:A:177 += 1
				;여유
				IF ABL:A:마조끼 >= 5 || TALENT:A:마조 || EXP:A:음뇨경험 >= 50 || ABL:A:스캇중독 >= 3
					BASE:A:체력 -= 50
				;지친다
				ELSEIF ABL:A:마조끼 < 3 || EXP:A:음뇨경험 < 10
					BASE:A:체력 -= 300
				ELSE
					BASE:A:체력 -= 150
				ENDIF
			ENDIF
			
			;-----------------------
			;방뇨 감상
			;-----------------------
			IF (FLAG:160 & 64) && !RAND:3
				CFLAG:A:176 += 1
				;여유
				IF ABL:A:마조끼 >= 5 || TALENT:A:마조 || EXP:A:배뇨경험 >= 50 || ABL:A:스캇중독 >= 3
					BASE:A:체력 -= 50
					CFLAG:A:172 += 1
				;지친다
				ELSEIF ABL:A:마조끼 < 3 || EXP:A:배뇨경험 < 20
					BASE:A:체력 -= 200
					CFLAG:A:172 += 5
				ELSE
					BASE:A:체력 -= 100
					CFLAG:A:172 += 3
				ENDIF
			ENDIF
		ENDIF
		
		;-----------------------
		;하도SM(노예공)
		;-----------------------
		IF CFLAG:A:152 & 4
			CFLAG:A:171 += 1
			;여유
			IF LOCAL:2 && (ABL:A:새드끼 >= 5 || TALENT:A:새드)
				BASE:A:체력 -= 300
			;지친다
			ELSEIF LOCAL:2 && (ABL:A:새드끼 < 3 || TALENT:A:소악마)
				BASE:A:체력 -= 800
			ELSE
				BASE:A:체력 -= 500
			ENDIF
		ENDIF
		
		;-----------------------
		;하도SM(노예수)
		;-----------------------
		IF CFLAG:A:152 & 8
			CFLAG:A:170 += 1
			;여유
			IF LOCAL:1 && (ABL:A:마조끼 >= 5 || TALENT:A:마조)
				BASE:A:체력 -= 300
			;지친다
			ELSEIF LOCAL:1 && (ABL:A:마조끼 < 3 || !TALENT:A:도착적)
				BASE:A:체력 -= 800
			ELSE
				BASE:A:체력 -= 500
			ENDIF
		ENDIF
		
		;-----------------------
		;소프트 SM(노예공)
		;-----------------------
		IF CFLAG:A:152 & 16
			CFLAG:A:169 += 1
			;여유
			IF LOCAL:2 && (ABL:A:새드끼 >= 5 || TALENT:A:새드)
				BASE:A:체력 -= 100
			;지친다
			ELSEIF LOCAL:2 && (ABL:A:새드끼 < 3 || TALENT:A:소악마)
				BASE:A:체력 -= 300
			ELSE
				BASE:A:체력 -= 200
			ENDIF
		ENDIF
		
		;-----------------------
		;소프트 SM(노예수)
		;-----------------------
		IF CFLAG:A:152 & 32
			CFLAG:A:168 += 1
			;여유
			IF LOCAL:1 && (ABL:A:마조끼 >= 5 || TALENT:A:마조)
				BASE:A:체력 -= 100
			;지친다
			ELSEIF LOCAL:1 && (ABL:A:마조끼 < 3 || TALENT:A:도착적)
				BASE:A:체력 -= 300
			ELSE
				BASE:A:체력 -= 200
			ENDIF
		ENDIF
		
		;-----------------------
		;복수인(공)
		;-----------------------
		IF CFLAG:A:152 & 64
			CFLAG:A:174 += 1
			BASE:A:체력 -= 200
		ENDIF
		
		;-----------------------
		;복수인(수)
		;-----------------------
		IF CFLAG:A:152 & 128
			CFLAG:A:173 += 1
			;여유
			IF EXP:윤간경험 >= EXPLV:5
				BASE:A:체력 -= 100
				CFLAG:A:172 += 1
			;지친다
			ELSEIF EXP:윤간경험 < EXPLV:2
				BASE:A:체력 -= 300
				CFLAG:A:172 += 3
			ELSE
				BASE:A:체력 -= 200
				CFLAG:A:172 += 2
			ENDIF
		ENDIF
		
		;-----------------------
		;수치계
		;-----------------------
		IF CFLAG:A:152 & 512
			;여유
			IF ABL:A:노출벽 >= 5 || TALENT:A:수치심없음
				CFLAG:A:172 += 5
			;부끄럽다
			ELSEIF ABL:A:노출벽 < 3 || TALENT:A:부끄럼쟁이
				CFLAG:A:172 += 15
			ELSE
				CFLAG:A:172 += 10
			ENDIF
		ENDIF
		
		;-----------------------
		;실전이 있을까 판정(페티시즘, 초조하게 해 꾸짖고 이외)
		;-----------------------
		IF (CFLAG:A:152 & 256) == 0 && (CFLAG:A:152 & 4096) == 0
			;노예공
			IF LOCAL:2
				;실전 없음(노예동정로 옵션이 없는, 혹은 랜덤 1/5)
				IF TALENT:A:동정 && (FLAG:160 & 4) == 0 || RAND:4 == 0
					BASE:A:체력 -= 100
				;-----------------------
				;실전 있어
				;-----------------------
				ELSE
					CFLAG:A:164 += 1
					BASE:A:체력 -= 200
					;-----------------------
					;동정 먹어
					;-----------------------
					IF TALENT:A:동정 && (FLAG:160 & 4)
						CFLAG:A:167 += 1
						BASE:A:체력 -= 200
					ENDIF
				ENDIF
			;노예수
			ELSE
				;V실전 없음(노예처녀로 옵션이 없는, 혹은 랜덤 1/5, 혹은바기나 없다)
				IF !VAGINA(A) || ((TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1) == 0)
					SIF VAGINA(A)
						BASE:A:체력 -= 100
				;-----------------------
				;V실전 있어
				;-----------------------
				ELSE
					CFLAG:A:162 += 1
					BASE:A:체력 -= 200
					;-----------------------
					;처녀간통
					;-----------------------
					IF (TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1)
						CFLAG:A:165 += 1
						BASE:A:체력 -= 300
					ENDIF
				ENDIF
				
				;A실전 없음(노예처녀로 옵션이 없는, 혹은 랜덤 1/5(여자는 3/4))
				IF (TALENT:A:백버진 && (FLAG:160 & 2) == 0) || (VAGINA(A) && RAND:3 > 0)
					BASE:A:체력 -= 100
				;-----------------------
				;A실전 있어
				;-----------------------
				ELSE
					CFLAG:A:163 += 1
					BASE:A:체력 -= 200
					;-----------------------
					;백버진간통
					;-----------------------
					IF TALENT:A:백버진 && (FLAG:160 & 2)
						CFLAG:A:166 += 1
						BASE:A:체력 -= 300
					ENDIF
				ENDIF
			ENDIF
		ENDIF
		
		;-----------------------
		;성적 봉사는 절대로 있다
		;-----------------------
		;노예공
		IF LOCAL:2
			CFLAG:A:161 += 1
			BASE:A:체력 -= 150
		;노예수
		ELSE
			;쾌락 고문이나 초조하게 해 고문 때는, 회수배에.
			IF CFLAG:A:152 & 5120
				CFLAG:A:160 += 2
				BASE:A:체력 -= 200
			ELSE
				CFLAG:A:160 += 1
				BASE:A:체력 -= 100
			ENDIF
		ENDIF
		
	ENDIF
	X += 1
REND




;====================================================================
;렌탈의 결과는 반환시에 처리
;====================================================================
@RETURN_SLAVE
DRAWLINE
PRINTFORML %CALLNAME:A%가 돌아왔다

;보완하고 있었던 플래그를 변수에 대입
B = CFLAG:A:14
S = CFLAG:A:15

;이벤트입상의 표시
T = TARGET
TARGET = A
TFLAG:13 = 17
CALL KOJO_MESSAGE_EVENT
TARGET = T


;--------------------------------------------------------
;경험의 가산 처리와 표시
C = 0
D = 0
E = 0
F = 0
G = 0
L = 0
Q = 0
R = 0
U = 0
V = 0
;--------------------------------------------------------
PRINTFORML 【%CALLNAME:A%의 경험과 구슬】


;-----------------------------
;봉사계
;-----------------------------
IF CFLAG:A:160 || CFLAG:A:161
	L = CFLAG:A:160+CFLAG:A:161
	
	IF CFLAG:A:160
		EXP:A:펠라경험 += L
		PRINTFORML 펠라경험＋{L}
		EXP:A:정액경험 += L/2
		PRINTFORML 정액경험＋{L/2}
	ENDIF
	
	EXP:A:봉사쾌락경험 += L*ABL:A:봉사정신
	PRINTFORML 봉사쾌락경험＋{L*ABL:A:봉사정신}
	
	;습득의 주용
	Q += L*(ABL:A:봉사정신+1)
	
	;불쾌의 구슬
	SIF TALENT:A:연모
		G += E+F
	SIF TALENT:A:친애
		G += (E+F)*2
ENDIF


;-----------------------------
;성교, 절정계
;-----------------------------
;C =회수와쾌C의 주용
IF !TEQUIP:A:정조대 && (CFLAG:A:160 || CFLAG:A:161 || CFLAG:A:164 || CFLAG:A:167)
	;성적 봉사(수/공), 역섹스
	C += CFLAG:A:160+CFLAG:A:161+CFLAG:A:164
	
	;U =절정 회수 보존
	U += C/3
	
	;습득의 주용
	Q += (CFLAG:A:161+CFLAG:A:164)*(ABL:A:욕망+1)
	;쾌C의 주용
	C += C*(ABL:A:C감각+1)
ENDIF

;E =회수와쾌V의 주용
IF !TEQUIP:A:정조대 && VAGINA(A) && (CFLAG:A:160 || CFLAG:A:162 || CFLAG:A:165)
	;성적 봉사(수), V섹스
	E += CFLAG:A:160+CFLAG:A:162
	
	;U =절정 회수 보존
	U += E/3
	
	EXP:A:V경험 += E
	PRINTFORML %EXPNAME:0%＋{E}
	
	;쾌V의 주용
	E += E*(ABL:A:V감각+1)
ENDIF

;F =회수와쾌A의 주용
IF CFLAG:A:160 || CFLAG:A:163 || CFLAG:A:166 || CFLAG:A:178
	;성적 봉사(수), A섹스, 관장플레이
	F += CFLAG:A:160
	F += CFLAG:A:163
	F += CFLAG:A:178/3
	
	;U =절정 회수 보존
	U += F/3
	
	EXP:A:A경험 += F
	PRINTFORML %EXPNAME:1%＋{F}
	
	;쾌A의 주용
	F += F*(ABL:A:A감각+1)
		
	;고통의 주용
	SIF CFLAG:A:178
		D += CFLAG:A:178
ENDIF

;L = 성교경험
L = 0
IF CFLAG:A:162 || CFLAG:A:163 || CFLAG:A:164
	L = CFLAG:A:162+CFLAG:A:163+CFLAG:A:164
	
	EXP:A:성교경험 += L
	PRINTFORML 성교경험＋{L}
	
	IF CFLAG:A:164
		EXP:A:삽입경험 += L
		PRINTFORML 삽입경험＋{L}
	ENDIF
	
	;동정상실역섹스때만
	IF CFLAG:A:167
		PRINTFORML 노예 렌탈로동정상실
		PRINTFORML 이상경험＋1
		EXP:A:이상경험 += 1
		TALENT:A:동정 = 0
		CSTR:A:11 =노예 렌탈
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += E+F
		SIF TALENT:A:친애
			G += (E+F)*2
	ENDIF
	
	;처녀상실
	IF CFLAG:A:165 && TALENT:A:처녀
		PRINTFORML 노예 렌탈로처녀상실
		PRINTFORML 이상경험＋1
		EXP:A:이상경험 += 1
		TALENT:A:처녀 = 0
		CSTR:A:10 =노예 렌탈
		
		;고통의 주용
		D += 10
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*3
		SIF TALENT:A:친애
			G += (E+F)*5
	ELSEIF CFLAG:A:165 && TALENT:A:재생처녀
		PRINTFORML 노예 렌탈로재생처녀상실
		TALENT:A:재생처녀 = 0
		
		;고통의 주용
		D += 5
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*2
		SIF TALENT:A:친애
			G += (E+F)*3
	ENDIF
	
	;질내사정경험
	IF E && (CFLAG:A:108 == 0 || TEQUIP:A:정조대 == 0)
		EXP:A:질내사정경험 += E/2
		;창관 따위의 손님과 노예 사이의 나카타씨 카운트용
		CFLAG:A:105 += E/2
		PRINTFORML %EXPNAME:5%＋{E/2}
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*2
		SIF TALENT:A:친애
			G += (E+F)*3
	ENDIF
	
	;백버진상실
	IF CFLAG:A:166
		PRINTFORML 노예 렌탈로백버진상실
		PRINTFORML 이상경험＋1
		EXP:A:이상경험 += 1
		TALENT:A:백버진 = 0
		CSTR:A:12 =노예 렌탈
		
		;고통의 주용
		D += 10
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*2
		SIF TALENT:A:친애
			G += (E+F)*3
	ENDIF
	
	;장내사정경험
	IF F && CFLAG:A:108 == 0
		EXP:A:장내사정경험 += F/2
		;창관 따위의 손님과 노예 사이의 장나카타씨 카운트용
		CFLAG:A:705 += F/2
		PRINTFORML %EXPNAME:14%＋{F/2}
		
		;불쾌의 구슬
		SIF TALENT:A:연모
			G += (E+F)*2
		SIF TALENT:A:친애
			G += (E+F)*3
	ENDIF
	
	;U =절정 회수 보존
	U += L/3
ENDIF

;U =절정 회수
;초조하게 해 고문 때는 절정 할 수 없다
SIF CFLAG:A:152 & 4096
	U = 0
;쾌락 고문 때는 절정 회수 대폭 UP
SIF CFLAG:A:152 & 1024
	U *= 2

IF U
	EXP:A:절정경험 += U
	PRINTFORML 절정경험＋{U}
	
	IF PENIS(A)
		IF TALENT:A:아이사이즈
			EXP:A:허공사정경험 += U
			PRINTFORML 허공사정경험＋{U}
		ELSEIF TEQUIP:A:정조대
			EXP:A:사정경험 += U/2
			PRINTFORML 사정경험＋{U/2}
		ELSE
			EXP:A:사정경험 += U
			PRINTFORML 사정경험＋{U}
		ENDIF
	ENDIF
ENDIF


;--------------------------------------------------------
;수간경험
;--------------------------------------------------------
IF CFLAG:A:175
	;수간 첫체험에 의해이상경험
	IF EXP:A:수간경험 == 0
		EXP:A:이상경험 += 1
		PRINTL 수간첫경험으로이상경험＋1
	ENDIF
	EXP:A:수간경험 += CFLAG:A:175
	PRINTFORML 수간경험＋{CFLAG:A:175}
	
	;불쾌의 구슬
	SIF EXP:A:수간경험 < 30
		G += CFLAG:A:175*5
ENDIF

;--------------------------------------------------------
;윤간경험
;G = 불쾌의 구슬
;--------------------------------------------------------
IF CFLAG:A:173 || CFLAG:A:174
	IF EXP:A:윤간경험 == 0
		EXP:A:이상경험 += 1
		PRINTL 윤간첫경험으로이상경험＋1
	ENDIF
	EXP:A:윤간경험 += CFLAG:A:173+CFLAG:A:174
	PRINTFORML 윤간경험＋{CFLAG:A:173+CFLAG:A:174}
	
	;불쾌의 주용
	IF TALENT:A:친애
		G += (CFLAG:A:173+CFLAG:A:174)*5
	ELSEIF TALENT:A:연모
		G += (CFLAG:A:173+CFLAG:A:174)*2
	ENDIF
ENDIF


;-----------------------------
;SM계
;-----------------------------
;공
L = 0
IF CFLAG:A:169 || CFLAG:A:171
	L += CFLAG:A:169
	L += CFLAG:A:171*2
	
	IF ABL:A:새드끼 >= 8 || TALENT:A:새드 == 2
		L *= 3
	ELSEIF ABL:A:새드끼 >= 5 || TALENT:A:새드
		L *= 2
	ELSEIF ABL:A:새드끼 >= 3
	ELSEIF ABL:A:새드끼 == 2 || TALENT:A:소악마
		L /= 2
	ELSEIF ABL:A:새드끼 == 1
		L /= 3
	ELSE
		L /= 5
	ENDIF
	
	IF L > 0
		EXP:A:가학쾌락경험 += L
		PRINTFORML 가학쾌락경험＋{L}
	ENDIF
	
	;습득의 주용
	Q += L*(ABL:A:새드끼+1)
ENDIF

;수
L = 0
IF CFLAG:A:168 || CFLAG:A:170
	L += CFLAG:A:168
	L += CFLAG:A:170*3
	
	EXP:A:속박경험 += (CFLAG:A:168+CFLAG:A:170)/2
	PRINTFORML 속박경험＋{(CFLAG:A:168+CFLAG:A:170)/2}
	
	IF ABL:A:마조끼 >= 8 || TALENT:A:마조 == 2
		L *= 3
	ELSEIF ABL:A:마조끼 >= 5 || TALENT:A:마조
		L *= 2
	ELSEIF ABL:A:마조끼 >= 3
	ELSEIF ABL:A:마조끼 == 2 || TALENT:A:도착적
		L /= 2
	ELSEIF ABL:A:마조끼 == 1
		L /= 3
	ELSE
		L /= 5
	ENDIF
	
	IF L > 0
		EXP:A:고통쾌락경험 += L
		PRINTFORML 고통쾌락경험＋{L}
	ENDIF
	
	;습득의 주용
	Q += L*(ABL:A:마조끼+1)
	
	;고통, 공포의 주용
	D += L
ENDIF


;-----------------------------
;스카톨로지계
L = 0
;-----------------------------
IF CFLAG:A:176 || CFLAG:A:177 || CFLAG:A:178 || CFLAG:A:179
	;식분플레이
	IF CFLAG:A:179
		L += CFLAG:A:179
		
		EXP:A:식분경험 += CFLAG:A:179
		PRINTFORML 식분경험＋{CFLAG:A:179}
		
		;불쾌의 구슬
		G += (10-ABL:A:스캇중독)*5
	ENDIF
	
	;관장플레이
	IF CFLAG:A:178
		;하도SM는 수가 증가한다
		IF CFLAG:A:170
			L += CFLAG:A:178*2
			
			;고통, 공포의 주용
			D += CFLAG:A:178*2
		ELSE
			L += CFLAG:A:178
			
			;고통, 공포의 주용
			D += CFLAG:A:178
		ENDIF
		
		EXP:A:관장경험 += CFLAG:A:178*2
		PRINTFORML 관장경험＋{CFLAG:A:178*2}
		EXP:A:배변경험 += CFLAG:A:178
		PRINTFORML 배변경험＋{CFLAG:A:178}
		
		;불쾌의 구슬
		G += (10-ABL:A:스캇중독)*2
	ENDIF
	
	;음뇨플레이
	IF CFLAG:A:177
		L += CFLAG:A:177
		
		EXP:A:음뇨경험 += CFLAG:A:177
		PRINTFORML 음뇨경험＋{CFLAG:A:177}
		
		;불쾌의 구슬
		G += (10-ABL:A:스캇중독)*2
	ENDIF
	
	;방뇨 감상
	IF CFLAG:A:176
		L += CFLAG:A:176
		
		EXP:A:배뇨경험 += CFLAG:A:176
		PRINTFORML 배뇨경험＋{CFLAG:A:176}
		
		;불쾌의 구슬
		G += 10-ABL:A:스캇중독
	ENDIF
	
	EXP:A:스캇경험 += L
	PRINTFORML 스캇경험＋{L}
ENDIF


;-----------------------------
;수치
;-----------------------------
R = CFLAG:A:172


;--------------------------------------------------------
;구슬
;E = V, F = A
;C =회수와쾌C의 주용
;R = 수치의 주용
;D = 고통, 공포의 주용
;Q = 습득의 구슬에 사용
;G = 불쾌의 구슬에 사용
;--------------------------------------------------------
IF C > 0
	JUEL:A:쾌C += C*50
	PRINTFORML 쾌C의 구슬＋{C*50}
ENDIF
IF E > 0
	JUEL:A:쾌V += E*30
	PRINTFORML 쾌V의 구슬＋{E*30}
ENDIF
IF F > 0
	JUEL:A:쾌A += F*30
	PRINTFORML 쾌A의 구슬＋{F*30}
ENDIF

JUEL:A:순종 += B*100
PRINTFORML 순종의 구슬＋{B*100}

JUEL:A:습득 += Q*35
PRINTFORML 습득의 구슬＋{Q*35}

IF R > 0
	JUEL:A:수치 += R*50
	PRINTFORML 수치의 구슬＋{R*50}
ENDIF

IF CFLAG:A:152 & 4352 || TEQUIP:A:정조대
	JUEL:A:욕정 += B*200+1000
	PRINTFORML 욕정의 구슬＋{B*200+1000}
ELSE
	JUEL:A:욕정 += B*50
	PRINTFORML 욕정의 구슬＋{B*50}
ENDIF

IF D > 0
	JUEL:A:고통 += D*30
	PRINTFORML 고통의 구슬＋{D*30}
	JUEL:A:공포 += D*15
	PRINTFORML 공포의 구슬＋{D*15}
ENDIF
IF G > 0
	JUEL:A:불쾌 += G*50
	PRINTFORML 불쾌의 구슬＋{G*50}
ENDIF

IF D > 0 || G > 0 || E > 0 || F > 0
	JUEL:A:부정 += ((D+G+E+F)*200) /ABL:A:순종
	PRINTFORML 부정의 구슬＋{((D+G+E+F)*200) /ABL:A:순종}
ENDIF


;--------------------------------------------------------
;체력의 감소, 스트레스, 금욕 포인트
;--------------------------------------------------------

;금욕 포인트
IF TEQUIP:A:정조대 || CFLAG:A:152 & 4352
	CFLAG:A:602 += B*5
	SIF TALENT:PLAYER:천리안
		PRINTFORML 금욕 포인트＋{B*5}
ELSE
	CFLAG:A:602 = 0
ENDIF

;스트레스치의 부가
CFLAG:A:10 += B*2
SIF TALENT:PLAYER:천리안
	PRINTFORML 피로도＋{B}


;-----------------------------
;보수액의 가산
;-----------------------------
;최후에 금액의 가산
;수간(노예수만)
SIF CFLAG:A:175
	S += B*5000
;스카톨로지대(노예수만)
SIF CFLAG:A:179
	S += B*5000
;하도SM
SIF CFLAG:A:170
	S += B*3500
;복수 플레이
SIF CFLAG:A:173 || CFLAG:A:174
	S += B*2000
;스카톨로지소(노예수만)
SIF CFLAG:A:176 || CFLAG:A:177
	S += B*1000

;만발 해 받을 수 없었던 경우는 오히려 줄인다
;하도SM공
SIF CFLAG:A:171 && ABL:A:새드끼 < 4
	S -= B*5000
;소프트 SM공
SIF CFLAG:A:169 && ABL:A:새드끼 < 2
	S -= B*3500

PRINTFORMW %CALLNAME:A%의 렌탈 이마로서{S}고슈금를 얻었습니다
MONEY += S

;플래그의 리셋트
CFLAG:A:12 = 0
CFLAG:A:13 = 0
CFLAG:A:14 = 0
CFLAG:A:15 = 0


RETURN 1




