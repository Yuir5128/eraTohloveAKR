;--------------------------------------------------------
;렌탈 대상의 선택
;--------------------------------------------------------
PRINTL 렌탈하는 노예를 선택해 주세요
REPEAT CHARANUM
	SIF COUNT == MASTER
		CONTINUE
	SIF CFLAG:COUNT:1 < 2
		CONTINUE
	SIF CFLAG:COUNT:12 > 0
		CONTINUE
	C = COUNT
	CALL CHECK_CHILD_CARE
	SIF RESULT == 1
		CONTINUE
	SIF CFLAG:COUNT:1 == 2 && (CFLAG:COUNT:151 & 16) && BASE:COUNT:체력 > 500
		PRINTFORML [{COUNT}] - %CALLNAME:COUNT%
REND
PRINTL [ 999] - 옵션을 선택한다
PRINTL [1000] - 돌아온다
$INPUT_LOOP
INPUT
IF RESULT == 1000
	RETURN 0
ELSEIF RESULT == 999
	CALL OPTION_WORK
	CALL RENTAL_SLAVE
ELSEIF RESULT < 0 || RESULT >= CHARANUM
	GOTO INPUT_LOOP
ELSEIF RESULT == MASTER
	GOTO INPUT_LOOP
ELSEIF CFLAG:RESULT:1 < 2
	GOTO INPUT_LOOP
ELSEIF CFLAG:RESULT:12 > 0
	GOTO INPUT_LOOP
ELSEIF ((CFLAG:RESULT:151 & 16) == 0) || BASE:RESULT:체력 <= 500
	GOTO INPUT_LOOP
ELSE
	A = RESULT
ENDIF


SIF A == 0
	RETURN 0

