;====================================================================
;플레이 내용의 판정
;====================================================================
@RENTAL_PLAY_CHECK

;-----------------------
;노예의수(LOCAL:1)공(LOCAL:2) 보존
;-----------------------
IF TALENT:A:공
	LOCAL:2 = 1
ELSEIF TALENT:A:수 || TALENT:A:오메가
	LOCAL:1 = 1
ELSE
	IF !RAND:5
		LOCAL:2 = 1
	ELSE
		LOCAL:1 = 1
	ENDIF
ENDIF

;-----------------------
;손님의 도S(LOCAL:3), S(LOCAL:4), 보통(LOCAL:5), M(LOCAL:6)
;-----------------------
;노예가공
IF LOCAL:2
	;M의 손님이 증가한다
	IF RAND:10 < ABL:A:새드끼
		LOCAL:6 = 1
	;(이)가 아니면 보통 손님
	ELSE
		LOCAL:5 = 1
	ENDIF
;노예가수
ELSE
	K:1 = RAND:10
	;도S
	IF K:1 < 2
		LOCAL:3 = 1
	;S의 손님이 증가한다
	ELSEIF K:1 < ABL:A:마조끼
		LOCAL:4 = 1
	;(이)가 아니면 보통 손님
	ELSE
		LOCAL:5 = 1
	ENDIF
ENDIF

;-----------------------
;메인이 되는 플레이의 판정
;-----------------------
;수간(노예수만)
IF RAND:12 == 0 && (FLAG:160 & 32) && LOCAL:1
	CFLAG:A:152 |= 1
;스카톨로지(노예수만)
ELSEIF RAND:11 == 0 && (FLAG:160 & 960) && LOCAL:1
	CFLAG:A:152 |= 2
;하도SM
ELSEIF RAND:10 == 0 && (FLAG:160 & 8) && (LOCAL:4 || LOCAL:6)
	;손님이 도S
	IF LOCAL:4
		CFLAG:A:152 |= 8
	;손님이 M
	ELSE
		CFLAG:A:152 |= 4
	ENDIF
;복수 플레이
ELSEIF RAND:9 == 0 && (FLAG:160 & 16)
	;노예공
	IF LOCAL:2
		CFLAG:A:152 |= 64
	;노예수
	ELSE
		CFLAG:A:152 |= 128
	ENDIF
;소프트 SM
ELSEIF RAND:8 == 0 && (LOCAL:3 || LOCAL:6)
	;손님이 S
	IF LOCAL:3
		CFLAG:A:152 |= 32
	;손님이 M
	ELSE
		CFLAG:A:152 |= 16
	ENDIF
;페티시즘. 삽입 없음
ELSEIF RAND:7 == 0
	CFLAG:A:152 |= 256
;수치계
ELSEIF RAND:6 == 0
	CFLAG:A:152 |= 512
;쾌락 고문
ELSEIF RAND:5 == 0
	;랜덤으로 초조하게 해 고문에
	IF TEQUIP:A:정조대 || !RAND:3
		CFLAG:A:152 |= 4096
	ELSE
		CFLAG:A:152 |= 1024
	ENDIF
;아무것도 없었다(러브러브 포함한다)
ELSE
	CFLAG:A:152 = 0
ENDIF


;-----------------------
;실제로 행해진 플레이의 상세
;-----------------------
;일수에 의해 시행 회수가 다르다
LOCAL = B*2
X = 0

