MONEY += S

;휴게 플래그를 세운다
FLAG:0 = 1
BEGIN TURNEND

RETURN 1



@TOILET_PLAY_CHECK
;--------------------------------------------------------
;인원수와 플레이 내용의 계산
;최대 인원수 30명
IF B == 1
	LOCAL = RAND:20+1
ELSEIF B == 2
	LOCAL = RAND:15+1
ELSE
	LOCAL = RAND:10+1
ENDIF
LOCAL = 10+LOCAL
X = 0

;성적 봉사	CFLAG:A:160
;복수인		CFLAG:A:161
;삽입		CFLAG:A:162
;처녀		CFLAG:A:163
;AF		CFLAG:A:164
;A처녀		CFLAG:A:165
;음뇨		CFLAG:A:166
;체내 방뇨	CFLAG:A:167

CFLAG:A:160 = 0
CFLAG:A:161 = 0
CFLAG:A:162 = 0
CFLAG:A:163 = 0
CFLAG:A:164 = 0
CFLAG:A:165 = 0
CFLAG:A:166 = 0
CFLAG:A:167 = 0
;--------------------------------------------------------
REPEAT 30
	;최대 인원수 넘으면 빠진다
	IF X == LOCAL
		BREAK
	;도 없으면 손님을 선출
	ELSE
		;-----------------------
		;인원수를 늘린다
		;-----------------------
		X += 1
		
		C = 10-ABL:A:스캇중독
		;음뇨
		LOCAL:1 = 0
		;체내 방뇨
		LOCAL:2 = 0
		;방뇨 목적인가 어떤가 판정
		IF (FLAG:160 & 128 || FLAG:160 & 768) && RAND:C == 0
			;먹이려고 한다
			IF FLAG:160 & 128 && (FLAG:160 & 768) == 0
				;먹인다
				LOCAL:1 = 1
			;체내 방뇨하려고 한다
			ELSEIF FLAG:160 & 768 && (FLAG:160 & 128) == 0
				;A실전 불가(노예처녀로 옵션이 없는, 혹은 노예가공)
				IF (TALENT:A:백버진 && (FLAG:160 & 2) == 0) || TALENT:A:공
					;먹인다
					LOCAL:1 = 1
				ELSE
					;체내 방뇨
					LOCAL:2 = 1
				ENDIF
			ELSE
				;A실전 불가(노예처녀로 옵션이 없는, 혹은 랜덤 1/3, 혹은 노예가공)
				IF (TALENT:A:백버진 && (FLAG:160 & 2) == 0) || RAND:2 == 0 || TALENT:A:공
					;먹인다
					LOCAL:1 = 1
				ELSE
					;체내 방뇨
					LOCAL:2 = 1
				ENDIF
			ENDIF
		ENDIF
		
		;체내 방뇨 목적의 손님
		IF LOCAL:2
			CFLAG:A:167 += 1
			;A섹스 회수도 증가한다
			CFLAG:A:164 += 1
			SIF TALENT:A:백버진 && (FLAG:160 & 2)
				CFLAG:A:165 += 1
		;먹이는 목적의 손님
		ELSEIF LOCAL:1
			CFLAG:A:166 += 1
		ENDIF
		
		;1/5의 확률로, 이후 아무것도 하지 않고 돌아간다
		SIF (LOCAL:2 || LOCAL:1) && RAND:4 == 0
			CONTINUE
		
		;봉사
		LOCAL:3 = 0
		;V실전
		LOCAL:4 = 0
		;A실전
		LOCAL:5 = 0
		;실전 목적인가 어떤가 판정
		;노예가공라면 삽입 할 수 없다
		IF TALENT:A:공
			LOCAL:3 = 1
		;V가 없으면 A 밖에 노릴 수 없다
		ELSEIF !VAGINA(A)
			;노예 A처녀로 옵션이 없으면 A도 노릴 수 없다
			;1/5의 확률로 A에 흥미를 나타내지 않는다
			IF (TALENT:A:백버진 && (FLAG:160 & 2) == 0) || RAND:4 == 0
				LOCAL:3 = 1
			ELSE
				LOCAL:5 = 1
			ENDIF
		;V를 노린다
		ELSE
			;노예처녀로 옵션이 없으면 실전 할 수 없다
			;1/5의 확률로 V에 흥미를 나타내지 않는다
			IF ((TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1) == 0) || RAND:4 == 0
				LOCAL:3 = 1
			ELSE
				LOCAL:4 = 1
			ENDIF
		ENDIF
		
		;아무것도 시키지 않은 손님이 있었을 경우, 봉사 플래그를 세운다
		SIF !LOCAL:1 && !LOCAL:2 && !LOCAL:3 && !LOCAL:4 && !LOCAL:5
			LOCAL:3 = 1
		
		;V실전
		IF LOCAL:4
			CFLAG:A:162 += 1
			SIF (TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1)
				CFLAG:A:163 += 1
		ENDIF
		
		;A실전
		IF LOCAL:5
			CFLAG:A:164 += 1
			SIF TALENT:A:백버진 && (FLAG:160 & 2)
				CFLAG:A:165 += 1
		ENDIF
		
		;봉사
		IF LOCAL:3
			CFLAG:A:160 += 1
		ENDIF
	ENDIF
REND

;인원수 확정후, 수플레이 판정을 붙인다
CFLAG:A:161 += (X/10)+1



