IF TEQUIP:비디오카메라
	CALL PRICE_VIDEO
ENDIF

;녹화하고 있지 않으면 종료
SIF TFLAG:36 == 0
	RETURN 0


PRINTFORML 녹화한 비디오는{TFLAG:36}고슈금정도의 값이 붙을 것 같다
PRINTL 녹화한 비디오를 어떻게 하든가 선택해 주세요
PRINTL [0] - 판다
PRINTL [1] - 손바탕으로 남긴다
DRAWLINE

$INPUT_LOOP
INPUT
IF RESULT == 0
	PRINTFORML 조교시에 녹화한 비디오를 팔아,{TFLAG:36}고슈금을 손에 넣었습니다
	MONEY += TFLAG:36
	TFLAG:36 = 0
	;이벤트대사 호출(비디오 매각시)
	TFLAG:13 = 19
	CALL KOJO_MESSAGE_EVENT
ELSEIF RESULT == 1
	PRINTFORML 조교시에 녹화한 비디오를손바탕으로 남겨 두었다
	CFLAG:20 += 1
	;이벤트대사 호출(비디오 보유시) 23은 아침밥으로 사용
	TFLAG:13 = 24
	CALL KOJO_MESSAGE_EVENT
ELSE
	GOTO INPUT_LOOP
ENDIF
DRAWLINE



;-------------------------------------------------
@PRICE_PICTURE
;-------------------------------------------------
A = 100
B = 0
C = 0

;조교자의 ABL:촬영기능를 본다
IF ABL:PLAYER:13 == 0
	B = 10
ELSEIF ABL:PLAYER:촬영기능 == 1
	B = 100
ELSEIF ABL:PLAYER:촬영기능 == 2
	B = 150
ELSEIF ABL:PLAYER:촬영기능 == 3
	B = 250
ELSEIF ABL:PLAYER:촬영기능 == 4
	B = 300
ELSE
	B = 500
ENDIF

;입계
;볼개그장착, 촉수구욕
IF TEQUIP:볼개그 || TEQUIP:촉수구욕
	A += 20
	C += 1
ENDIF
;A계
;로터A삽입, 애널바이브장착, 애널비즈장착, 벌룬 장착
IF TEQUIP:로터A삽입 || TEQUIP:애널바이브 || TEQUIP:애널비즈 || TEQUIP:확장벌룬
	A += 10
	C += 1
ENDIF
;C계
;클리캡장착, 전동오나홀장착, 음핵클립
IF TEQUIP:클리캡 || TEQUIP:전동오나홀 || TEQUIP:음핵클립
	A += 10
	C += 1
ENDIF
;B계
;니플캡장착, 착유기, 유두클립
IF TEQUIP:니플캡 || TEQUIP:착유기 || TEQUIP:유두클립
	A += 15
	C += 1
ENDIF
;V계
;바이브장착, 로터삽입
IF TEQUIP:바이브 || TEQUIP:로터삽입
	A += 15
	C += 1
ENDIF
;긴박계
;아이마스크장착
IF TEQUIP:아이마스크
	A += 20
	C += 1
ENDIF
;밧줄로 긴박
IF TEQUIP:밧줄
	A += 25
	C += 1
ENDIF
;약제계
;미약, 이뇨제
IF TEQUIP:미약 || TEQUIP:이뇨제
	A += 5
	C += 1
ENDIF
;장소계
;야외플레이, 수치플레이중, 목욕탕플레이
IF TEQUIP:야외플레이 || TEQUIP:수치플레이 || TEQUIP:목욕탕
	A += 20
	C += 1
ENDIF
;상황계
;샤워, 비디오촬영
IF TEQUIP:샤워 || TEQUIP:비디오카메라
	A += 10
	C += 1
ENDIF
;촉수조교
IF TEQUIP:촉수
	A += 5
	C += 1
ENDIF
;착의계외
;신혼부부플레이중
IF TEQUIP:에이프런
	A += 10
	C += 1
ENDIF
;아무것도 장착하고 있지 않는 경우의 보너스
SIF C == 0
	A += 50

;정액의 보정을 설정하기 위해(때문에) C를 리셋트
C = 0
;정액의 더러워지고(입, 손, 페니스, 바기나, 애널의 순서)
SIF STAIN:PLAYER:입 & 4
	C += 1
SIF STAIN:PLAYER:손 & 4
	C += 1
SIF STAIN:PLAYER:페니스 & 4
	C += 1
SIF STAIN:PLAYER:바기나 & 4
	C += 1
SIF STAIN:PLAYER:애널 & 4
	C += 1
;정액의 더러움이 없는 경우의 보너스
SIF C == 0
	A += 30


;소질:무관심를 본다
SIF TALENT:무관심
	A -= 30
;소질:감정부족를 본다
SIF TALENT:감정부족
	A -= 30
;소질:부끄럼쟁이를 본다
SIF TALENT:부끄럼쟁이
	A += 10
;소질:작은 몸집 체계를 본다
SIF TALENT:작은체구
	A += 20
;소질:절벽를 본다
SIF TALENT:절벽
	A -= 10
;소질:거유를 본다
SIF TALENT:거유
	A += 20
;소질:폭유를 본다
SIF TALENT:폭유
	A += 10
;소질:함몰유두를 본다
SIF TALENT:함몰유두
	A += 10
;소질:동물귀를 본다
SIF TALENT:동물귀
	A += 10
;소질:인기를 본다
SIF TALENT:인기
	A += 20
;소질:수수께끼의매력를 본다
SIF TALENT:수수께끼의매력
	A += 30
;소질:매력를 본다
SIF TALENT:매력
	A += 20
;소질:발정기를 본다
SIF TALENT:발정기
	A += 10

SIF A < 0
	A = 0

B *= A
B /= 100

;난이도 처리
;EASY
IF FLAG:4 == 1
	TIMES B , 1.20
;NORMAL
ELSEIF FLAG:4 == 2
	TIMES B , 1.00
;HARD
ELSEIF FLAG:4 == 3
	TIMES B , 0.80
ENDIF

TFLAG:37 += B


;-------------------------------------------------
@SELL_PICTURE
;-------------------------------------------------
SIF TFLAG:73 == 0
	RETURN 0
SIF TFLAG:37 <= 0
	RETURN 0

PRINTFORMW 이번 찍은{TFLAG:73}매의 사진에는{TFLAG:37}의 매입가가 붙었다
MONEY += TFLAG:37

;플래그 리셋트. 이것으로 조교 메뉴 이외에도 재이용할 수 있도록(듯이)
TFLAG:37 = 0
TFLAG:73 = 0


;-------------------------------------------------
@SELL_MILK
;-------------------------------------------------
O = TFLAG:35 * 100
IF O > 0
	IF EXP:분유경험 <= EXPLV:0
		O *= 1
	ELSEIF EXP:분유경험 <= EXPLV:1
		O *= 2
	ELSEIF EXP:분유경험 <= EXPLV:2
		O *= 3
	ELSEIF EXP:분유경험 <= EXPLV:3
		O *= 4
	ELSEIF EXP:분유경험 <= EXPLV:4
		O *= 5
	ELSE
		O *= 6
	ENDIF

	;난이도 처리
	;EASY
	IF FLAG:4 == 1
		TIMES O , 1.20
	;NORMAL
	ELSEIF FLAG:4 == 2
		TIMES O , 1.00
	;HARD
	ELSEIF FLAG:4 == 3
		TIMES O , 0.80
	ENDIF

	PRINTFORML 착유기를 사용해%CALLNAME%로부터모유를 짜냈다
	PRINTFORMW %CALLNAME%의모유에는{O}의 매입가가 붙었다
	MONEY += O
	PRINTFORMW 소지 코슈금 {O}증가
ENDIF

