PRINTL [100]- 돌아온다
$INPUT_LOOP
INPUT
IF RESULT == 100
	RETURN 0
ELSEIF RESULT == 0
	CALL CHARA_SELL
ELSEIF RESULT == 1 && A && ITEM:92
	CALL LUNCH_STALL
ELSEIF RESULT == 2 && E
	CALL CONCERT
ELSEIF RESULT == 3 && B
	CALL PROSTITUTION
ELSEIF RESULT == 4 && F
	CALL PROMISCUOUS_SEX_LIVE
ELSEIF RESULT == 5 && D && TIME == 0
	CALL RENTAL_SLAVE
ELSEIF RESULT == 6 && G && ITEM:94
	CALL PUBLIC_TOILET
ELSE
	GOTO INPUT_LOOP
ENDIF
D = 0
CALL DAILY_LIFE_MASTER_CHANGE

RETURN 0


