	CASE 2 TO 8,226,227,228,270,271,285,286,300 TO 304,310 TO 313,315,316,320,321,323,411,440,441
		RETURNF 7
	;봉사계·기학계
	CASE 70,71,73 TO 75,77,80,82,85 TO 87,150,152,184,185,220 TO 225,280 TO 284,317,420,421,516
		RETURNF 8
	;커뮤니케이션계
	CASE 200 TO 207,319
		RETURNF 9
	CASEELSE
		RETURNF 0
ENDSELECT

