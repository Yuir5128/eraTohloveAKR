;비  고:식 나카노세키수
;-------------------------------------------------
@BASE46(ARG)
#FUNCTION
SELECTCASE ARG
	;어느 쪽인가
	CASE 0
		RETURNF BASE:뇨의 >= 60 || BASE:변의 >= 60
	;뇨의만
	CASE 1
		RETURNF BASE:뇨의 >= 60
	;변의만
	CASE 2
		RETURNF BASE:변의 >= 60
	;양쪽 모두
	CASE 3
		RETURNF BASE:뇨의 >= 60 && BASE:변의 >= 60
ENDSELECT
RETURNF 0

