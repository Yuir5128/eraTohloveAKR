				PRINTFORMW 
			ENDIF
			CFLAG:238 = 4
		;굴복각인Lv3
		ELSEIF MARK:2 == 3 && (CFLAG:238 < 3 || FLAG:7 == 2)
			IF ASSI > 0 && ASSIPLAY
				PRINTFORMW 「후아…… 아,열… 좋다……%UNICODE(0x2661)%」
			ELSEIF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
				PRINTFORMW 「아%UNICODE(0x2661)% 대장의 자식…… 쿨렁쿨렁 말해들……」
			ELSE
				PRINTFORMW 「아,응읏!안쪽,뜨겁다……」
			ENDIF
			CFLAG:238 = 3
		;그 이외
		ELSEIF CFLAG:238 < 2 || FLAG:7 == 2
			PRINTFORMW 
		CFLAG:238 = 2
		ENDIF
	ENDIF
