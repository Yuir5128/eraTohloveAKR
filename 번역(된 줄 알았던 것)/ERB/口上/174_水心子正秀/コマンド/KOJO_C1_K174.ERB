;=================================================
;기본계 커맨드 FIRSTTIME(100~109)
;=================================================
;-------------------------------------------------
;COM0,애무 FIRSTTIME(100)
;-------------------------------------------------
@KOJO_COM_K174_0
;-------------------------------------------------
;기입 체크(=0,비표시,1,표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;지문 호출(칼라 변경 대응)
	CALL TRAIN_MESSAGE_COLOR
	
	;첫회
	IF FIRSTTIME(100)
		PRINTFORML 「에? 에,? 무엇을 하고 있지……?」
		PRINTFORML %CALLNAME%는 의아스러울 것 같은 얼굴로 %CALLNAME:PLAYER%를 보고 있다
	ELSE
		;미함락 반발
		IF !KANRAKU(TARGET) || (CFLAG:5005 & 32)
			SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「응…,… 읏…… 앗…… 응응,야… 째,읏…」
				CASE 0
					CALL PANT_VOICE_EX
			ENDSELECT
			RETURN LOCAL
		ENDIF
		
		PRINTFORML %CALLNAME:PLAYER%가 접할 때,%CALLNAME%는 소리를 누설하고 있다
		SELECTCASE RAND:2
			CASE 1
				PRINTFORML 「하,… 읏…… 앗…… 아,거기…,읏…」
			CASE 0
				CALL PANT_VOICE_EX
		ENDSELECT
		
	ENDIF
ENDIF
RETURN LOCAL

