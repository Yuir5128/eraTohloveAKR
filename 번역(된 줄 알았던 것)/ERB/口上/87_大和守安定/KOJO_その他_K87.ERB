;@KOJO_MESSAGE_PALAMCNG 관계(1을 캐릭터 번호에 치환)
;파라미터 변화 따위를 트리거로 한입상 CFLAG 221~260을 사용
;==================================================
;-------------------------------------------------
;@KOJO_MESSAGE_PALAMCNG_A(X를 캐릭터 번호에 치환)
;조교자의사정를 트리거로 한입상
;-------------------------------------------------
@KOJO_MESSAGE_PALAMCNG_A_K87
;조수가 조교했을 때에입상을 스킵 한다(기호에 응해 사용하는,줄머리의 것;을 지우면 스킵 하게 된다)
;SIF ASSI >= 0 && ASSIPLAY
;	RETURN 0 
;볼개그착용시에는 지문을 표시해입상을 스킵 한다
;IF TEQUIP:볼개그 && SELECTCOM != 107
;	CALL PALAM_MESSAGE_A
;	RETURN 0
;ENDIF
;--------------------------------------------------
;특정조수,주인시에 특수조수입상을 호출한다(Y1를 특정조수,Z를 주인의 캐릭터 번호에 치환)
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL KOJO_MESSAGE_PALAMCNG_A_PY1_KZ
;	RETURN 0
;ENDIF
;IF ASSI >= 0 && ASSIPLAY
;	IF NO:ASSI == Y1
;		CALL KOJO_MESSAGE_PALAMCNG_A_PY1_KZ
;		RETURN 0
;	ENDIF
;ENDIF
;=================================================
;조교자의사정시의 텍스트
;=================================================
;-------------------------------------------------
;사정개소 변경시의 텍스트 유효시는 여기서 함수 종료 CFLAG:221~230
;-------------------------------------------------
IF CFLAG:106
	CALL PALAM_MESSAGE_A
	;머리카락
	IF CFLAG:106 == 1
		CFLAG:221 = 1
	;안
	ELSEIF CFLAG:106 == 2
		CFLAG:222 = 1
	;가슴
	ELSEIF CFLAG:106 == 3
		CFLAG:223 = 1
	;복
	ELSEIF CFLAG:106 == 4
		CFLAG:224 = 1
	;액
	ELSEIF CFLAG:106 == 5
		CFLAG:225 = 1
	;허벅지
	ELSEIF CFLAG:106 == 6
		CFLAG:226 = 1
	;균열
	ELSEIF CFLAG:106 == 7
		CFLAG:227 = 1
	;고
	ELSEIF CFLAG:106 == 8
		CFLAG:228 = 1
	;고무
	ELSEIF CFLAG:106 == 9
		;섹스로사정 한편,애널섹스 이외를 실행시
		IF TFLAG:2 > 0 && SELECTCOM != 110 && SELECTCOM != 111 && SELECTCOM != 112 && SELECTCOM != 113 && SELECTCOM != 114 && SELECTCOM != 115 && SELECTCOM != 116
			CFLAG:229 = 1
		ELSE
			CFLAG:230 = 1
		ENDIF
	ENDIF
	RETURN 0
ENDIF

;-------------------------------------------------
;손,입 이외로사정 CFLAG:231
;TFLAG:9 == 1은사정,2는 대량사정
;대응 커맨드는 키스,스마타,풋잡,애널강제 봉사,더블스마타
;-------------------------------------------------
IF TFLAG:9 == 1 || TFLAG:9 == 2
	CALL PALAM_MESSAGE_A
	CFLAG:231 = 1
ENDIF

;-------------------------------------------------
;조수를범한다로사정 CFLAG:232
;-------------------------------------------------
IF TFLAG:7 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;입로사정 CFLAG:233
;-------------------------------------------------
IF TFLAG:0 > 0
	CALL PALAM_MESSAGE_A
	;함락이 끝난+정액중독 혹은봉사정신
	IF (ABL:봉사정신 >= 4 || ABL:정액중독 >= 2) && KANRAKU(TARGET) && (CFLAG:233 < 3 || FLAG:7 == 2)
		CFLAG:233 = 3
	;함락이 끝난 상태
	ELSEIF KANRAKU(TARGET) && (CFLAG:233 < 2 || FLAG:7 == 2)
		CFLAG:233 = 2
	;그 이외
	ELSEIF CFLAG:233 < 1 || FLAG:7 == 2
		CFLAG:233 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;손로사정 CFLAG:234
;-------------------------------------------------
IF TFLAG:1 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:234 = 1
ENDIF

;-------------------------------------------------
;가슴로사정 CFLAG:235
;-------------------------------------------------
IF TFLAG:34 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:235 = 1
ENDIF

;-------------------------------------------------
;발로사정 CFLAG:236
;-------------------------------------------------
IF TFLAG:16 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:236 = 1
ENDIF

;-------------------------------------------------
;섹스로사정 CFLAG:237 CFLAG:238
;-------------------------------------------------
IF TFLAG:2 > 0
	CALL PALAM_MESSAGE_A
	;애널섹스계
	IF SELECTCOM == 110 || SELECTCOM == 111 || SELECTCOM == 112 || SELECTCOM == 113 || SELECTCOM == 114 || SELECTCOM == 115 || SELECTCOM == 116
		CFLAG:237 = 1
	;섹스계
	ELSE
		CFLAG:238 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;역섹스로사정 시킨 CFLAG:239
;-------------------------------------------------
IF (SELECTCOM == 260 || SELECTCOM == 261 || SELECTCOM == 262 || SELECTCOM == 263 || SELECTCOM == 264 || SELECTCOM == 265) && TFLAG:10 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:239 = 1
ENDIF

;-------------------------------------------------
;조교자 하늘사정 CFLAG:239
;-------------------------------------------------
IF TFLAG:164 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:239 = 1
ENDIF



