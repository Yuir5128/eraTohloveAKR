	IF FLAG:160 & 512
		FLAG:160 -= 512
		FLAG:160 |= 256
	ELSEIF FLAG:160 & 256
		FLAG:160 -= 256
	ELSEIF (FLAG:160 & 768) == 0
		FLAG:160 |= 512
	ENDIF
;모두 유효하게 한다
ELSEIF RESULT == 50
	FLAG:160 |= 703
;모두 무효로 한다
ELSEIF RESULT == 51
	FLAG:160 = 0
ELSE
