			TALENT:단소 = 1
		;단소→아이사이즈
		ELSE
			;[단소]삭제[아이사이즈]추가
			TALENT:아이사이즈 = 1
			TALENT:단소 = 0
		ENDIF
	;되감기약
	CASE 81
		;CSV의 설정으로부터 호출하므로,입상에서 선천적으로 여체화,미경험 따위의 설정을 붙일 때는
		;이벤트입상:되감아 약으로 소질 변경을 가르치는 것
		
		;체격은 되돌리지 않고,CSV가소인체형가 아니고 현소인체형 때,경험·능력·처녀계 소질도 백지로 한다
		
		;원래는소인체형가 아니다
		SIF !CSVTALENT(NO:TARGET,142,CFLAG:0)
			LOCAL:1 = 1
		;개체차이로소인체형를 얻지 않았다
		SIF DIFF_GET_TALENT(TARGET ,4) != 1
			LOCAL:1 = 1
		;되감아 경험
		CFLAG:52++
		;스트레스 30 추가
		CFLAG:10 += 30
		;체력의 초기화
		MAXBASE:체력 = CSVBASE(NO:TARGET,0,CFLAG:0)
		SIF BASE:체력 > MAXBASE:체력
			BASE:체력 = MAXBASE:체력
		;경험의 초기화
		FOR LOCAL,0,70
			EXP:LOCAL = CSVEXP(NO:TARGET,LOCAL,CFLAG:0)
			SIF LOCAL:1
				EXP:LOCAL = 0
		NEXT
		;능력의 일부 초기화
		FOR LOCAL,0,23
			SIF LOCAL < 2 || (LOCAL > 6 && LOCAL < 12) || LOCAL > 14
				CONTINUE
			ABL:LOCAL = CSVABL(NO:TARGET,LOCAL,CFLAG:0)
			SIF LOCAL:1
				ABL:LOCAL = 0
		NEXT
		;소질의 일부 초기화
		FOR LOCAL,0,205
			SELECTCASE LOCAL
				;낙인결손은 지운다
				CASE 159,200 TO 205
					TALENT:LOCAL = 0
				;변화하고 있을지도 모르는 소질을 원래대로 되돌린다
				;요리치,독약내성,실금버릇,악취민감
				;신체 특징 소질,성별,동물귀,모유체질,절륜,낙인결손
				CASE 1,44 TO 47,56,57,62,100 TO 140,142,143,149,163,172,173
					;아이에는 효과 없음
					SIF NO:TARGET >= 890 && NO:TARGET <= 899
						CONTINUE
					TALENT:LOCAL = CSVTALENT(NO:TARGET,LOCAL,CFLAG:0)
				;처녀계 소질
				CASE 0,160,161
					TALENT:LOCAL = CSVTALENT(NO:TARGET,LOCAL,CFLAG:0)
					SIF LOCAL:1
						TALENT:LOCAL = 1
			ENDSELECT
		NEXT
		;개체차이 처리
		CALL DIFF_GET_BUY(TARGET)
		;고통/쾌락각인의 초기화
		MARK:고통각인 = 0
		MARK:쾌락각인 = 0
		;경험계 플래그를 초기화
		CFLAG:23 = 0
		CFLAG:24 = 0
		CFLAG:25 = 0
		CFLAG:43 = 0
		CFLAG:44 = 0
		CFLAG:45 = 0
		CFLAG:46 = 0
		CFLAG:47 = 0
		CFLAG:49 = 0
		CFLAG:50 = 0
		;임신패치 주위의 플래그를 리셋트
		SIF !CFLAG:110
			CALL CLEAR_FLAG
	CASE 96
		CFLAG:693 += 1
	CASE 97
		CFLAG:694 += 1
ENDSELECT
;아이템에 의한붕괴의 처리
SIF CFLAG:10 >= 100 && !TALENT:붕괴
	CALL COLLAPSE_MIND


