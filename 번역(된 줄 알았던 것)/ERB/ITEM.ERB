	;웨딩세트
	CASE 39
		;상사상애의 커플이 없다
		SIF !LOCAL:5
			ITEMSALES:ARG = 0
	;수술도구세트
	CASE 90
		;엽기 커맨드 확장 오프
		SIF (FLAG:2 & 32) == 0
			ITEMSALES:ARG = 0
	;비디오테이프
	CASE 43
		;비디오카메라가 없다
		SIF !ITEM:18
