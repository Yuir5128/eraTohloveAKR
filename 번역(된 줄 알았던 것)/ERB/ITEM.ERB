;--------------------------------------------------
;아이템 구입
;--------------------------------------------------
@EVENTBUY
;이 가게의 상품은 아닌,판매 조건을 채우지 않았다
IF !SHOW_ITEM(TFLAG:1,BOUGHT) || !ITEMSALES:BOUGHT
	CLEARLINE 1
	RETURN 0
ENDIF

;--------------------------------------------------
;구입 처리
;--------------------------------------------------
;아이템 설명 표시
MONEY += ITEMPRICE:BOUGHT
ITEM:BOUGHT--
CALL ITEM_LIST
SELECTCASE BOUGHT
	;【더러움무시】
	CASE 50
		PRINTFORML %ITEMNAME:BOUGHT%
		TRYCALL DICTIONARY_TALENT_64
	;【조제지식】
	CASE 51
		PRINTFORML %ITEMNAME:BOUGHT%
		TRYCALL DICTIONARY_TALENT_55
	;【금단의지식】
	CASE 52
		PRINTFORML %ITEMNAME:BOUGHT%
		TRYCALL DICTIONARY_TALENT_144
	;【천리안】
	CASE 53
		PRINTFORML %ITEMNAME:BOUGHT%
		TRYCALL DICTIONARY_TALENT_156
	;【절륜】
	CASE 54
		PRINTFORML %ITEMNAME:BOUGHT%
		TRYCALL DICTIONARY_TALENT_47
	;【수】
	CASE 55
		PRINTFORML %ITEMNAME:BOUGHT%
		TRYCALL DICTIONARY_TALENT_157
	;【공】
	CASE 56
		PRINTFORML %ITEMNAME:BOUGHT%
		TRYCALL DICTIONARY_TALENT_158
	;【오메가】
	CASE 59
		PRINTFORML %ITEMNAME:BOUGHT%
		TRYCALL DICTIONARY_TALENT_170
	CASEELSE
		PRINTFORML 【%ITEMNAME:BOUGHT%】
		TRYCALLFORM DICTIONARY_ITEM_{BOUGHT}
		PRINTL 
ENDSELECT
SETCOLOR 140,140,140
DRAWLINE
RESETCOLOR
SELECTCASE BOUGHT
	;기본 아이템
	;0~26 어덜트 상품,30정조대,31~33동물귀,34헤드셋,38에이프런,39 웨딩 세트
	;57촉수사역술,58진화의비법,90손 방법 도구,91우리,92~93포장마차(증축),94플랜카드
	CASE 0 TO 26,30 TO 34,38,39,57,58,90 TO 94,96 TO 94
		PRINTFORML 구입합니까?
		SETCOLOR 140,140,140
		DRAWLINE
		RESETCOLOR
		PRINTBUTTON "[999 캔슬]",999
		PRINT 　
		PRINTBUTTON "[0구입한다]",0
		PRINTL 
		$INPUT_LOOP_2
		INPUT
		SIF RESULT == 999
			RETURN 0
		IF RESULT != 0
			CLEARLINE 1
			GOTO INPUT_LOOP_2
		ELSEIF MONEY < ITEMPRICE:BOUGHT
			CLEARLINE 1
			PRINTW 돈이 충분하지 않습니다
			GOTO INPUT_LOOP_2
		ENDIF
		IF BOUGHT == 57 || BOUGHT == 58
			PRINTFORMW ＜%ITEMNAME:BOUGHT%를 몸에 익혔습니다＞
		ELSE
			PRINTFORMW ＜%ITEMNAME:BOUGHT%를 구입했습니다＞
		ENDIF
		ITEMSALES:BOUGHT = 0
		ITEM:BOUGHT++
		MONEY -= ITEMPRICE:BOUGHT
		TFLAG:13 = 28
		CALL KOJO_MESSAGE_EVENT
	;소비 아이템
	;36,37스타킹,타이츠,40~49 약품계,86~88 주류
	CASE 36,37,40 TO 49,86 TO 88
		IF (100 - ITEM:BOUGHT) <= (MONEY / ITEMPRICE:BOUGHT)
			LOCAL:3 = 100 - ITEM:BOUGHT
		ELSE
			LOCAL:3 = MONEY / ITEMPRICE:BOUGHT
			LOCAL:4 = 1
		ENDIF
		SETCOLOR 140,140,140
		DRAWLINE
		RESETCOLOR
		PRINTFORML %ITEMNAME:BOUGHT%를 몇개 구입합니까? (1-{LOCAL:3}개까지)
		PRINTFORML 숫자를 입력할까 이하의 버튼을 클릭 해 주세요
		PRINTL 
		PRINTBUTTON "[999 캔슬]",999
		PRINT 　
		PRINTBUTTON "[1개]",1
		IF LOCAL:3 >= 5
			PRINT 　
			PRINTBUTTON "[5개]",5
		ENDIF
		IF LOCAL:3 >= 10
			PRINT 　
			PRINTBUTTON "[10개]",10
		ENDIF
		PRINT 　
		IF LOCAL:4
			PRINTBUTTON "[살 수 있을 뿐]",LOCAL:3
		ELSE
			PRINTBUTTON "[가질 수 있을 뿐]",LOCAL:3
		ENDIF
		PRINTL 
		$INPUT_LOOP_3
		INPUT
		SIF RESULT == 999
			RETURN 0
		IF RESULT < 0
			CLEARLINE 1
			GOTO INPUT_LOOP_3
		ELSEIF RESULT > LOCAL:3
			CLEARLINE 1
			IF RESULT > LOCAL:3 && !LOCAL:4
				PRINTW 그렇게 가질 수 없습니다
			ELSEIF RESULT > LOCAL:3 && LOCAL:4
				PRINTW 돈이발선
			ENDIF
			GOTO INPUT_LOOP_3
		ENDIF
		PRINTFORMW ＜%ITEMNAME:BOUGHT%를{RESULT}개 구입했습니다＞
		ITEM:BOUGHT += RESULT
		MONEY -= ITEMPRICE:BOUGHT * RESULT
		TFLAG:13 = 28
		CALL KOJO_MESSAGE_EVENT
	;사용 아이템
	;50더러움무시,51조제지식,52금지된지식,53천리안,54절륜,55수,56공,59오메가
	;60~85 체질 변화계의 약,96부적,97부적·극
	CASE 50 TO 56,59 TO 85,96,97
		IF BOUGHT < 60
			PRINTFORML 누가 몸에 익힙니까?
		ELSEIF BOUGHT > 95
			PRINTFORML 누구에게 갖게합니까?
		ELSE
			PRINTFORML 누구에게 사용합니까?
		ENDIF
		SETCOLOR 140,140,140
		DRAWLINE
		RESETCOLOR
		FOR COUNT,0,CHARANUM
			;아이템 사용 가능
			SIF !ITEM_USE_CHECK(COUNT,BOUGHT)
				CONTINUE
			SETCOLOR 0x8888FF
			SIF (BOUGHT:96 || BOUGHT:97) && COUNT == MASTER
				CONTINUE
			IF COUNT == MASTER
				PRINTFORM %UNICODE(0x27A0)%
			ELSEIF COUNT == ASSI
				PRINT 조
			ELSEIF CFLAG:COUNT:1 == 3
				PRINT 모두
			ELSEIF COUNT == TARGET
				PRINT ☆
			ELSE
				PRINT 　
			ENDIF
			RESETCOLOR
			PRINTFORM [{COUNT,2}] %CALLNAME:COUNT,16,LEFT%
			PRINTFORM %UNICODE(0x208D)%
			CALL PRINT_COLORBAR,BASE:COUNT:체력,MAXBASE:COUNT:체력,8,UNICODE(0x2583),UNICODE(0x2583),BARCOLORSET("빨강"),RESULT:1,327685
			PRINTFORM %UNICODE(0x208E)%
			PRINTFORM %UNICODE(0x208D)%
			CALL PRINT_COLORBAR,BASE:COUNT:기력,MAXBASE:COUNT:기력,8,UNICODE(0x2583),UNICODE(0x2583),BARCOLORSET("파랑"),RESULT:1,327685
			PRINTFORM %UNICODE(0x208E)%
			IF TALENT:MASTER:천리안
				PRINT  스트레스
				IF CFLAG:COUNT:10 >= 95
					SETCOLOR 0xC0C0C0
				ELSEIF CFLAG:COUNT:10 >= 80
					SETCOLOR 0x7070C0
				ELSEIF CFLAG:COUNT:10 >= 50
					SETCOLOR 0x70C070
				ELSEIF CFLAG:COUNT:10 >= 30
					SETCOLOR 0xC07070
				ELSE
					SETCOLOR 0xC070C0
				ENDIF
				PRINTFORM {CFLAG:COUNT:10,3} 
				RESETCOLOR
			ENDIF
			;변경계는 현재의 소질 표시
			SELECTCASE BOUGHT
				CASE 55,56,59
					IF TALENT:COUNT:수
						PRINT  수
					ELSEIF TALENT:COUNT:공
						PRINT  공
					ELSEIF TALENT:COUNT:오메가
						PRINT  오메가
					ENDIF
				CASE 73,74,75
					IF TALENT:COUNT:아이사이즈
						PRINT  아이사이즈
					ELSEIF TALENT:COUNT:단소
						PRINT  단소
					ELSEIF TALENT:COUNT:거근
						PRINT  거근
					ELSEIF TALENT:COUNT:말자지
						PRINT  말자지
					ELSE
						PRINT  남들 수준
					ENDIF
				CASE 65,66
					IF TALENT:COUNT:빈유
						PRINT  빈유
					ELSEIF TALENT:COUNT:절벽
						PRINT  절벽
					ELSEIF TALENT:COUNT:거유
						PRINT  거유
					ELSEIF TALENT:COUNT:폭유
						PRINT  폭유
					ELSE
						PRINT  남들 수준
					ENDIF
				CASE 71,72
					IF TALENT:COUNT:소인체형
						PRINT  소인체형
					ELSEIF TALENT:COUNT:작은체구
						PRINT  작은체구
					ELSEIF TALENT:COUNT:큰체구
						PRINT  큰체구
					ELSE
						PRINT  남들 수준
					ENDIF
				CASE 61,62,69,70
					IF TALENT:COUNT:반음반양 || TALENT:COUNT:후타나리
						SETCOLOR 0x999999
						PRINT  쌍
					ENDIF
					IF TALENT:COUNT:컨트보이
						SETCOLOR 0x8491c3
						PRINTFORM 상♂하♀
					ELSEIF TALENT:COUNT:쉬메일
						SETCOLOR 0xcc7eb1
						PRINTFORM 상♀하♂
					ELSEIF MALE(COUNT)
						SETCOLOR 0x8491c3
						PRINTFORM  ♂
					ELSE
						SETCOLOR 0xcc7eb1
						PRINTFORM  ♀
					ENDIF
					RESETCOLOR
				CASE 96,97
					SIF CFLAG:COUNT:693 == 0
						SETCOLOR 0x808080
						PRINTFORM  부적({CFLAG:COUNT:693})
						RESETCOLOR
					PRINTFORM  /
					SIF CFLAG:COUNT:694 == 0
						SETCOLOR 0x808080
						PRINTFORM  부적·극({CFLAG:COUNT:694})
						RESETCOLOR
					ENDSELECT
			PRINTL 
		NEXT
		SETCOLOR 140,140,140
		DRAWLINE
		RESETCOLOR
		PRINT [999] 돌아온다　　　　　　　　
		$INPUT_LOOP_4
		INPUT
		IF RESULT == 999
			TFLAG:0 = -2
			RETURN 0
		ELSEIF MONEY < ITEMPRICE:BOUGHT
			CLEARLINE 1
			PRINTW 돈이 충분하지 않습니다
			GOTO INPUT_LOOP_2
		ELSEIF !ITEM_USE_CHECK(RESULT,BOUGHT)
			CLEARLINE 1
			GOTO INPUT_LOOP_2
		ENDIF
		MONEY -= ITEMPRICE:BOUGHT
		LOCAL:5 = RESULT
		LOCAL:6 = TARGET
		TARGET = LOCAL:5
		;아이템 사용
		CALL USE_ITEM
		;입상 호출 처리
		;산란 이벤트는 이벤트입상을 별도 기준으로 호출한다(아이템:노란환약분의 텐프레는 없다)
		IF BOUGHT == 84
			TFLAG:13 = 10
		ELSE
			TFLAG:13 = 28
		ENDIF
		CALL KOJO_MESSAGE_EVENT
		TARGET = LOCAL:6
ENDSELECT
TFLAG:0 = -2


;--------------------------------------------------
;아이템 구입의 처리
;--------------------------------------------------
@SHOP_ITEM_LIST(ARG)
DRAWLINE
CALL ITEM_LIST
LOCALS = 
LOCAL:1 = 0
FOR LOCAL,0,199
		CALL SALEITEM_CHECK(LOCAL)
NEXT

;미션 모드
SIF FLAG:5 == 8
	CALL FREE_MISSION_SALEITEM_CHECK

FOR LOCAL,0,199
	;이 가게에서 살 수 있는 상품인가,판매 플래그가 있을까
	SIF !SHOW_ITEM(ARG,LOCAL) || !ITEMSALES:LOCAL
		CONTINUE
	LOCALS = [{LOCAL,2}] %ITEMNAME:LOCAL%(%MONEY_FORM(ITEMPRICE:LOCAL)%고슈금)
	IF STRLENS(LOCALS)
		IF STRLENS(LOCALS) > 26
			LOCAL:1++
			SIF (LOCAL:1 % 3) == 0
				PRINTL 
			PRINTFORM %LOCALS,52,LEFT%
			PRINT  
			SIF (LOCAL:1 % 3) == 0
				LOCAL:1++
		ELSE
			PRINTFORM %LOCALS,26,LEFT%
		ENDIF
		LOCAL:1++
	ENDIF
	IF (LOCAL:1 % 3) == 0
		PRINTL 
	ELSE
		PRINT  
	ENDIF
NEXT
SIF (LOCAL:1 % 3) != 0
	PRINTL 
SETCOLOR 140,140,140
DRAWLINE
RESETCOLOR
TFLAG:0 = -1


;--------------------------------------------------
@USE_ITEM
;--------------------------------------------------
VARSET LOCAL
TFLAG:407 = 0
SELECTCASE BOUGHT
	;【더러움무시】
	CASE 50
		TALENT:더러움무시 = 1
	;【조제지식】
	CASE 51
		TALENT:조제지식 = 1
	;【금단의지식】
	CASE 52
		TALENT:금지된지식 = 1
	;【천리안】
	CASE 53
		TALENT:천리안 = 1
	;【절륜】
	CASE 54
		TALENT:절륜 = 1
	;수
	CASE 55
		;공라면 소질 삭제
		IF TALENT:공
			TALENT:공 = 0
		;없으면[수]추가
		ELSE
			TALENT:수 = 1
		ENDIF
	;공
	CASE 56
		;수라면 소질 삭제
		IF TALENT:수
			TALENT:수 = 0
		;없으면[공]추가
		ELSE
			TALENT:공 = 1
		ENDIF
	;오메가
	CASE 59
		TALENT:오메가 = 1
		;발정기의 카운트 0-6으로 랜덤으로 초기치를 부여
		CFLAG:700 = RAND:7
	;갓빠의 묘약
	CASE 60
		;체력기력 500 회복,기절회복,스트레스 5 추가
		;주인이나 조수가[치료]나[조제지식]소유라면 추가로체력기력 300 회복
		CFLAG:10 += 5
		BASE:체력 += 500
		BASE:기력 += 500
		IF TALENT:MASTER:치료 || TALENT:MASTER:조제지식 || (ASSI >= 0 && (TALENT:ASSI:치료 || TALENT:ASSI:조제지식))
			BASE:체력 += 300
			BASE:기력 += 300
		ENDIF
		SIF BASE:체력 > MAXBASE:체력
			BASE:체력 = MAXBASE:체력
		SIF BASE:기력 > MAXBASE:기력
			BASE:기력 = MAXBASE:기력
		TALENT:기절 = 0
	;약의재료(후타)
	CASE 61
		;스트레스 30 추가
		CFLAG:10 += 30
		;남자 때는반음반양(남성 체형에바기나 있어)(이)가 된다
		IF TALENT:남자
			;바기나 난 경험 플래그가 없으면 랜덤으로 V관계 소질＆[처녀]추가
			SIF !CFLAG:24
				CALL VAGINA_SET(TARGET)
			;바기나 난 경험 플래그,페니스 있던 적 있는 경험 플래그
			CFLAG:24 = 1
			CFLAG:25 = 1
			;[남자]소거[반음반양]추가
			TALENT:남자 = 0
			TALENT:반음반양 = 1
		;여자 때는후타나리(여성 체형에페니스 있어)(이)가 된다
		ELSE
			;페니스 난 경험 없으면,랜덤으로페니스사이즈 설정＆랜덤으로 싸는＆[동정]
			IF !CFLAG:25
				CALL PENIS_SET(TARGET)
				;입상 추가 표시 플래그
				TFLAG:407 = 1
			ENDIF
			;페니스 난 경험 플래그,바기나·가슴 있던 적 있는 경험 플래그
			CFLAG:23 = 1
			CFLAG:24 = 1
			CFLAG:25 = 1
			;[후타나리]추가
			TALENT:후타나리 = 1
		ENDIF
	;약의재료(소멸)
	CASE 62
		;스트레스 10 추가
		CFLAG:10 += 10
		;반음반양 때는 남자에게
		IF TALENT:반음반양
			;바기나 난 경험 플래그,페니스 있던 적 있는 경험 플래그
			CFLAG:24 = 1
			CFLAG:25 = 1
			;[반음반양]소거[남자]추가
			TALENT:남자 = 1
			TALENT:반음반양 = 0
		;후타나리 때는 여자에게
		ELSE
			;페니스 난 경험 플래그,바기나·가슴 있던 적 있는 경험 플래그
			CFLAG:23 = 1
			CFLAG:24 = 1
			CFLAG:25 = 1
			;[후타나리]소거
			TALENT:후타나리 = 0
		ENDIF
	;푸른환약
	CASE 63
		;스트레스 5 추가
		CFLAG:10 += 5
		;[모유체질]추가
		TALENT:모유체질 = 1
	;붉은환약
	CASE 64
		;스트레스 10 추가
		CFLAG:10 += 10
		;[재생처녀]추가
		TALENT:재생처녀 = 1
	;풍유환
	CASE 65
		;스트레스 10 추가
		CFLAG:10 += 10
		;절벽→빈유
		IF TALENT:절벽
			TALENT:절벽 = 0
			TALENT:빈유 = 1
		;빈유→보유
		ELSEIF TALENT:빈유
			TALENT:빈유 = 0
		;보유→거유
		ELSEIF !TALENT:거유
			TALENT:거유 = 1
		;거유→폭유
		ELSE
			TALENT:거유 = 0
			TALENT:폭유 = 1
		ENDIF
	;축유환
	CASE 66
		;스트레스 10 추가
		CFLAG:10 += 10
		;폭유→거유
		IF TALENT:폭유
			TALENT:거유 = 1
			TALENT:폭유 = 0
		;거유→보유
		ELSEIF TALENT:거유
			TALENT:거유 = 0
		;보유→빈유
		ELSEIF !TALENT:빈유
			TALENT:빈유 = 1
		;빈유→절벽,모유도 나오지 않게 된다
		ELSE
			TALENT:절벽 = 1
			TALENT:빈유 = 0
			;입상 추가 표시 플래그
			SIF TALENT:모유체질
				TFLAG:407 = 1
			TALENT:모유체질 = 0
		ENDIF
	;프롤락틴억제제
	CASE 67
		;스트레스 5 추가
		CFLAG:10 += 5
		;[모유체질]삭제
		TALENT:모유체질 = 0
	;3환계항우울제
	CASE 68
		;스트레스 5 추가
		CFLAG:10 += 5
		;[실금버릇]삭제
		TALENT:실금버릇 = 0
	;노란환약
	CASE 84
		;LOCAL =최종적으로 가산되는 스트레스치
		;LOCAL:1 =감산되는 스트레스치(산란경험) LCOAL:2 =가산되는 스트레스치(부친)
		;LOCAL:3 =가산되는이상경험의 수
		;PREGNANT = 임신 하고 있는 캐릭터의 등록 번호 FATHER =부친의 등록 번호
		PREGNANT = TARGET
		;[임신]삭제
		TALENT:PREGNANT:임신 = 0
		;임신이벤트로 줄어든체력의 최대치가 회복(적은)
		MAXBASE:PREGNANT:체력 += 300
		;질로 임신하고 있다면 여자
		IF CFLAG:PREGNANT:101 / 100 == 1
