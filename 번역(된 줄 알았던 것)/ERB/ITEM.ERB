	;포장마차·증축
	CASE 93
		;포장마차를 가지고 있지 않다
		SIF !ITEM:92
			ITEMSALES:ARG = 0
	;진화의비법
	CASE 58
		;촉수사역술를 가지고 있지 않다
		SIF !ITEM:57
