;-------------------------------------------------
;26,도시락
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_KX1_26
;-------------------------------------------------
;기입 체크(=0,비표시,1,표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;최초부터 도시락을 먹을 때
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 1
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;파는 것을 그만두어 먹을 때
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 2
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;주인이 도시락을 만들 때
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 3
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;밥후의 운동(성적인 의미로) 직전
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 4
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;독을 담아졌다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 5
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;독을 간파했으므로 약점을 잡는 이벤트(궁합이 좋은 아이에게 먹이는 ver)
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 6
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;독을 간파했으므로 약점을 잡는 이벤트(캐묻는 ver)
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 7
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;독살되었다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && K == 8
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;일반
	PRINTFORMW 
	PRINTFORMW 
	RETURN LOCAL
ENDIF

