		LOCAL = -3
	ELSEIF TALENT:(ARG:1):소인체형
		LOCAL = 0
	ELSEIF TALENT:(ARG:1):작은체구
		LOCAL = -1
	ELSE
		LOCAL = -2
	ENDIF
;普通体型の男
ELSEIF MALE(ARG)
	IF TALENT:(ARG:1):큰체구 && MALE(ARG:1)
		LOCAL = -1
	ELSEIF TALENT:(ARG:1):소인체형 || TALENT:(ARG:1):작은체구
