					;姉弟
					RETURNF 3
				;キャラ２が女性
				ELSE
					;姉妹
					RETURNF 5
				ENDIF
			ENDIF
		;キャラ２の方が年上
		ELSE
			;キャラ１が男性
			IF MALE(ARG)
				;キャラ２が男性
				IF MALE(ARG:1)
					;兄弟
					RETURNF 1
				;キャラ２が女性
				ELSE
					;姉弟
					RETURNF 3
				ENDIF
			;キャラ１が女性
			ELSE
				;キャラ２が男性
				IF MALE(ARG:1)
					;兄妹
					RETURNF 4
				;キャラ２が女性
				ELSE
					;姉妹
					RETURNF 5
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF

RETURNF 0

