;-------------------------------------------------
;음핵전극
;Ｃ系調教コマンド
;-------------------------------------------------
@COM25

PRINTL 음핵전극
STR:0 = 음핵전극
CALL KOJO_MESSAGE_COM

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
SOURCE:쾌C += 500

IF TEQUIP:지하실 || TFLAG:56 == 3
	;消費체력、기력
	LOSEBASE:체력 += 300
	LOSEBASE:기력 += 450
	
	SOURCE:통증 += 5000
	SOURCE:두려움 += 3000
	SOURCE:굴복 += 1600
ELSEIF TFLAG:56 == 2
	;消費체력、기력
	LOSEBASE:체력 += 200
	LOSEBASE:기력 += 350
	
	SOURCE:통증 += 3500
	SOURCE:두려움 += 1500
	SOURCE:굴복 += 1200
ELSE
	;消費체력、기력
	LOSEBASE:체력 += 100
	LOSEBASE:기력 += 250
	
	SOURCE:통증 += 1500
	SOURCE:두려움 += 500
	SOURCE:굴복 += 800
ENDIF

SOURCE:노출 += 300
SOURCE:일탈 += 1200
SOURCE:반발 += 1200

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 1200

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;電극初体験のとき、이상경험追加
IF EXP:전극경험 == 0
	EXP:이상경험 += 1
	PRINTL 電극初経験で이상경험＋１
ENDIF
EXP:전극경험 += 1

;게이경험とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(복종で1、예속で2）
IF TALENT:복종 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:예속 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


