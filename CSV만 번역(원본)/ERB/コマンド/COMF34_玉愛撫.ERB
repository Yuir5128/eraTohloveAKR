;-------------------------------------------------
;구슬애무
;Ｐ系調教コマンド
;-------------------------------------------------
@COM34

PRINTL 구슬애무
STR:0 = 구슬애무
CALL KOJO_MESSAGE_COM

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 30

SOURCE:쾌C += 30

SOURCE:두려움 += 20
SOURCE:노출 += 30
SOURCE:불결 += 50
SOURCE:일탈 += 80
SOURCE:반발 += 80

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 80

;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 1



RETURN 1


