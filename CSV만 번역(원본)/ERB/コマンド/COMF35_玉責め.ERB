;-------------------------------------------------
;玉を舐める
;Ｐ系調教コマンド
;-------------------------------------------------
@COM35

PRINTL 구슬고문
STR:0 = 구슬고문
CALL KOJO_MESSAGE_COM

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 100
LOSEBASE:기력 += 250

SOURCE:쾌C += 20

IF TEQUIP:삼각목마
	SOURCE:통증 += 1500
ELSE
	SOURCE:통증 += 800
ENDIF

SOURCE:두려움 += 1300
SOURCE:노출 += 30
SOURCE:불결 += 20
SOURCE:일탈 += 100
SOURCE:반발 += 600


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 600

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(2)

;依存度ベクトル
TFLAG:33 = 3

;依存度補正値(복종で1、예속で2）
IF TALENT:복종 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:예속 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

