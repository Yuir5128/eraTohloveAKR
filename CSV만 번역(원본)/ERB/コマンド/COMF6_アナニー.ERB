;-------------------------------------------------
;자위
;자위系コマンド
;-------------------------------------------------
@COM6

;바이브、비디오촬영の時に内容変化
;비디오촬영中なら공개자위
;야외플레이中なら野外オナニー
;애널바이브装備で바이브オナニー
LOCALS = 

SIF TEQUIP:비디오카메라
	LOCALS = 公開
SIF TEQUIP:야외플레이
	LOCALS = 野外

IF TEQUIP:확장벌룬
	LOCALS = %LOCALS%バルーン
ELSEIF TEQUIP:애널바이브
	LOCALS = %LOCALS%바이브
ELSEIF TEQUIP:로터A삽입
	LOCALS = %LOCALS%로터
ELSEIF TEQUIP:애널비즈
	LOCALS = %LOCALS%ビーズ
ENDIF

LOCALS = %LOCALS%자위

STR:0 = %LOCALS%
PRINTFORML %STR:0%

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER

;実行判定操作
CALL KOJO_DENIAL_PLUS

;合計を表示(35以上で実行)
PRINT  = 
PRINTV A

;難易度上昇
;公開で+10、野外で+10
V = 38
SIF TEQUIP:비디오카메라
	V += 10
SIF TEQUIP:야외플레이
	V += 10

SIF TEQUIP:확장벌룬
	V += 10
SIF TEQUIP:애널바이브
	V += 5
SIF TEQUIP:애널비즈
	V += 5
SIF TEQUIP:로터A삽입
	V += 3

SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

;実行不可입上/実行判定操作
SIF A < V
	CALL KOJO_DENIAL

WAIT

;実行できない
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM


;実行者が奴隷であるフラグ
TFLAG:28 = 1

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 20
LOSEBASE:기력 += 70

SOURCE:쾌A += 80
SOURCE:통증 += 80

IF TEQUIP:애널바이브 || TEQUIP:로터A삽입 || TEQUIP:애널비즈 || TEQUIP:확장벌룬
	TIMES SOURCE:쾌A , 1.50

	SIF TEQUIP:확장벌룬
		SOURCE:통증 += 200
	SIF TEQUIP:애널바이브
		SOURCE:통증 += 200
	SIF TEQUIP:애널비즈
		SOURCE:통증 += 200
	SIF TEQUIP:로터A삽입
		SOURCE:통증 += 130

	SOURCE:굴복 += 150
	SOURCE:반발 += 150
ENDIF


;Aへの고통は先に処理しておく
;EXP:A경험をみる
IF EXP:A경험 < EXPLV:1
	TIMES SOURCE:통증 , 3.00
	TIMES SOURCE:반발 , 1.50
ELSEIF EXP:A경험 < EXPLV:2
	TIMES SOURCE:통증 , 1.20
	TIMES SOURCE:반발 , 1.20
ELSEIF EXP:A경험 < EXPLV:3
	TIMES SOURCE:통증 , 0.80
ELSEIF EXP:A경험 < EXPLV:4
	TIMES SOURCE:통증 , 0.60
ELSEIF EXP:A경험 < EXPLV:5
	TIMES SOURCE:통증 , 0.40
ELSEIF EXP:A경험 >= EXPLV:5
	TIMES SOURCE:통증 , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:윤활 < PALAMLV:1
	TIMES SOURCE:통증 , 1.20
	TIMES SOURCE:반발 , 1.20
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES SOURCE:통증 , 1.00
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES SOURCE:통증 , 0.60
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES SOURCE:통증 , 0.30
ELSEIF PALAM:윤활 >= PALAMLV:4
	TIMES SOURCE:통증 , 0.20
ENDIF

SOURCE:성행동 += 100
SOURCE:두려움 += 250
SOURCE:중독충족 += 250
SOURCE:노출 += 450
SOURCE:굴복 += 600
SOURCE:일탈 += 350
SOURCE:반발 += 200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 450

IF TEQUIP:비디오카메라 || TEQUIP:야외플레이
	SELECTCASE ABL:노출벽
		CASE 0
			TIMES SOURCE:반발 , 2.00
		CASE 1
			SOURCE:욕정 += 10
			TIMES SOURCE:반발 , 1.20
		CASE 2
			SOURCE:욕정 += 20
			TIMES SOURCE:반발 , 1.00
		CASE 3
			SOURCE:욕정 += 50
			TIMES SOURCE:반발 , 0.70
		CASE 4
			SOURCE:욕정 += 75
			TIMES SOURCE:반발 , 0.50
		CASE 5
			SOURCE:욕정 += 80
			TIMES SOURCE:반발 , 0.30
		CASE 6
			SOURCE:욕정 += 100
			TIMES SOURCE:반발 , 0.10
		CASE 7
			SOURCE:욕정 += 120
			TIMES SOURCE:반발 , 0.10
		CASE 8
			SOURCE:욕정 += 150
			TIMES SOURCE:반발 , 0.10
		CASE 9
			SOURCE:욕정 += 180
			TIMES SOURCE:반발 , 0.10
		CASEELSE
			SOURCE:욕정 += 200
			TIMES SOURCE:반발 , 0.10
	ENDSELECT

	;ビデオ
	IF TEQUIP:비디오카메라
		SOURCE:노출 += 50
		SOURCE:액체 += 50
		SOURCE:욕정 += 50
		SOURCE:반발 += 80
	ENDIF
	;야외플레이
	IF TEQUIP:야외플레이
		SOURCE:노출 += 100
		SOURCE:굴복 += 100
		SOURCE:반발 +=80
	ENDIF
ENDIF

;EXP:A경험をみる
IF EXP:A경험 < EXPLV:1
	TIMES SOURCE:반발 , 1.50
ELSEIF EXP:A경험 < EXPLV:2
	TIMES SOURCE:반발 , 1.20
ENDIF

;中毒による중독충족のソースは先に処理しておく
;자위중독を見る
IF ABL:자위중독 == 1
	TIMES SOURCE:쾌A , 1.00
	TIMES SOURCE:중독충족 , 0.00
ELSEIF ABL:자위중독 == 2
	TIMES SOURCE:쾌A , 1.10
	TIMES SOURCE:중독충족 , 0.30
ELSEIF ABL:자위중독 == 3
	TIMES SOURCE:쾌A , 1.20
	TIMES SOURCE:중독충족 , 0.60
ELSEIF ABL:자위중독 == 4
	TIMES SOURCE:쾌A , 1.30
	TIMES SOURCE:중독충족 , 0.90
ELSEIF ABL:자위중독 == 5
	TIMES SOURCE:쾌A , 1.50
	TIMES SOURCE:중독충족 , 1.20
ELSEIF ABL:자위중독 == 6
	TIMES SOURCE:쾌A , 1.50
	TIMES SOURCE:중독충족 , 1.50
ELSEIF ABL:자위중독 == 7
	TIMES SOURCE:쾌A , 1.70
	TIMES SOURCE:중독충족 , 1.80
ELSEIF ABL:자위중독 == 8
	TIMES SOURCE:쾌A , 2.00
	TIMES SOURCE:중독충족 , 2.30
ELSEIF ABL:자위중독 == 9
	TIMES SOURCE:쾌A , 2.50
	TIMES SOURCE:중독충족 , 3.00
ELSEIF ABL:자위중독 >= 10
	TIMES SOURCE:쾌A , 3.50
	TIMES SOURCE:중독충족 , 5.00
ENDIF


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の손（장갑）⇔奴隷のＡ장내の汚れが移動
;道具を使う場合は移動しない
IF TEQUIP:애널바이브 || TEQUIP:로터A삽입 || TEQUIP:애널비즈 || TEQUIP:확장벌룬
ELSE
	CALL COM_STAIN
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:A경험 += 1
PRINTL A경험＋１
;비디오촬영時は経験にプラス
IF TEQUIP:비디오카메라
	EXP:자위경험 += 2
	PRINTL 자위경험＋２
	EXP:조교자위경험 += 2
	PRINTL 조교자위경험＋２
	
	IF !EXP:공개자위 && !TALENT:음란 && !TALENT:창부
		EXP:이상경험 += 1
		PRINTL 공개자위初経験で이상경험＋１
	ENDIF
	EXP:공개자위 += 1
	PRINTL 공개자위経験＋１
ELSE
	EXP:자위경험 += 1
	PRINTL 자위경험＋１
	EXP:조교자위경험 += 1
	PRINTL 조교자위경험＋１
ENDIF

;게이경험とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 3

;依存度補正値(복종で1、예속で2）
IF TALENT:복종 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:예속 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

RETURN 1


