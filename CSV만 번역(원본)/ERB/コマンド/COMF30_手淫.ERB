;-------------------------------------------------
;수음한다
;Ｐ系調教コマンド
;-------------------------------------------------
@COM30

PRINTL 수음한다
STR:0 = 수음한다
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 30

SOURCE:쾌C += 80
SIF TEQUIP:전동오나홀
	TIMES SOURCE:쾌C , 1.50

;着衣のとき
SIF TEQUIP:속옷（아래） || TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TIMES SOURCE:쾌C , 0.90
SIF BOTTOMS(TARGET)
	TIMES SOURCE:쾌C , 0.70

SOURCE:두려움 += 20
SOURCE:욕정 += 30
SOURCE:노출 += 20
SOURCE:일탈 += 15
SOURCE:반발 += 15

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 20

;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;調教者の손⇔奴隷のＣ（下着、ズボン系衣類）の汚れが移動
;道具を使う場合は汚れは移動しない
IF TEQUIP:전동오나홀
ELSE
	CALL COM_STAIN
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1



