;-------------------------------------------------
;６９파이즈리
;派生系コマンド
;-------------------------------------------------
@COM504

PRINTL ６９파이즈리
STR:0 = ６９파이즈리

SELECTCOM = 504
CALL KOJO_MESSAGE_COM

;実行者が奴隷であるフラグ
TFLAG:28 = 1

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;사정ゲージチェック
;-------------------------------------------------
B = 800

CALL SHOOT_GAUGE_CHECK_MT

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:사정 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 30
LOSEBASE:기력 += 100

SOURCE:쾌C += 150
SOURCE:쾌B += 80

SOURCE:애정 += 350

;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

SOURCE:성행동 += 500
SOURCE:성취감 += 300
SOURCE:액체 += 80
SOURCE:노출 += 100
SOURCE:굴복 += 1200
SOURCE:일탈 += 700
SOURCE:반발 += 700

;上のほうで計算した汚れデータ
SOURCE:불결 += Y*10 + 60


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 1000


;-------------------------------------------------
;사정チェック
;-------------------------------------------------
CALL SAMEN_CHECK(PLAYER,1)

;사정時の処理
IF E >= 1
	;사정先タイプの設定
	TFLAG:703 = 4
	;사정先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（사정ゲージの再計算や사정경험の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(7)

;애정경험
IF (TALENT:연모 == 1 || TALENT:친애 == 1) && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +1
	EXP:애정경험 += 1
ENDIF

;依存度ベクトル
TFLAG:33 = 1

;依存度補正値(主人かつ연모、음란、복종で1。친애、창부、예속で2）
IF (TALENT:연모 || TALENT:음란 || TALENT:복종) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:친애 || TALENT:창부 || TALENT:예속) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

TFLAG:100 = 1


RETURN 1






