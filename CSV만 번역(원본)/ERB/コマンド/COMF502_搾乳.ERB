;-------------------------------------------------
;搾乳
;派生系コマンド
;-------------------------------------------------
@COM502

PRINTL 搾乳
STR:0 = 搾乳

SELECTCOM = 502
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 30
LOSEBASE:기력 += 50

SOURCE:쾌B += 200

SOURCE:애정 += 50
;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

;유아／유아퇴행もしくは소인체형だと애정のソース追加
SIF TALENT:유아／유아퇴행 || TALENT:소인체형
	TIMES SOURCE:애정 , 1.20
;모성持ちだと애정のソース追加
SIF TALENT:모성
	TIMES SOURCE:애정 , 2.00

;지하실にいると고통のソース追加
SIF TEQUIP:지하실
	SOURCE:통증 += 150

SOURCE:노출 += 100
SOURCE:불결 += 50
SOURCE:반발 += 100


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 100

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(5)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1



