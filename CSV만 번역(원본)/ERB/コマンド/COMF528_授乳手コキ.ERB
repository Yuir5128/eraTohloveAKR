;-------------------------------------------------
;授乳손コキ
;派生系コマンド
;-------------------------------------------------
@COM528

PRINTL 授乳손コキ
STR:0 = 授乳손コキ
SELECTCOM = 528

;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER

;実行判定操作
CALL KOJO_DENIAL_PLUS

;合計を表示(14以上で実行)
PRINT  = 
PRINTV A

V = 14
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

;実行不可입上/実行判定操作
SIF A < V
	CALL KOJO_DENIAL

WAIT

;実行できない
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;사정ゲージチェック
;-------------------------------------------------
B = 650

CALL SHOOT_GAUGE_CHECK_MT

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:사정 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 25
LOSEBASE:기력 += 80

SOURCE:쾌B += 100

SOURCE:애정 += 500

;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

;모성持ちだと애정のソース追加
SIF TALENT:모성
	TIMES SOURCE:애정 , 2.00

SOURCE:성취감 += 500
SOURCE:순종 += 1000
SOURCE:굴복 += 1000
SOURCE:일탈 += 100
SOURCE:반발 += 100

;上のほうで計算した汚れデータ
SOURCE:불결 += Y*10 + 60


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;調教者の입←奴隷の体へ汚れが移動
STAIN:PLAYER:입 |= STAIN:가슴
STAIN:가슴 = 0

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 600

;-------------------------------------------------
;사정チェック
;-------------------------------------------------
CALL SAMEN_CHECK(PLAYER,1)

;사정時の処理
IF E >= 1
	;사정先タイプの設定
	TFLAG:703 = 3
	;사정先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（사정ゲージの再計算や사정경험の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(5)

;애정경험
IF (TALENT:연모 == 1 || TALENT:친애 == 1) && ASSIPLAY == 0
	EXP:애정경험 += 1
	PRINTL 애정경험+１
ENDIF

;依存度ベクトル
TFLAG:33 = 4

;依存度補正値(主人かつ연모で1、친애で2）
IF TALENT:연모 && ASSIPLAY == 0
	TFLAG:30 += 2
ELSEIF TALENT:친애 && ASSIPLAY == 0
	TFLAG:30 += 4
ENDIF

TFLAG:100 = 1


RETURN 1



