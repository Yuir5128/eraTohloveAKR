;-------------------------------------------------
;전동오나홀
;Ｐ系調教コマンド
;-------------------------------------------------
@COM32

IF TEQUIP:촉수
	PRINTL 촉수페니스고문
	STR:0 = 촉수페니스고문
	SELECTCOM = 343
ELSE
	PRINTL 전동오나홀
	STR:0 = 전동오나홀
ENDIF

CALL KOJO_MESSAGE_COM


;着脱の処理
IF TEQUIP:전동오나홀 == 0
	TEQUIP:전동오나홀 = 1
ELSE
	TEQUIP:전동오나홀 = 0
ENDIF

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 10
LOSEBASE:기력 += 50

SOURCE:쾌C += 200

SOURCE:노출 += 20
SOURCE:일탈 += 30
SOURCE:반발 += 30

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 30


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(1)

;촉수소환時、촉수경험のリセットをする
SIF TEQUIP:촉수 == 1
	T = 0

;依存度ベクトル
TFLAG:33 = 1


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM32
;オナホール装備中
IF TEQUIP:촉수
	PRINTL ＜촉수페니스고문中＞
ELSE
	PRINTL ＜オナホール装備中＞
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
LOSEBASE:체력 += 5
LOSEBASE:기력 += 20

SOURCE:쾌C += 100
SOURCE:노출 += 10
SOURCE:일탈 += 15
SOURCE:반발 += 15


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;촉수경험加算
SIF TEQUIP:촉수 == 1
	T += 1


RETURN 1

