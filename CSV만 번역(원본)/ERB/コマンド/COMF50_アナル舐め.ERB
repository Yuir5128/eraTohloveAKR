;-------------------------------------------------
;애널자극
;Ａ系調教コマンド
;-------------------------------------------------
@COM50

;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;변의が高いと식분한다に派生
IF BASE:변의 >= 90
	CALL COM_ABLE286
	SIF RESULT == 1
		JUMP COM286
ENDIF

PRINTL 애널자극
STR:0 = 애널자극
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 30

SOURCE:쾌A += 30

;調教者のTALENT:혀놀림をみる
IF TALENT:PLAYER:혀놀림
	TIMES SOURCE:쾌A , 1.50
ELSEIF TALENT:PLAYER:고양이혀
	TIMES SOURCE:쾌A , 0.75
ENDIF

;着衣のとき
SIF (TEQUIP:속옷（아래） && TEQUIP:속옷（아래） != 5) || TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TIMES SOURCE:쾌A , 0.70
SIF EXP:A경험 >= 100 && PALAM:욕정 >= PALAMLV:3
	TIMES SOURCE:쾌A , 1.20

SOURCE:액체 += 50
SOURCE:일탈 += 40
SOURCE:반발 += 30


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 50

;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:A경험 += 1
PRINTL A경험＋１

IF TALENT:PLAYER:고양이혀
	EXP:PLAYER:가학쾌락경험 += 1
	PRINTFORML %CALLNAME:PLAYER%の가학쾌락경험＋１
ENDIF

;게이경험とか加算
CALL COM_EXP(3)

;依存度ベクトル
TFLAG:33 = 3


RETURN 1


