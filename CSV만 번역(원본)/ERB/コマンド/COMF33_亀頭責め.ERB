;-------------------------------------------------
;귀두고문
;Ｐ系調教コマンド
;-------------------------------------------------
@COM33

PRINTL 귀두고문
STR:0 = 귀두고문
CALL KOJO_MESSAGE_COM

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 100
LOSEBASE:기력 += 200

SOURCE:쾌C += 350

SOURCE:통증 += 200
SIF TALENT:C민감
	SOURCE:통증 += 200
SOURCE:두려움 += 200
SOURCE:노출 += 120
SOURCE:굴복 += 160
SOURCE:일탈 += 80
SOURCE:반발 += 80


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 80

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 3

RETURN 1



