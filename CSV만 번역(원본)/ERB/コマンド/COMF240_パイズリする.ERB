;-------------------------------------------------
;파이즈리한다
;奉仕する系コマンド
;-------------------------------------------------
@COM240

;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前回の調教が파이즈리、６９파이즈리のとき、６９파이즈리に
	IF PREVCOM == 73 || PREVCOM == 504
		CALL COM_ABLE504
		SIF RESULT == 1
			JUMP COM504
	ENDIF
ENDIF

PRINTL 파이즈리한다
STR:0 = 파이즈리한다
CALL KOJO_MESSAGE_COM

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;사정ゲージチェック
;-------------------------------------------------
B = 50

CALL SHOOT_GAUGE_CHECK_MT

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:사정 += B


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 30

SOURCE:쾌C += 80

SOURCE:애정 += 100

;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50
;プレイヤーが거유もしくは폭유だと애정のソース追加
SIF TALENT:PLAYER:거유 || TALENT:PLAYER:폭유
	TIMES SOURCE:애정 , 1.20


SOURCE:욕정 += 50
SOURCE:노출 += 30
SOURCE:일탈 += 20
SOURCE:반발 += 20

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 50


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(2)

;依存度ベクトル
TFLAG:33 = 1

;依存度補正値(음란で1、창부で2）
IF TALENT:음란 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:창부 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


