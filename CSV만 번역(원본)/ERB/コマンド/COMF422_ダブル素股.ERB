;-------------------------------------------------
;더블스마타
;主人×（助손＋奴隷）系コマンド
;-------------------------------------------------
@COM422

PRINTL 더블스마타
STR:0 = 더블스마타
SELECTCOM = 422

;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER

;実行判定操作
CALL KOJO_DENIAL_PLUS

;合計を表示(40以上で実行)
PRINT  = 
PRINTV A

V = 40
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

;実行不可입上/実行判定操作
SIF A < V
	CALL KOJO_DENIAL

WAIT

;実行できない
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM


;実行者が奴隷であるフラグ
TFLAG:28 = 1

;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;사정ゲージチェック
;-------------------------------------------------
B = 950

CALL SHOOT_GAUGE_CHECK_3PM

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:MASTER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:MASTER:사정 += B


;-------------------------------------------------
;사정ゲージチェック（助손）
;-------------------------------------------------
B = 50

CALL SHOOT_GAUGE_CHECK_3PA

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:MASTER:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:사정 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 20
LOSEBASE:기력 += 80

SOURCE:쾌C += 80
SIF PENIS(TARGET)
	TIMES SOURCE:쾌C , 0.50

SOURCE:애정 += 150

;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

SOURCE:성행동 += 300
SOURCE:성취감 += 100
SOURCE:굴복 += 600
SOURCE:일탈 += 200
SOURCE:반발 += 200

;上のほうで計算した汚れデータ
SOURCE:불결 += Y*10 + 60


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 400


;-------------------------------------------------
;사정チェック
;-------------------------------------------------
CALL SAMEN_CHECK(PLAYER,1)

;사정時の処理
IF E >= 1
	;사정先タイプの設定
	TFLAG:703 = 5
	;사정先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（사정ゲージの再計算や사정경험の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＶ⇔調教者のＰの汚れが移動
;助손のＶ⇔調教者のＰの汚れが移動
STAIN:바기나 |= STAIN:MASTER:페니스
STAIN:ASSI:바기나 |= STAIN:MASTER:페니스

STAIN:MASTER:페니스 |= STAIN:ASSI:바기나
STAIN:MASTER:페니스 |= STAIN:바기나

;奴隷のＶ⇔助손のＶの汚れが移動
STAIN:바기나 |= STAIN:ASSI:바기나
STAIN:ASSI:바기나 |= STAIN:바기나


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(7)

;依存度ベクトル
P = PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;依存度補正値(相性100↑かつ음란で2、창부で4）
P = PLAYER
IF TALENT:음란 && RELATION:P > 100
	TFLAG:30 += 2
ELSEIF TALENT:창부 && RELATION:P > 100
	TFLAG:30 += 4
ENDIF

;奉仕快楽フラグ
TFLAG:100 = 1


RETURN 1

