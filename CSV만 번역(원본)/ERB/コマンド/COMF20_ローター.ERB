;-------------------------------------------------
;로터
;Ｃ系調教コマンド
;-------------------------------------------------
@COM20

IF PENIS(TARGET)
	PRINTL 페니스로터
	STR:0 = 페니스로터
ELSE
	PRINTL クリトリス로터
	STR:0 = クリトリス로터
ENDIF

CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 30

SOURCE:쾌C += 150
;着衣（ズボン系）のとき
SIF BOTTOMS(TARGET) >= 4
	TIMES SOURCE:쾌C , 0.60
SIF TEQUIP:속옷（아래） || TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TIMES SOURCE:쾌C , 0.90

SOURCE:노출 += 20
SOURCE:일탈 += 30
SOURCE:반발 += 30


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 30


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1




