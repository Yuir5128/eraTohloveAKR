;-------------------------------------------------
;유두로터
;Ｂ系調教コマンド
;-------------------------------------------------
@COM12

PRINTL 유두로터
STR:0 = 유두로터
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 30

SOURCE:쾌B += 150
;着衣プレイ
SIF TEQUIP:속옷（위） || TOPS(TARGET)
	TIMES SOURCE:쾌B , 0.70

SOURCE:노출 += 20
SOURCE:일탈 += 30
SOURCE:반발 += 30


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 30


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1




