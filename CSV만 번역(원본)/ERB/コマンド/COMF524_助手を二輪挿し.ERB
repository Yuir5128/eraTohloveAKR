;-------------------------------------------------
;助손を二輪挿し
;派生系コマンド
;-------------------------------------------------
@COM524

PRINTL 助손を二輪挿し
STR:0 = 助손を二輪挿し

SELECTCOM = 524
CALL KOJO_MESSAGE_COM


;스타킹びりびりフラグ
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF

;처녀喪失フラグ
SIF TALENT:ASSI:처녀 || TALENT:ASSI:재생처녀
	TFLAG:19 |= 3

;調教者が동정だった場合の喪失フラグ
SIF TALENT:동정
	TFLAG:160 |= 1

;調教者が동정だった場合の喪失フラグ
SIF TALENT:MASTER:동정
	TFLAG:160 |= 2


;-------------------------------------------------
;사정ゲージチェック（主人）
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PM

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:MASTER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:MASTER:사정 += B

;-------------------------------------------------
;사정ゲージチェック（助손）
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PA

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:ASSI:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:사정 += B


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
LOSEBASE:체력 += 250
LOSEBASE:기력 += 250

SOURCE:쾌C += 350

SOURCE:애정 += 300

;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

SOURCE:성행동 += 500
SOURCE:통증 += 400
SOURCE:중독충족 += 250
SOURCE:노출 += 100
SOURCE:굴복 += 500
SOURCE:반발 += 250

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 400
	
;정조관념持ちで연모、친애持ち以外には常に반발のソースを追加する（セックス系のみ）
SIF TALENT:정조관념 && (TALENT:연모 == 0 && TALENT:친애 == 0)
	SOURCE:반발 += 1500

;中毒による중독충족のソースは先に処理しておく
;섹스중독を見る
IF ABL:섹스중독 == 1
	TIMES SOURCE:중독충족 , 0.00
ELSEIF ABL:섹스중독 == 2
	TIMES SOURCE:중독충족 , 0.30
ELSEIF ABL:섹스중독 == 3
	TIMES SOURCE:중독충족 , 0.60
ELSEIF ABL:섹스중독 == 4
	TIMES SOURCE:중독충족 , 0.90
ELSEIF ABL:섹스중독 == 5
	TIMES SOURCE:중독충족 , 1.20
ELSEIF ABL:섹스중독 == 6
	TIMES SOURCE:중독충족 , 1.50
ELSEIF ABL:섹스중독 == 7
	TIMES SOURCE:중독충족 , 1.80
ELSEIF ABL:섹스중독 == 8
	TIMES SOURCE:중독충족 , 2.30
ELSEIF ABL:섹스중독 == 9
	TIMES SOURCE:중독충족 , 3.00
ELSEIF ABL:섹스중독 >= 10
	TIMES SOURCE:중독충족 , 5.00
ENDIF

;말자지持ちで、調教者が소인체형だった場合、일탈ソース追加
SIF TALENT:ASSI:소인체형 && TALENT:말자지
	SOURCE:일탈 += 500

;-------------------------------------------------
;사정チェック１（主人）
;-------------------------------------------------
CALL SAMEN_CHECK(MASTER,1)

;사정時の処理
IF E >= 1
	;사정先タイプの設定
	TFLAG:703 = 1
	;사정先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（사정ゲージの再計算や사정경험の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;조수를범한다で사정したフラグ
SIF E == 2
	TFLAG:7 = 2
SIF E == 1
	TFLAG:7 = 1

;-------------------------------------------------
;사정チェック２（助손）
;-------------------------------------------------
CALL SAMEN_CHECK(ASSI)

;사정時の処理
IF E >= 1
	;사정先タイプの設定
	TFLAG:703 = 6
	;사정先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（사정ゲージの再計算や사정경험の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＰ⇔調教者のＶの汚れが移動
;페니스밴드を使う場合、汚れは移動しない
IF PENIS(TARGET)
	STAIN:MASTER:페니스 |= STAIN:ASSI:바기나
	STAIN:ASSI:바기나 |= STAIN:MASTER:페니스
	STAIN:MASTER:페니스 |= STAIN:ASSI:질내
	STAIN:ASSI:질내 |= STAIN:MASTER:페니스
ENDIF

;奴隷のＰ⇔調教者のＶの汚れが移動
;페니스밴드を使う場合、汚れは移動しない
IF PENIS(TARGET)
	STAIN:페니스 |= STAIN:ASSI:바기나
	STAIN:ASSI:바기나 |= STAIN:페니스
	STAIN:페니스 |= STAIN:ASSI:질내
	STAIN:ASSI:질내 |= STAIN:페니스
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;V경험
EXP:ASSI:V경험 += 10
PRINTFORML %CALLNAME:ASSI%のV경험＋１０

;V확장경험
IF EXP:ASSI:V확장경험 == 0
	EXP:ASSI:이상경험 += 1
	PRINTFORML Ｖ拡張初経験で%CALLNAME:ASSI%の이상경험＋１
ENDIF
EXP:ASSI:V확장경험 += 2
PRINTFORML %CALLNAME:ASSI%のV확장경험＋2

;성교경험
EXP:PLAYER:성교경험 += 1
EXP:성교경험 += 1
EXP:ASSI:성교경험 += 2
PRINTFORML %CALLNAME:ASSI%の성교경험＋2

;삽입경험
EXP:PLAYER:삽입경험 += 1
EXP:삽입경험 += 1

;레즈경험
A = 5
B = NO:MASTER + 1099
C = NO:ASSI + 1099
D = 8

IF MALE(TARGET) == 0 && MALE(MASTER) == 0 && MALE(ASSI) == 0
	PRINTFORML %EXPNAME:40%+{D}
	EXP:레즈경험 += D
ELSEIF MALE(TARGET) == 0 && MALE(MASTER) == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:레즈경험 += A
ELSEIF MALE(TARGET) == 0 && MALE(ASSI) == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:레즈경험 += A
ELSEIF MALE(TARGET) && MALE(MASTER) && MALE(ASSI)
	PRINTFORML %EXPNAME:41%+{D}
	EXP:게이경험 += D
ELSEIF MALE(TARGET) && MALE(MASTER)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:게이경험 += A
ELSEIF MALE(TARGET) && MALE(ASSI)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:게이경험 += A
ENDIF
;調教者を記録
CFLAG:B += A
CFLAG:C += A

;依存度ベクトル
TFLAG:33 = 1

;奉仕快楽フラグ
TFLAG:100 = 1

RETURN 1


