;-------------------------------------------------
;가슴애무
;Ｂ系調教コマンド
;-------------------------------------------------
@COM10

PRINTL 가슴애무
STR:0 = 가슴애무
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費체력、기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 20

SOURCE:쾌B += 80
;着衣プレイ
SIF TEQUIP:속옷（위） || TOPS(TARGET)
	TIMES SOURCE:쾌B , 0.70

SOURCE:애정 += 50
;스킨십선호だと애정のソース追加
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

SOURCE:성행동 += 60
SOURCE:노출 += 10
SOURCE:불결 += 20
SOURCE:반발 += 20


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース追加
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 20



;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;게이경험とか加算
CALL COM_EXP(5)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1


