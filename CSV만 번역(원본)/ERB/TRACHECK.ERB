;-------------------------------------------------
;처녀／재생처녀喪失時の処理
;43바이브、100～106セックス、
;-------------------------------------------------
@LOST_VIRGIN
;--------------------
;奴隷の처녀喪失
;--------------------
IF TALENT:처녀 && TFLAG:19 == 1
	PRINTW 처녀喪失
	TALENT:처녀 = 0
	
	STAIN:질내 |= 64
	STAIN:바기나 |= 64
	
	;처녀喪失フラグ(今回のコマンド)
	TFLAG:3 = 1
	;처녀喪失フラグ(今回の調教)
	TFLAG:31 = 1
	
	;정조관념
	IF TALENT:정조관념
		;정조관념+연모or친애（主人による처녀喪失のみ）
		IF (TALENT:연모 || TALENT:친애) && TFLAG:20 == 1
			TIMES SOURCE:애정 , 1.20
			TIMES SOURCE:반발 , 0.50
			;발정기
			SIF TALENT:발정기
				TIMES SOURCE:울굴 , 1.50
		ELSE
			TIMES SOURCE:애정 , 0.60
			TIMES SOURCE:반발 , 10.00
			;발정기
			SIF TALENT:발정기
				TIMES SOURCE:울굴 , 2.00
		ENDIF
	;정조무관심
	ELSEIF TALENT:정조무관심
		TIMES SOURCE:애정 , 0.60
		TIMES SOURCE:반발 , 0.50
	;발정기
	ELSEIF TALENT:발정기
		TIMES SOURCE:울굴 , 1.50
	ENDIF
	
	;陥落素質による処理
	IF TFLAG:20
		;연모かつ相손が主人
		IF TALENT:연모
			PRINTFORML %CALLNAME%は%CALLNAME:MASTER%に처녀を捧げられた事に幸福を感じているようだ…
			;반발각인の回避
			TFLAG:150 = 2
			TIMES SOURCE:애정 , 2.00
			TIMES SOURCE:반발 , 0.30
		;친애かつ相손が主人
		ELSEIF TALENT:친애
			PRINTFORML %CALLNAME%は%CALLNAME:MASTER%が初めての相손である事にこの上ない幸せを感じているようだ…
			;반발각인の回避
			TFLAG:150 = 2
			TIMES SOURCE:애정 , 2.00
			TIMES SOURCE:반발 , 0.10
		ENDIF
		
		;行為によって、애정경험を加算する
		SELECTCASE SELECTCOM
			;정상위
			CASE 100
				E = 10
			;対面位
			CASE 102, 104
				E = 8
			;それ以外
			CASEELSE
				E = 5
		ENDSELECT
		IF E
			PRINTFORML %EXPNAME:23%+{E}
			EXP:애정경험 += E
		ENDIF
	ENDIF
	
	;처녀喪失フラグ(비디오촬영)
	SIF TEQUIP:비디오카메라
		TFLAG:39 = 1
	
	;行為によって이상경험を加算する
	SELECTCASE SELECTCOM
		;바이브
		CASE 43
			PRINTFORML 玩具を使った처녀喪失で%EXPNAME:50%＋１
			EXP:이상경험 += 1
		;기승위、윤간、촉수삽입、촉수Ｖ拡張、촉수양구멍확장、촉수산란、수간、뱀장어탕、목조르기セックス、３Ｐ系
		CASE 106,322,344,366,368,369,381,382,513,515 TO 524
			PRINTFORML 異常な行為での처녀喪失で%EXPNAME:50%＋１
			EXP:이상경험 += 1
	ENDSELECT
	
	;異常な状態での처녀喪失で이상경험を加算する
	;처녀喪失を撮影された
	IF TFLAG:39
		PRINTFORML 비디오촬영中に처녀喪失で%EXPNAME:50%+1
		EXP:이상경험 += 1
	ENDIF
	
	;目・입・身体が全て拘束されているとき처녀喪失
	IF TEQUIP:아이마스크 && (TEQUIP:볼개그 || TEQUIP:촉수구욕) && TEQUIP:밧줄
		PRINTFORML 三点拘束中に처녀喪失で%EXPNAME:50%+1
		EXP:이상경험 += 1
	ENDIF
	
	;처녀喪失相손によって이상경험を入れる処理
	IF SELECTCOM == 405
		P = ASSI
	ELSEIF SELECTCOM >= 515 && SELECTCOM <= 525
		IF TFLAG:41 == 1
			P = ASSI
		ELSE
			P = MASTER
		ENDIF
	ELSE
		P = PLAYER
	ENDIF
	
	;へたくしょん用面識判定の更新
	;바이브、뱀장어탕、수간、촉수삽입、촉수拡張、촉수양구멍확장、촉수산란、윤간を抜かす
	IF SELECTCOM != 43 && SELECTCOM != 381 && SELECTCOM != 382 && SELECTCOM != 344 && SELECTCOM != 366 && SELECTCOM != 368 && SELECTCOM != 369 && SELECTCOM != 322
		;にょたちびサーチ
		R = NO:TARGET
		L = NO:P
		CALL GET_JIBUN
		IF A
			PRINTFORML %CALLNAME:P%相손に처녀喪失で%EXPNAME:50%+1
			EXP:이상경험 += 1
		ENDIF
		G = (2099 + NO:PLAYER)
		H = (2099 + NO:TARGET)
		CFLAG:PLAYER:H |= 64
		CFLAG:TARGET:G |= 128
	ENDIF
	;처녀喪失相손の記録
	;相손が主人
	IF TFLAG:20 || TFLAG:40 == 1
		CSTR:10 = %CALLNAME:MASTER%
	ELSEIF TFLAG:41 == 1 || SELECTCOM == 405
		CSTR:10 = %CALLNAME:ASSI%
	ELSE
		SELECTCASE SELECTCOM
			CASE 100 TO 106, 515 TO 524
				CSTR:10 = %CALLNAME:PLAYER%
			CASE 405,
				CSTR:10 = %CALLNAME:ASSI%
			CASEELSE
				CSTR:10 = %STR:0%
		ENDSELECT
	ENDIF
;奴隷の재생처녀喪失
ELSEIF TALENT:재생처녀 && TFLAG:19 == 1
