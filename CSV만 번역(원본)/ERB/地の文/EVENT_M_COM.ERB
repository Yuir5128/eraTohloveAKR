;260,정상위시킨다
;-------------------------------------------------
@MESSAGE_COM260
;前回と今回の調教者が同じで、前回も主人수Ｖセックス
IF ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)) && PREVCOM >= 260 && PREVCOM <= 262
	IF TALENT:새드 || TALENT:공 || TALENT:PLAYER:수 || TALENT:PLAYER:오메가 || (PENIS(TARGET) && ABL:욕망 >= 3) || TEQUIP:역조교
		PRINTFORM %CALLNAME%は
		;前回のコマンドが今回と同じ
		IF PREVCOM == 260
			;奴隷に本物の페니스がある
			IF PENIS(TARGET)
				PRINTFORM %CALLNAME:PLAYER%の질내の感触を味わいながら、再び腰を動かしてきた…
			;페니스밴드を使用している場合
			ELSE
				PRINTFORM %CALLNAME:PLAYER%に疑似페니스を挿入したまま、再び腰を動かしてきた…
			ENDIF
		;前回のコマンドが후배위시킨다、기승위한다
		ELSE
			PRINTFORM 질내から抜き去ることなく%CALLNAME:PLAYER%を仰向けに寝かせ、再び腰を動かしてきた…
		ENDIF
	ELSE
		IF PENIS(TARGET)
			PRINTFORM %CALLNAME%の
		ELSE
			PRINT 疑似
		ENDIF
		PRINTFORM 페니스が抜けないよう食い締めたまま
		;前回のコマンドが今回と同じ
		IF PREVCOM == 260
			PRINT 抱き付いて、
		ELSE
			PRINT 仰向けになり、
		ENDIF
		PRINTL さらなる突き上げを強請った…
	ENDIF
ELSE
	;역조교モード
	IF TEQUIP:역조교
		PRINTFORML %CALLNAME%は%CALLNAME:PLAYER%を組み敷くと、
	;通常モード
	ELSE
		IF TALENT:새드 || TALENT:공 || TALENT:PLAYER:수 || TALENT:PLAYER:오메가 || (PENIS(TARGET) && ABL:욕망 >= 3)
			PRINTFORML %CALLNAME:PLAYER%が寝そべり脚を開くと、%CALLNAME%は%CALLNAME:PLAYER%を組み敷き、
		ELSE
			PRINTFORM %CALLNAME:PLAYER%は%CALLNAME%の前に寝そべると
			SIF ABL:PLAYER:욕망 > 3
				PRINT 秘裂を見せ付けるように大きく
			PRINTFORML 股を開き、
		ENDIF
	ENDIF
	
	;스타킹系
	IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
		SIF TEQUIP:특수2 == 5
			PRINT 스타킹
		SIF TEQUIP:특수2 == 6
			PRINT 타이츠
		PRINT を破って取り出した
	ENDIF
	
	;奴隷に本物の페니스がある
	IF PENIS(TARGET)
		;아이사이즈
		IF TALENT:아이사이즈
			;소인체형で無い
			SIF !TALENT:소인체형
				PRINT 体に似合わず
			PRINT 幼い
		;말자지
		ELSEIF TALENT:말자지
			;소인체형
			IF TALENT:소인체형
				PRINT 身体が隠れてしまうほど
			ELSE
				PRINT 目を見張るほど
			ENDIF
			PRINT 大きな
		ENDIF
		SIF TALENT:포경
			PRINT 포경
		PRINT 페니스
	ELSE
		PRINT 疑似페니스
	ENDIF
	
	IF TALENT:새드 || TALENT:공 || TALENT:PLAYER:수 || TALENT:PLAYER:오메가 || (PENIS(TARGET) && ABL:욕망 >= 3) || TEQUIP:역조교
		PRINT で
	ELSE
		PRINT を
	ENDIF
	
	;처녀
	SIF TALENT:PLAYER:처녀
		PRINT 男を知らない
	;작은체구
	IF TALENT:PLAYER:작은체구 || TALENT:PLAYER:소인체형
		PRINT 幼いワレメ
	ELSE
		PRINT 바기나
	ENDIF
	
	IF TALENT:새드 || TALENT:공 || TALENT:PLAYER:수 || TALENT:PLAYER:오메가 || (PENIS(TARGET) && ABL:욕망 >= 3) || TEQUIP:역조교
		PRINTL を貫いてきた…
	ELSE
		PRINTFORM に수入れた…
	ENDIF
ENDIF


