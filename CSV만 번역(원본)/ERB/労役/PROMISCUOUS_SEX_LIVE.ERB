	PRINTFORML 고통の珠＋{D*30}
	JUEL:A:공포 += D*15
	PRINTFORML 공포の珠＋{D*15}
ENDIF
IF G > 0
	JUEL:A:불쾌 += G*50
	PRINTFORML 불쾌の珠＋{G*50}
ENDIF

IF D > 0 || G > 0 || E > 0 || F > 0
	JUEL:A:부정 += ((D+G+E+F)*100)/ABL:A:순종
	PRINTFORML 부정の珠＋{((D+G+E+F)*200)/ABL:A:순종}
ENDIF


;-----------------------------
;ストレス、禁欲ポイント
Q = 0
R = 0
;-----------------------------
;ストリップショー
IF B == 1
	BASE:A:체력 -= 300
	Q = 1
	R = 30
;乱交ライブ
ELSEIF B == 2
	BASE:A:체력 -= 500
	Q = 3
	R = 50
;ＳＭショー
ELSEIF B == 3
	BASE:A:체력 -= 800
	Q = 5
	R = 20
;拷問ショー
ELSEIF B == 4
	BASE:A:체력 -= 1000
	Q = 8
	R = 0
;수간ショー
ELSEIF B == 5
	BASE:A:체력 -= 1200
	Q = 10
	R = 10
ENDIF

;禁欲ポイント
IF TEQUIP:A:정조대
	CFLAG:A:602 += R
	SIF TALENT:PLAYER:천리안
		PRINTFORML 禁欲ポイント＋{R}
ELSE
	CFLAG:A:602 = 0
ENDIF

;ストレス値の付加
CFLAG:A:10 += Q
SIF TALENT:PLAYER:천리안
	PRINTFORML 疲労度＋{Q}


;-----------------------------
;報酬額の加算
;-----------------------------
;最후に金額の加算
PRINTFORMW ショーの収益として{S}고슈금を得ました
MONEY += S

DRAWLINE

;休憩フラグを立てる
FLAG:0 = 1
BEGIN TURNEND






;====================================================================
;プレイ内容の判定
;====================================================================
@SEX_LIVE_PLAY_CHECK

CFLAG:A:160 = 0
CFLAG:A:161 = 0
CFLAG:A:162 = 0
CFLAG:A:163 = 0
CFLAG:A:164 = 0
CFLAG:A:165 = 0
CFLAG:A:166 = 0
CFLAG:A:167 = 0
CFLAG:A:168 = 0
CFLAG:A:169 = 0
CFLAG:A:170 = 0
CFLAG:A:171 = 0
CFLAG:A:172 = 0
CFLAG:A:173 = 0
CFLAG:A:174 = 0
CFLAG:A:175 = 0
CFLAG:A:176 = 0
CFLAG:A:177 = 0
CFLAG:A:178 = 0
CFLAG:A:179 = 0

;奴隷の수(LOCAL:1)공(LOCAL:2)保存
IF B == 5
	LOCAL:1 = 1
ELSEIF B == 4
	IF ABL:A:새드끼 > ABL:A:마조끼
		LOCAL:2 = 1
	ELSEIF ABL:A:마조끼 > ABL:A:새드끼
		LOCAL:1 = 1
	ELSE
		IF !RAND:5
			LOCAL:2 = 1
		ELSE
			LOCAL:1 = 1
		ENDIF
	ENDIF
ELSE
	IF TALENT:A:공
		LOCAL:2 = 1
	ELSEIF TALENT:A:수 || TALENT:A:오메가
		LOCAL:1 = 1
	ELSE
		IF !RAND:5
			LOCAL:2 = 1
		ELSE
			LOCAL:1 = 1
		ENDIF
	ENDIF
ENDIF

;--------------------------------
;수간
;--------------------------------
SIF B == 5
	CFLAG:A:175 += 1

;--------------------------------
;자위
;--------------------------------
SIF B == 1 && (ABL:A:노출벽 >= 4 || ABL:A:자위중독 >= 2)
	CFLAG:A:162 += 1

;--------------------------------
;複数人プレイ
;--------------------------------
;奴隷수
IF B == 2 && LOCAL:1
	CFLAG:A:173 += 1
;奴隷공
ELSEIF B == 2 && LOCAL:2
	CFLAG:A:174 += 1
ENDIF

;--------------------------------
;ＳＭ系
;--------------------------------
;拷問ショーは絶対ハードSM
IF B == 4
	;奴隷Ｓ
	IF LOCAL:2
		CFLAG:A:172 += 1
	;奴隷Ｍ
	ELSE
		CFLAG:A:171 += 1
	ENDIF
;ＳＭショーはオプションと、奴隷Ｓの場合奴隷のＳっ気で変動
ELSEIF B == 3
	;オプションがなければ絶対ソフト
	IF (FLAG:160 & 8) == 0
		IF LOCAL:2
			CFLAG:A:170 += 1
		ELSE
			CFLAG:A:169 += 1
		ENDIF
	;奴隷Ｓのとき
	ELSEIF LOCAL:2
		;새드끼４以上あるとハードに
		IF ABL:A:새드끼 >= 4
			CFLAG:A:172 += 1
		ELSE
			CFLAG:A:170 += 1
		ENDIF
	;奴隷Ｍのとき
	ELSE
		;마조끼４以上ある、もしくはランダム1/5でハードに
		IF ABL:A:마조끼 >= 4 || !RAND:4
			CFLAG:A:171 += 1
		ELSE
			CFLAG:A:169 += 1
		ENDIF
	ENDIF
ENDIF

;--------------------------------
;スカ系
;--------------------------------
;食糞プレイはハードSMで、奴隷Ｍで、オプションあるときだけランダム
SIF CFLAG:A:171 && FLAG:160 & 512 && !RAND:3
	CFLAG:A:179 += 1

;관장プレイはハードSM系なら絶対ある。ソフトだとランダム
SIF (B == 4 && !RAND:3) || (B == 3 && !RAND:5)
	CFLAG:A:178 += 1

;飲尿プレイはハードSMで、奴隷Ｍで、オプションあるときだけ
SIF CFLAG:A:171 && FLAG:160 & 128 && !RAND:3
	CFLAG:A:177 += 1

;放尿鑑賞はソフトSMのときだけランダム（관장があれば絶対）
SIF CFLAG:A:178 || (B == 3 && !RAND:3)
	CFLAG:A:176 += 1


;--------------------------------
;セックス系
;--------------------------------
;乱交ライブ、ＳＭショー、수간ショー
IF B == 2 || B == 3 || B == 5
	;奴隷공
	IF LOCAL:2
		;奴隷동정でオプションがないと駄目
		IF TALENT:A:동정 && (FLAG:160 & 4) == 0
		ELSE
			CFLAG:A:165 += 1
			;-----------------------
			;동정食い
			;-----------------------
			SIF TALENT:A:동정 && (FLAG:160 & 4)
				CFLAG:A:168 += 1
		ENDIF
	;奴隷수
	ELSE
		;바기나ない、もしくは奴隷처녀でオプションがないと駄目
		IF !VAGINA(A) || ((TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1) == 0)
		ELSE
			CFLAG:A:163 += 1
			;-----------------------
			;처녀姦通
			;-----------------------
			SIF (TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1)
				CFLAG:A:166 += 1
		ENDIF
		
		;奴隷처녀でオプションがないと駄目
		;女だとランダム3/4だが、관장があった場合は本番有り
		IF (TALENT:A:백버진 && (FLAG:160 & 2) == 0) || (!CFLAG:A:178 && (VAGINA(A) && RAND:3 > 0))
		ELSE
			CFLAG:A:164 += 1
			;-----------------------
			;백버진姦通
			;-----------------------
			SIF TALENT:A:백버진 && (FLAG:160 & 2)
				CFLAG:A:167 += 1
		ENDIF
	ENDIF
ENDIF

;--------------------------------
;性的奉仕は乱交ライブ、ＳＭショー、수간ショーなら絶対ある
;--------------------------------
IF B == 2 || B == 3 || B == 5
	;奴隷공
	IF LOCAL:2
		CFLAG:A:161 += 1
	;奴隷수
	ELSE
		CFLAG:A:160 += 1
	ENDIF
ENDIF



