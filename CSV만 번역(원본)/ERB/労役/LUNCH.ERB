		PRINTFORML 
		WAIT
		;욕망
		SIF ABL:욕망
			S += ABL:욕망
		;자위중독
		SIF ABL:자위중독
			S += ABL:자위중독
		;음란
		SIF TALENT:음란 || TALENT:창부
			S += 1
		;자제심
		SIF TALENT:자제심
			S -= 2
		;快楽の부정
		SIF TALENT:쾌감을부정
			S -= 3
		;반발각인
		SIF MARK:반발각인
			S -= MARK:반발각인
		S /= 2
		S -= 3
		SIF TFLAG:14 == 2
			S += 5
		IF S > 0
			PRINTFORM 部屋に一人残された%CALLNAME%は堪え切れず
			IF TEQUIP:속옷（아래） == 5
				PRINTL 자위をしようとしたが、
				PRINTFORML 下半身を覆う정조대に阻まれ、苦しい思いをしたようだ…
			ELSE
				PRINT 、
				PRINTV S
				PRINTL 回자위をしたようだ…
				PRINTFORML %EXPNAME:10%＋{S}
				PRINTFORML %PALAMNAME:0%の珠＋{S*100}
				PRINTFORML %PALAMNAME:5%の珠＋{S*50}
				PRINTFORMW %PALAMNAME:6%の珠＋{S*100}
				EXP:자위경험 += S
				JUEL:쾌C += S*100
				JUEL:순종 += S*50
				JUEL:욕정 += S*100
			ENDIF
			SIF TEQUIP:속옷（아래） == 5
				CFLAG:602 += 5
		ENDIF
	;-------------------------------------------------
	;押し倒す
	;-------------------------------------------------
	ELSEIF RESULT == 1
		SIF TFLAG:14 == 2
			S += 2
		K = 4
		;イベント입上呼び出し
		TFLAG:13 = 26
		CALL KOJO_MESSAGE_EVENT
		PRINTL 
		WAIT
		TFLAG:130 = 4
		;정조대
		IF TEQUIP:속옷（아래） != 5
			PRINTFORML %EXPNAME:4%＋{S}
			EXP:성교경험 += S
			EXP:MASTER:삽입경험 += S
		ENDIF
		;상사상애・복종を持っていた場合봉사쾌락경험が入る
		IF TFLAG:14 == 2
			PRINTFORML %EXPNAME:21% ＋2
			EXP:봉사쾌락경험 += 2
		ENDIF
		PRINTFORML %PALAMNAME:5%の珠＋{S*100}
		PRINTFORML %PALAMNAME:6%の珠＋{S*300}
		JUEL:순종 += S*100
		JUEL:욕정 += S*300
		;처녀のとき
		IF TALENT:처녀 && TEQUIP:속옷（아래） != 5
			JUEL:순종 += S*250
			PRINTFORML %PALAMNAME:5%の珠＋{S*250}
			PRINTL 
			PRINTL 
