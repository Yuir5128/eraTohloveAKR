		BASE:MASTER:체력 /= 2
		BASE:MASTER:체력 -= 75 * (RAND:5 + 1)
	ELSE
		BASE:MASTER:체력 -= 75 * (RAND:5 + 1)
	ENDIF
;-------------------------------------------------
;毒を見抜く
;-------------------------------------------------
ELSE
	PRINTFORML 　　　･
	PRINTFORML 　　　･
	PRINTFORML 　　　･
	PRINTFORML 　　　！
	PRINTFORMW %NAME:MASTER%は出された食べ物に毒が盛られていることを見抜いた
	;약점잡힘を握るイベント発生判定用
	B = 0
	C = 0
	REPEAT CHARANUM
		;MASTERは除外
		SIF COUNT == MASTER
			CONTINUE
		;労役中などこの場にいないなら除外
		SIF CFLAG:COUNT:12 >= 1
			CONTINUE
		;助손可能、순종Lv4以上、굴복각인Lv3、약점잡힘、연모系素質のどれかを持っている必要がある
		SIF ABL:COUNT:순종 < 4 && MARK:COUNT:굴복각인 < 3 && TALENT:COUNT:약점잡힘 == 0 && TALENT:COUNT:질투 == 0 && TALENT:COUNT:맹신 == 0
			CONTINUE
		;上の条件を満たすキャラを奴隷の中からピックアップし、調教対象との相性を見比べる
		;ここで調教対象と相性のいいキャラがピックアップされ、2/3の確率で
		;(鬼畜な主人によって)調教対象の毒入り料理を食べさせるように仕向けられる
		A = NO:COUNT
		SIF RELATION:A < 100
			CONTINUE
		IF RELATION:A > RELATION:TARGET && (C == 0 || RAND:3 > 0)
			B = NO:COUNT
			C = COUNT
		ENDIF
	REND
	IF C > 0 && RAND:3 > 0
		K = 6
		TFLAG:13 = 26
		CALL KOJO_MESSAGE_EVENT

		M += 12
		IF TALENT:약점잡힘 == 0
			IF RAND:3 == 0
				PRINTFORMW %NAME:TARGET%の【약점잡힘】を握ることに成功した
				TALENT:약점잡힘 = 1
				TIMES M , 2.00
			ENDIF
		ENDIF
		JUEL:욕정 += M*8
		PRINTFORML %PALAMNAME:7%の珠＋{M*8}
		SIF TALENT:겁쟁이
			M *= 2
		SIF TALENT:낙관적
			M -= 5
		SIF TALENT:비관적
			M += 10
		SIF TALENT:감정부족
			M /= 2
		SIF M <= 0
			M = 0
		IF M > 0
			PRINTFORML %PALAMNAME:100%の珠＋{M*30}
			JUEL:부정 += M*30
		ENDIF
	ELSE
		K = 7
		TFLAG:13 = 26
		CALL KOJO_MESSAGE_EVENT

		JUEL:욕정 += M*5
		PRINTFORML %PALAMNAME:7%の珠＋{M*5}
		SIF TALENT:겁쟁이
			M *= 2
		SIF TALENT:낙관적
			M -= 5
		SIF TALENT:비관적
			M += 10
		SIF TALENT:감정부족
			M /= 2
		SIF M <= 0
			M = 0
		IF M > 0
			PRINTFORML %PALAMNAME:100%の珠＋{M*20}
			JUEL:부정 += M*20
		ENDIF
	ENDIF
ENDIF


