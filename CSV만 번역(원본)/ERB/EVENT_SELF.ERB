;SELF.ERB、奴隷からの奉仕などのイベント
;-------------------------------------------------
;朝に起こるイベント
;-------------------------------------------------
@EVENT_NEWDAY_SELF
;調教対象が居ない場合は終了
SIF TARGET < 0
	RETURN 0
;全体判定、陥落素質が無い場合もしくは반발각인があるときは関数終了
SIF (TALENT:연모 == 0 && TALENT:음란 == 0 && TALENT:복종 == 0 && TALENT:친애 == 0 && TALENT:창부 == 0 && TALENT:예속 == 0) || MARK:반발각인
	RETURN 0
;기절していたら関数終了
SIF TALENT:기절
	RETURN 0

;朝イベントキャンセルフラグが立ってる場合は終了
SIF CFLAG:42
	RETURN 0

VARSET LOCAL
LOCAL:1 = 10
;イベント決定
FOR LOCAL, 1, 10
	CALLFORM SELF_MORNING_ABLE{LOCAL}
	IF RESULT
		LOCAL:(LOCAL:1) = LOCAL
		LOCAL:1 += 1
	ENDIF
NEXT
LOCAL:1 -= 10
SIF LOCAL:1 == 0
	RETURN 0
LOCAL:2 = LOCAL:(RAND:(LOCAL:1) + 10)
IF LOCAL:2 == 7 || LOCAL:2 == 8
	CALLFORM MORNING_EVENT_{LOCAL:2}
ELSE
	CALLFORM SELF_MORNING_{LOCAL:2}
ENDIF

;-------------------------------------------------
;朝フェラ
;-------------------------------------------------
@SELF_MORNING_ABLE1
SIF !PENIS(MASTER)
	RETURN 0
SIF !ABL:정액중독
	RETURN 0
RETURN 1

;-------------------------------------------------
;朝파이즈리
;-----------------------------------------------
@SELF_MORNING_ABLE2
SIF !PENIS(MASTER)
	RETURN 0
SIF !BREAST(TARGET)
	RETURN 0
SIF ABL:봉사정신 < 5
	RETURN 0
SIF !ABL:정액중독
	RETURN 0
RETURN 1

;-------------------------------------------------
;朝기승위
;-----------------------------------------------
@SELF_MORNING_ABLE3
SIF !PENIS(MASTER)
	RETURN 0
SIF TALENT:MASTER:동정
	RETURN 0
SIF (VAGINA(TARGET) && (TALENT:처녀 || TALENT:재생처녀) && TALENT:백버진) || (!VAGINA(TARGET) && TALENT:백버진)
	RETURN 0
SIF ABL:봉사정신 < 7 && !TALENT:음란 && !TALENT:창부
	RETURN 0
SIF ((VAGINA(TARGET) && !TALENT:처녀 && !TALENT:재생처녀 && ABL:V감각 < 3) || ((!VAGINA(TARGET) || (VAGINA(TARGET) && (TALENT:처녀 || TALENT:재생처녀))) && ABL:A감각 < 3))
	RETURN 0
SIF TALENT:MASTER:수 || TALENT:MASTER:오메가 || TALENT:공
	RETURN 0
SIF !ABL:섹스중독
	RETURN 0
RETURN 1

;-------------------------------------------------
;朝料理
;-----------------------------------------------
@SELF_MORNING_ABLE4
SIF !TALENT:연모 && !TALENT:친애
	RETURN 0
SIF (!ABL:요리기능 && RAND:2) || (ABL:요리기능 < 3 && !RAND:3)
	RETURN 0
RETURN 1

