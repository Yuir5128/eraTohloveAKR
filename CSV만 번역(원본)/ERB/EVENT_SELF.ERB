	RETURN 0
SIF ABL:순종 < 5
	RETURN 0
RETURN 1

;-------------------------------------------------
;３Ｐ
;-----------------------------------------------
@SELF_MORNING_ABLE8
SIF ASSI < 0
	RETURN 0
SIF TEQUIP:속옷（아래） == 5 || TEQUIP:ASSI:속옷（아래） == 5
	RETURN 0
SIF !ABL:섹스중독 && !ABL:ASSI:섹스중독 && ABL:욕망 < 5 && ABL:ASSI:욕망 < 5
	RETURN 0
SIF (MALE(TARGET) && MALE(ASSI) && MALE(MASTER) && ABL:게이끼 && ABL:ASSI:게이끼 && ABL:MASTER:게이끼) || (!MALE(TARGET) && !MALE(ASSI) && !MALE(MASTER) && ABL:레즈끼 && ABL:ASSI:레즈끼 && ABL:MASTER:레즈끼)
	RETURN 1

;-------------------------------------------------
;飲尿
;-----------------------------------------------
@SELF_MORNING_ABLE9
SIF !PENIS(MASTER)
	RETURN 0
SIF EXP:음뇨경험 < 30
	RETURN 0
SIF ABL:마조끼 < 3
	RETURN 0
SIF ABL:스캇중독 < 2
	RETURN 0
RETURN 1


;-----------------------------------------------
;朝フェラ
;-----------------------------------------------
@SELF_MORNING_1
DRAWLINE
A = 0
TFLAG:13 = 1
CALL KOJO_MESSAGE_EVENT

;정액중독
IF ABL:정액중독 == 0
	A = 1
ELSEIF ABL:정액중독 == 1
	A = 2
ELSEIF ABL:정액중독 == 2
	A = 3
ELSEIF ABL:정액중독 == 3
	A = 4
ELSEIF ABL:정액중독 == 4
	A = 5
ELSEIF ABL:정액중독 >= 5
	A = 6
ENDIF

;악취둔감　더러움무시
SIF TALENT:악취둔감 || TALENT:더러움무시
	A += 1
;악취민감
SIF TALENT:악취민감
	A -= 1
;헌신적
SIF TALENT:헌신적
	A += 1
;발정기
SIF TALENT:발정기
	A += 1
;パブロフカウンター
CALL SEIIN_CHECK
;精液절정경험
SIF EXP:정음절정경험 > P
	A += 3

SIF A == 0
	A = 1

EXP:펠라경험 += A
;아이사이즈で無い
SIF TALENT:MASTER:아이사이즈 == 0
	EXP:정액경험 += A/2
JUEL:순종 += A*100
JUEL:굴복 += A*30
JUEL:습득 += A*40
PRINTFORML %EXPNAME:22%＋{A}
;아이사이즈で無い
SIF TALENT:MASTER:아이사이즈 == 0
	PRINTFORML %EXPNAME:20%＋{A/2}
;精液절정경험
IF EXP:정음절정경험 > P
	PRINTFORML %EXPNAME:24%＋1
	EXP:정음절정경험 += 1
ENDIF
PRINTFORML %PALAMNAME:5%の珠＋{A*100}
PRINTFORML %PALAMNAME:7%の珠＋{A*30}
PRINTFORMW %PALAMNAME:8%の珠＋{A*40}
DRAWLINE

RETURN 0


;-----------------------------------------------
;朝파이즈리
;-----------------------------------------------
@SELF_MORNING_2
DRAWLINE
A = 0
TFLAG:13 = 2
CALL KOJO_MESSAGE_EVENT

;정액중독
IF ABL:정액중독 == 0
	A = 1
ELSEIF ABL:정액중독 == 1
	A = 2
ELSEIF ABL:정액중독 == 2
	A = 3
ELSEIF ABL:정액중독 == 3
	A = 4
ELSEIF ABL:정액중독 == 4
	A = 5
ELSEIF ABL:정액중독 >= 5
	A = 6
ENDIF

;악취둔감　더러움무시
SIF TALENT:악취둔감 || TALENT:더러움무시
	A += 1
;악취민감
SIF TALENT:악취민감
	A -= 1
;헌신적
SIF TALENT:헌신적
	A += 1
;パブロフカウンター
CALL SEIIN_CHECK
;精液절정경험
SIF EXP:정음절정경험 > P
	A += 1

SIF A == 0
	A = 1

EXP:펠라경험 += A
;아이사이즈で無い
SIF TALENT:MASTER:아이사이즈 == 0
	EXP:정액경험 += A/2
JUEL:쾌B += A*20
JUEL:순종 += A*100
JUEL:굴복 += A*30
JUEL:습득 += A*40
PRINTFORML %EXPNAME:22%＋{A}
;아이사이즈で無い
SIF TALENT:MASTER:아이사이즈 == 0
	PRINTFORML %EXPNAME:20%＋{A/2}
;精液절정경험
IF EXP:정음절정경험 > P
	PRINTFORML %EXPNAME:24%＋1
	EXP:정음절정경험 += 1
ENDIF
PRINTFORML %PALAMNAME:3%の珠＋{A*20}
PRINTFORML %PALAMNAME:5%の珠＋{A*100}
PRINTFORML %PALAMNAME:7%の珠＋{A*30}
PRINTFORMW %PALAMNAME:8%の珠＋{A*40}
DRAWLINE

RETURN 0


;-----------------------------------------------
;朝기승위
;-----------------------------------------------
@SELF_MORNING_3
DRAWLINE
A = 0
TFLAG:13 = 3

CALL KOJO_MESSAGE_EVENT

;元が男、もしくはＶよりＡが高ければＡ優先
IF MALE2(TARGET) || ABL:V감각 < ABL:A감각
	;ただし、Ａを使えない場合はＶの使用可否を見る
	IF ABL:A감각 < 3 || TALENT:백버진
		;Ｖが使えればＶを使おうとする
		IF VAGINA(TARGET) && !TALENT:처녀 && !TALENT:재생처녀 && ABL:V감각 >= 3
			;その上でＶも정조대で埋まってたら정조대입上を呼びだす
			IF TEQUIP:속옷（아래） == 5
				TFLAG:67 = 0
			;埋まってなければＶを使う
			ELSE
				TFLAG:67 = 1
			ENDIF
		ENDIF
	;Ａが使えれば정조대関係なくＡを使う
	ELSE
		TFLAG:67 = 2
	ENDIF
;元が女ならＶ優先
ELSE
	;정조대がある場合、Ａの使用可否を見る
	IF TEQUIP:속옷（아래） == 5
		;Ａも使えなければ、정조대입上を呼びだす
		IF ABL:A감각 < 3 || TALENT:백버진
			TFLAG:67 = 0
		;Ａが使えればＡを使う
		ELSE
			TFLAG:67 = 2
		ENDIF
	;もともとＶが使えない場合はＡの使用可否を見る
	ELSEIF TALENT:처녀 || TALENT:재생처녀 || ABL:V감각 < 3
		;Ａが使えればＡを使う
		SIF ABL:A감각 >= 3 && !TALENT:백버진
			TFLAG:67 = 2
	;Ｖが使えるのでＶを使う
	ELSE
		TFLAG:67 = 1
	ENDIF
ENDIF

;정조대の場合、これ以降の部分は読まない
SIF TFLAG:67 == 0
	RETURN 0

;정액중독
IF ABL:정액중독 == 0
	A = 1
ELSEIF ABL:정액중독 == 1
	A = 1
ELSEIF ABL:정액중독 == 2
	A = 2
ELSEIF ABL:정액중독 == 3
	A = 2
ELSEIF ABL:정액중독 == 4
	A = 3
ELSEIF ABL:정액중독 >= 5
	A = 3
ENDIF

;섹스중독
IF ABL:섹스중독 == 0
	A += 1
ELSEIF ABL:섹스중독 == 1
	A += 2
ELSEIF ABL:섹스중독 == 2
	A += 3
ELSEIF ABL:섹스중독 == 3
	A += 4
ELSEIF ABL:섹스중독 == 4
	A += 5
ELSEIF ABL:섹스중독 >= 5
	A += 6
ENDIF

;음란구멍
SIF TALENT:음란구멍
	A += 2
;항문광
SIF TALENT:항문광
	A += 2
;섹스광
SIF TALENT:섹스광
	A += 3
;헌신적
SIF TALENT:헌신적
	A += 1
;발정기
SIF TALENT:발정기
	A += 1

SIF A == 0
	A = 1

;Ａ使用の場合
IF TFLAG:67 == 2
	EXP:A경험 += A
	EXP:성교경험 += A
	;아이사이즈で無い
	SIF TALENT:MASTER:아이사이즈 == 0
		EXP:정액경험 += A/2
	IF CFLAG:108 == 0
		EXP:장내사정경험 += A/2
		CFLAG:702 += A/2
	ENDIF
	JUEL:쾌A += A*100
	JUEL:순종 += A*100
	JUEL:굴복 += A*30
	JUEL:습득 += A*40
	PRINTFORML %EXPNAME:1%＋{A}
	PRINTFORML %EXPNAME:4%＋{A}
	;아이사이즈で無い
	SIF TALENT:MASTER:아이사이즈 == 0
		PRINTFORML %EXPNAME:20%＋{A/2}
	SIF CFLAG:702 > 0 && CFLAG:108 == 0
		PRINTFORML %EXPNAME:14%+{A/2}
	PRINTFORML %PALAMNAME:2%の珠＋{A*20}
	PRINTFORML %PALAMNAME:5%の珠＋{A*100}
	PRINTFORML %PALAMNAME:7%の珠＋{A*30}
	PRINTFORMW %PALAMNAME:8%の珠＋{A*40}
	DRAWLINE
ELSEIF TFLAG:67 == 1
	EXP:V경험 += A
	EXP:성교경험 += A
	;아이사이즈で無い
	SIF TALENT:MASTER:아이사이즈 == 0
		EXP:정액경험 += A/2
	IF CFLAG:108 == 0
		EXP:질내사정경험 += A/2
		CFLAG:102 += A/2
	ENDIF
	JUEL:쾌V += A*100
	JUEL:순종 += A*100
	JUEL:굴복 += A*30
	JUEL:습득 += A*40
	PRINTFORML %EXPNAME:0%＋{A}
	PRINTFORML %EXPNAME:4%＋{A}
	;아이사이즈で無い
	SIF TALENT:MASTER:아이사이즈 == 0
		PRINTFORML %EXPNAME:20%＋{A/2}
	SIF CFLAG:102 > 0 && CFLAG:108 == 0
		PRINTFORML %EXPNAME:5%＋{A/2}
	PRINTFORML %PALAMNAME:1%の珠＋{A*20}
	PRINTFORML %PALAMNAME:5%の珠＋{A*100}
	PRINTFORML %PALAMNAME:7%の珠＋{A*30}
	PRINTFORMW %PALAMNAME:8%の珠＋{A*40}
	DRAWLINE
ENDIF

EXP:MASTER:성교경험 += A
EXP:MASTER:삽입경험 += A


RETURN 0

;-----------------------------------------------
;朝料理
;-----------------------------------------------
@SELF_MORNING_4
DRAWLINE
A = 0
TFLAG:13 = 23
CALL KOJO_MESSAGE_EVENT

;요리기능
IF ABL:요리기능 == 0
	A = 1
ELSEIF ABL:요리기능 == 1
	A = 2
ELSEIF ABL:요리기능 == 2
	A = 3
ELSEIF ABL:요리기능 == 3
	A = 4
ELSEIF ABL:요리기능 == 4
	A = 5
ELSEIF ABL:요리기능 >= 5
	A = 6
ENDIF

;헌신적
SIF TALENT:헌신적
	A += 1

SIF A == 0
	A = 1

EXP:애정경험 += A
EXP:가사경험 += A
JUEL:순종 += A*100
JUEL:습득 += A*100

PRINTFORML %EXPNAME:23%＋{A}
PRINTFORML %EXPNAME:61%＋{A}
PRINTFORML %PALAMNAME:5%の珠＋{A*100}
PRINTFORML %PALAMNAME:8%の珠＋{A*100}

	PRINTFORMW 

DRAWLINE

RETURN 0

;-----------------------------------------------
;朝풋잡
;-----------------------------------------------
@SELF_MORNING_5
DRAWLINE
A = 0

;새드끼で加算Lv3で1
;そもそもココに来る時点で3あるので最低1
A += (ABL:새드끼 - 2)
;도착적
SIF TALENT:도착적
	A += 1
;새드
SIF TALENT:새드
	A += 2
;소악마
SIF TALENT:소악마
	A += 1
;主人が【마조】
SIF  TALENT:MASTER:마조 == 1
	A += 5
;연모
SIF TALENT:연모 || TALENT:친애
	A += 1
;음란
SIF TALENT:음란
	A += 2
;창부
SIF TALENT:창부
	A += 4
;반발は3あるとダメ
SIF MARK:반발각인 >= 3
	RETURN 0
;条件を満たしていない
SIF A < 1
	RETURN 0

DRAWLINE


;입上の呼び出し
TFLAG:13 = 30
CALL KOJO_MESSAGE_EVENT

;남자の場合
IF MALE(TARGET)
	EXP:가학쾌락경험 += A
	PRINTFORML %CALLNAME%の%EXPNAME:32%＋{A}
	;主人も男なら、게이경험加算
	IF MALE(TARGET) && MALE(MASTER)
		EXP:게이경험 += A
		PRINTFORML %EXPNAME:41%＋{A}
	ENDIF
	;主人が아이사이즈でないとき、調教対象に정액경험、主人に사정경험追加
	;主人に고통쾌락경험追加
	;습득、욕정の珠獲得
	;아이사이즈で無い
	IF TALENT:MASTER:아이사이즈 == 0
		EXP:정액경험 += A/2
		EXP:MASTER:사정경험 += A/2
		;禁欲ポイント減らす
		CFLAG:MASTER:602 = 0
		PRINTFORML %CALLNAME%の%EXPNAME:20%＋{A/2}
		PRINTFORML %CALLNAME:MASTER%の%EXPNAME:3%＋{A/2}
	ENDIF
	EXP:MASTER:고통쾌락경험 += A/4
	JUEL:욕정 += A*20
	JUEL:습득 += A*70
	PRINTFORML %CALLNAME:MASTER%の%EXPNAME:30%＋{A/4}
	PRINTFORML %PALAMNAME:6%の珠＋{A*20}
	PRINTFORML %PALAMNAME:8%の珠＋{A*70}
	DRAWLINE
ELSE
	EXP:가학쾌락경험 += A
	PRINTFORML %CALLNAME%の%EXPNAME:32%＋{A}
	;主人も女なら、레즈경험加算
	IF  MALE(TARGET) == 0 && MALE(MASTER) == 0
		EXP:레즈경험 += A
		PRINTFORML %EXPNAME:40%＋{A}
	ENDIF
	;主人に고통쾌락경험追加
	;습득、욕정の珠獲得
	EXP:MASTER:고통쾌락경험 += A/4
	JUEL:욕정 += A*20
	JUEL:습득 += A*70
	PRINTFORML %CALLNAME:MASTER%の%EXPNAME:30%＋{A/4}
	PRINTFORML %PALAMNAME:6%の珠＋{A*20}
	PRINTFORML %PALAMNAME:8%の珠＋{A*70}
	DRAWLINE
ENDIF
RETURN 0


;-----------------------------------------------
;モーニングキス
;-----------------------------------------------
@SELF_MORNING_6
DRAWLINE
A = 0

;순종＋봉사정신
IF ABL:순종 + ABL:봉사정신 < 7
	A = 1
ELSEIF ABL:순종 + ABL:봉사정신 < 9
	A = 2
ELSE
	A = 4
ENDIF

;헌신적
SIF TALENT:헌신적
	A += 1
;친애
SIF TALENT:친애
	A += 2
;상사상애
SIF TALENT:상사상애
	A += 4
;반발각인があるとダメ
SIF MARK:반발각인 > 0
	RETURN 0
;条件を満たしていない
SIF A < 1
	RETURN 0

DRAWLINE

;입上呼び出し
TFLAG:13 = 31
CALL KOJO_MESSAGE_EVENT

;애정경험、습득の珠、순종の珠加算。調教対象と主人が男同士or女同士ならゲイor레즈경험も。
EXP:애정경험 += A
PRINTFORML %EXPNAME:23%＋{A}

IF MALE(TARGET) && MALE(MASTER)
	EXP:게이경험 += A
	PRINTFORML %EXPNAME:41%＋{A}
ELSEIF MALE(TARGET) == 0 && MALE(MASTER) == 0
	EXP:레즈경험 += A
	PRINTFORML %EXPNAME:40%＋{A}
ENDIF

JUEL:순종 += A*100
JUEL:습득 += A*100
PRINTFORML %PALAMNAME:5%の珠＋{A*100}
PRINTFORML %PALAMNAME:8%の珠＋{A*100}
RETURN 0


;-----------------------------------------------
;朝風呂
;-----------------------------------------------
@MORNING_EVENT_7
DRAWLINE
A = 0
;순종＋봉사정신
IF ABL:순종 + ABL:봉사정신 < 7
	A = 1
ELSEIF ABL:순종 + ABL:봉사정신 < 9
	A = 2
ELSE
	A = 4
ENDIF

;헌신적
SIF TALENT:헌신적
	A += 1
;복종
SIF TALENT:복종
	A += 1
;예속
