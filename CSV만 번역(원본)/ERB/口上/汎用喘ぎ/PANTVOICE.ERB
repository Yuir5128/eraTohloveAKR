;----------------------------------------------------------------------
;汎用喘ぎ別人版ERB
;ver0.02(10/11/28)

;>>118さん版または別人版0.01verでは「PANT_E |= 2とするとみさくら語が表示されなくなる」と説明していましたが、実際にはPANT_E |= 2とするとみさくら語が表示されるようになってました
;以前のバージョンを使っている方でみさくら語を禁止したい方はPANT_E |= 2を消してください。
;絶叫系喘ぎとみさくら語を禁止してた方はPANT_E |= 3をPANT_E |= 1にしてください。
;----------------------------------------------------------------------
;【使い方】
;まずキャラ입上ERBのどこかに以下の文を入れる。
;変数に入れる数字については下記参照。
;------------ここから-------------
;@PANT_VOICE_KX1
;	PANT_A = (1～5の数字)
;	PANT_E = (0～7の数字／特に付けたいオプションがなければ0でよい)
;CALL PANT_VOICE
;RETURN 0
;------------ここまで-------------
;X1にはキャラ番号を入れる。
;あとは喘がせたい場所で
;------------ここから-------------
;CALL PANT_VOICE_KX1
;------------ここまで-------------
;と書くと喘ぎます。
;回数または喘ぎパターンを指定したい場合は
;------------ここから-------------
;PANT_F = (1～10の数字)
;PANT_G = (1～7の数字)
;CALL PANT_VOICE_KX1
;------------ここまで-------------
;と書くと、指定した喘ぎパターンでPANT_F回喘ぎを反復します。
;いちいちPANT_F=なんちゃらとか書くのめんどくさいという方は
;------------ここから-------------
;@PANTVOICE_1_KX1
;	PANT_A = (1～5の数字)
;	PANT_E = (0～7の数字／特に付けたいオプションがなければ0でよい)
;	PANT_F = (1～10の数字)
;	PANT_G = (1～7の数字)
;CALL PANT_VOICE
;RETURN 0
;------------ここまで-------------
;と書いてCALL PANTVOICE_1_KX1とかすると良いと思います。
;記述例はreadme別人版に書いてあるのでそちらも参照ください。

;----------------------------------------------------------------------
;変数使用状況
;PANT_A = キャラ입調（現在5種）
;PANT_B = 喘ぎ回数の多さ
;PANT_C = 感度
;PANT_D = 分岐用ビット
;PANT_E = オプション追加
;PANT_F = 喘ぎ回数指定用
;PANT_G = 喘ぎの種類指定用
;PANT_P = ランダム用

;----------------------------------------------------------------------
;変数初期化
;----------------------------------------------------------------------
@PANT_VOICE
PANT_B = 0
PANT_C = 0
PANT_D = 0
PANT_P:1 = (-1)
PANT_B_CALC = 0
PANT_C_CALC = 0

;----------------------------------------------------------------------
;変数と対応数字一覧
;----------------------------------------------------------------------
;PANT_Aに入れるもの
;乱暴=1
;標準=2
;柔らかめ=3
;敬語=4
;関西弁=5

;PANT_Bに入ってるもの
;PANT_F=0のときの喘ぎ反復回数

;PANT_Cに入ってるもの
;PANT_G=0のときの喘ぎパターンを決定する数値

;PANT_Dに入ってるもの
;붕괴=1=0
;반발1=2=1
;반발2=4=2
;반발3=8=3
;굴복1=16=4
;굴복2=32=5
;굴복3=64=6
;연모=128=7
;음란=256=8
;복종=512=9
;친애=1024=10
;창부=2048=11
;예속=4096=12
;입=8192=13
;B=16384=14
;C=32768=15
;A=65536=16
;V=131072=17
;その他=262144=18
;本番=524288=19
;촉수=1048576=20
;道具=2097152=21
;고통1=4194304=22
;고통2=8388608=23
;奉仕系=16777216=24
;奴隷性別男=33554432=25
;慣らさず濡らさず突っこんだ==26

;PANT_Eに入れるもの
;絶叫系喘ぎ禁止=1
;(↑を入れると고통LV2になる状況でも고통LV1の喘ぎが、快楽LV4になる状況でも快楽LV3の喘ぎが出るようになる)
;みさくら語で喘ぐ(現状らめぇくらいしかセリフが増えない)=2
;淫語で喘ぐ(ちんぽ汁出るうううとか)=4
;淫語で喘ぐモードは一つひとつのセリフが長めなので、PANT_F = 4以下くらいで喘ぎ回数を指定しておいたほうがいいかもしれません。
;PANT_E = 0 :オプションなし
;PANT_E = 1 :絶叫系喘ぎ禁止
;PANT_E = 2 :みさくら語モード
;PANT_E = 3 :絶叫系喘ぎ禁止＋みさくら語モード
;PANT_E = 4 :淫語モード
;PANT_E = 5 :絶叫系喘ぎ禁止＋淫語モード
;PANT_E = 6 :みさくら語モード＋淫語モード
;PANT_E = 7 :絶叫系喘ぎ禁止＋みさくら語モード＋淫語モード

;PANT_Fに入れるもの
;1～10までの数字
;入れた数字の分だけ喘ぎを反復するようになる
;0または負の数を入れるとPANT_Fに何も入れなかったときと同じ動きをし、
;10より大きい数字を入れると喘ぎ反復回数は10回になる

;PANT_Gに入れるもの
;1～7までの数字
;入れた数字に対応する喘ぎパターンで喘ぐようになる
;1～7以外の数字を入れると何も入れなかったのと同じ動きをする
;弱快感=1
;快感LV2=2
;快感LV3=3
;快感LV4=4
;고통LV1=5
;고통LV2=6
;입が塞がってる(볼개그・촉수입虐中以外)=7

;----------------------------------------------------------------------
;素質によってB喘ぐ回数とC感度にプラスマイナス。
;上がり下がりは自分の想像です。
;感度や고통に関わる素質は最후に。
;----------------------------------------------------------------------
;男である
SIF TALENT:남자 == 1
	PANT_D |= 33554432
;반항적
IF TALENT:반항적 == 1
	PANT_B -= 1
	PANT_C -= 1
ENDIF
;꿋꿋함
SIF TALENT:꿋꿋함 == 1
	PANT_B -= 1
;솔직함
SIF TALENT:솔직함 == 1
	PANT_B += 1
;프라이드높음
IF TALENT:프라이드높음 == 1
	PANT_B -= 2
	PANT_C -= 1
ENDIF
;프라이드낮음
SIF TALENT:프라이드낮음 == 1
	PANT_B += 1
;츤데레は순종4以上ならPANT_B＋以下ならPANT_B－
SIF TALENT:츤데레 == 1 && ABL:순종 >= 4
	PANT_B += 1
SIF TALENT:츤데레 == 1 && ABL:순종 < 4
	PANT_B -= 1
;자제심
IF TALENT:자제심 == 1
	PANT_B -= 1
	PANT_C -= 2
ENDIF
;무관심
IF TALENT:무관심 == 1
	PANT_B -= 2
	PANT_C -= 2
ENDIF
;감정부족
IF TALENT:감정부족 == 1
	PANT_B -= 3
	PANT_C -= 1
ENDIF
;보수적
SIF TALENT:보수적 == 1
	PANT_B -= 1
;일선을넘지않음は陥落前ならPANT_B PANT_C-
IF TALENT:일선을넘지않음 == 1 && (TALENT:연모 == 1 || TALENT:음란 == 1 || TALENT:복종 == 1 || TALENT:친애 == 1 || TALENT:창부 == 1 || TALENT:예속 == 1)
	PANT_B -= 1
	PANT_C -= 2
ENDIF
;정조관념
IF TALENT:정조관념 == 1
	PANT_B -= 1
	PANT_C -= 2
ENDIF
;억압
SIF TALENT:억압 == 1
	PANT_C -= 1
;해방
IF TALENT:해방 == 1
	PANT_B += 2
	PANT_C += 1
ENDIF
;저항
SIF TALENT:저항 == 1
	PANT_C -= 1
;부끄럼쟁이
SIF TALENT:부끄럼쟁이 == 1
	PANT_B -= 1
;수치심없음
SIF TALENT:수치심없음 == 1
	PANT_B += 1
;자위하기쉬움または자위광は자위系COMで
SIF (TALENT:자위하기쉬움 == 1 || TALENT:자위광 == 1) && (SELECTCOM == 4 || SELECTCOM == 5 || SELECTCOM == 6 || SELECTCOM == 7 || SELECTCOM == 509)
	PANT_C += 1
;쾌감에솔직
IF TALENT:쾌감에솔직 == 1
	PANT_B += 2
	PANT_C += 2
ENDIF
;쾌감을부정
IF TALENT:쾌감을부정 == 1
	PANT_B -= 2
	PANT_C -= 2
ENDIF
;발정기
SIF TALENT:발정기 == 1
	PANT_B += 1

;一度BCの回数異常阻止
;真面目で얌전함性格だと感度上がってもどうしようもないので。
SIF PANT_B < 1
	PANT_B = 1
SIF PANT_C < -2
	PANT_C = -2
;快感が入ってなくても喘ぎだすので素質補正は２まで
SIF PANT_C > 2
	PANT_C = 2

;----------------------------------------------------------------------
;行為箇所＋感度計算
;----------------------------------------------------------------------
;感覚が高いとPANT_B PANT_Cにプラス。
;またこの時点でどこを使った行為かPANT_Dに付ける
;거품비비기ってなんだろう…。
;----------------------------------------------------------------------
;B
SELECTCASE SELECTCOM
	CASE 0,5,10 TO 18,73 TO 75,84,141,163,243,317,318,322,341,360,361,373,374,382,402,421,502,504,509,528
		SETBIT PANT_D,14
		;感覚低い～高いでC0～3
		IF ABL:B감각 >= 7
			PANT_B_CALC += 2
			PANT_C_CALC += 3
		ELSEIF ABL:B감각 >= 5
			PANT_B_CALC += 1
			PANT_C_CALC += 2
		ELSEIF ABL:B감각 >= 3
			PANT_C_CALC += 1
		ELSE
			PANT_B_CALC -= 1
		ENDIF
		;調教者の기교により
		IF ABL:PLAYER:기교 == 0
			PANT_C_CALC += 2
		ELSEIF ABL:PLAYER:기교 >= 5
			PANT_C_CALC += 1
		ELSEIF ABL:PLAYER:기교 < 3
			PANT_C_CALC -= 1
		ENDIF
ENDSELECT
;アイテムがついているときは最大+1
IF BCHECK(TARGET)
	SETBIT PANT_D,14
	SIF ABL:B감각 >= 5
		PANT_B_CALC += 1
	SIF ABL:B감각 >= 3
		PANT_C_CALC += 1
ENDIF
;C
SELECTCASE SELECTCOM
	CASE 0,3,4,20 TO 40,60 TO 62,76,145,163,182,240 TO 242,244 TO 246,260 TO 265,280,285,317,318,322,342,343,365,373,374,380 TO 382,400,401,422,430,431,503,504,505,515,519 TO 525
		;애무のとき、정조대があるとＣは気持ちよくない
		IF SELECTCOM == 0 && TEQUIP:정조대
		;윤간のとき、逆セックスがないとＣは気持ちよくない
		ELSEIF SELECTCOM == 322 && (TFLAG:705 & 4) == 0
		;수간のとき、ＶかＡどっちかに突っ込まれてると、Ｃは気持ちよくない
		ELSEIF (SELECTCOM == 322 || SELECTCOM == 381) && TFLAG:705 != 4
		;음뇨한다のとき、その場の尿を飲むんだとＣは気持ちよくない
		ELSEIF SELECTCOM == 285 && BASE:뇨의 < 60
		;サンドイッチのとき、Ｃ애무されてないとＣは気持ちよくない
		ELSEIF SELECTCOM == 515 && TFLAG:40 != 3 && TFLAG:40 != 4 && TFLAG:40 != 5
		;サンドイッチさせるのとき、애무する側だとＣは気持ちよくない
		ELSEIF SELECTCOM == 520 && (TFLAG:49 == 8 || TFLAG:49 == 9 || TFLAG:49 == 10)
		;連結のとき、수だとＣは気持ちよくない
		ELSEIF SELECTCOM == 519 && TFLAG:49 != 6 && TFLAG:49 != 7 && TFLAG:49 != 11 && TFLAG:49 != 12
		ELSE
			SETBIT PANT_D,15
			;感覚低い～高いでC0～3
			IF ABL:C감각 >= 7
				PANT_B_CALC += 2
				PANT_C_CALC += 3
			ELSEIF ABL:C감각 >= 5
				PANT_B_CALC += 1
				PANT_C_CALC += 2
			ELSEIF ABL:C감각 >= 3
				PANT_C_CALC += 1
			ELSE
				PANT_B_CALC += 1
			ENDIF
			;調教者の기교により
			IF ABL:PLAYER:기교 == 0
				PANT_C_CALC += 2
			ELSEIF ABL:PLAYER:기교 >= 5
				PANT_C_CALC += 1
			ELSEIF ABL:PLAYER:기교 < 3
				PANT_C_CALC -= 1
			ENDIF
		ENDIF
ENDSELECT
;アイテムがついているときは最大+1
IF CCHECK(TARGET)
	SETBIT PANT_D,15
	SIF ABL:C감각 >= 5
		PANT_B_CALC += 1
	SIF ABL:C감각 >= 3
		PANT_C_CALC += 1
ENDIF
;A
SELECTCASE SELECTCOM
	CASE 6,7,8,50  TO 56,110 TO 116,161,162,163,180,181,183,286,322,345,352,364,367,368,370,371,373,374,381,382,404,406,501,509,510,512,514,515,518,519,526
		;애널자극のとき、애널자극からの派生じゃないとＡは気持ちよくない
		IF SELECTCOM == 286 && TFLAG:56 != 1
		;윤간のとき、Ａに挿入されてないとＡは気持ちよくない
		ELSEIF SELECTCOM == 322 && (TFLAG:705 & 1) == 0
		;수간のとき、Ａに挿入されてないとＡは気持ちよくない
		;바이브も정조대もないとＶに挿入してＡは使用しない
		ELSEIF SELECTCOM == 381 && !TEQUIP:바이브 && !TEQUIP:정조대
		;기승위乳房자위、駅弁のとき、Ａに挿入されてないとＡは気持ちよくない
		ELSEIF (SELECTCOM == 509 || SELECTCOM == 510) && TFLAG:705 != 1
		;サンドイッチ、連結のとき、Ａに挿入されてないとＡは気持ちよくない
		ELSEIF (SELECTCOM == 515 || SELECTCOM == 519) && TFLAG:40 != 2 && TFLAG:41 != 2
		ELSE
			SETBIT PANT_D,16
			;感覚低い～高いでC0～3
			IF ABL:A감각 >= 7
				PANT_B_CALC += 2
				PANT_C_CALC += 3
			ELSEIF ABL:A감각 >= 5
				PANT_B_CALC += 1
				PANT_C_CALC += 2
			ELSEIF ABL:A감각 >= 3
				PANT_C_CALC += 1
			;慣れてなかったら痛いと思うので고통＋
			ELSEIF SELECTCOM != 50
				PANT_B_CALC -= 1
				;애널애무、자위、애널바이브、애널비즈
				IF SELECTCOM == 6 || SELECTCOM == 7 || SELECTCOM == 51 || SELECTCOM == 53 || SELECTCOM == 54
					IF PALAM:윤활 < PALAMLV:2
						SETBIT PANT_D, 22
						SETBIT PANT_D, 26
					ENDIF
				ELSE
					;윤활により고통１or고통２
					IF PALAM:윤활 < PALAMLV:2
						SETBIT PANT_D, 23
					ELSE
						SETBIT PANT_D, 22
					ENDIF
					SETBIT PANT_D, 26
				ENDIF
			ENDIF
			;調教者の기교により
			IF ABL:PLAYER:기교 == 0
				PANT_C_CALC += 2
			ELSEIF ABL:PLAYER:기교 >= 5
				PANT_C_CALC += 1
			ELSEIF ABL:PLAYER:기교 < 3
				PANT_C_CALC -= 1
			ENDIF
		ENDIF
ENDSELECT
;アイテムがついているときは最大+1
IF ACHECK(TARGET)
	SETBIT PANT_D,16
	SIF ABL:A감각 >= 5
		PANT_B_CALC += 1
	IF ABL:A감각 >= 3
		PANT_C_CALC += 1
	;慣れてない＆濡れてないと痛い
	ELSE
		IF PALAM:윤활 < PALAMLV:2
			SETBIT PANT_D, 22
			SETBIT PANT_D, 26
		ENDIF
	ENDIF
ENDIF
;V
IF !GETBIT(PANT_D,25)
	SELECTCASE SELECTCOM
		CASE 4,7,41  TO 44,100 TO 106,160,162,163,246,322,344,362,363,366,368,369,373,374,381,382,403,405,500,506 TO 511,513,515,517,519
			;자위のとき、Ｖを使わないとＶは気持ちよくない
			IF SELECTCOM == 4 && TFLAG:56 != 1
			;윤간のとき、Ｖに挿入されてないとＡは気持ちよくない
			ELSEIF SELECTCOM == 322 && (TFLAG:705 & 2) == 0
			;수간のとき、Ｖに挿入されてないとＶは気持ちよくない
			;정조대か남자だと駄目
			ELSEIF SELECTCOM == 381 && (TEQUIP:정조대 || !VAGINA(TARGET))
			;기승위乳房자위、駅弁のとき、Ｖに挿入されてないとＶは気持ちよくない
			ELSEIF (SELECTCOM == 509 || SELECTCOM == 510) && TFLAG:705 != 0
			;サンドイッチ、連結のとき、Ｖに挿入されてないとＡは気持ちよくない
			ELSEIF (SELECTCOM == 515 || SELECTCOM == 519) && TFLAG:40 != 1 && TFLAG:41 != 1
			;전기안마のとき、下着か하의を着用しているとＶは気持ちよくない
			ELSEIF SELECTCOM == 246 && (TEQUIP:속옷（아래） || TEQUIP:하의)
			ELSE
				SETBIT PANT_D,17
				;感覚低い～高いでC0～3
				IF ABL:V감각 >= 7
					PANT_B_CALC += 2
					PANT_C_CALC += 3
				ELSEIF ABL:V감각 >= 5
					PANT_B_CALC += 1
					PANT_C_CALC += 2
				ELSEIF ABL:V감각 >= 3
					PANT_C_CALC += 1
				ELSE
					;慣れてない＆濡れてなかったら고통
					PANT_B_CALC -= 1
					IF PALAM:윤활 < PALAMLV:2
						SETBIT PANT_D, 22
						SETBIT PANT_D, 26
					ENDIF
				ENDIF
				;調教者の기교により
				IF ABL:PLAYER:기교 == 0
					PANT_C_CALC += 2
				ELSEIF ABL:PLAYER:기교 >= 5
					PANT_C_CALC += 1
				ELSEIF ABL:PLAYER:기교 < 3
					PANT_C_CALC -= 1
				ENDIF
			ENDIF
	ENDSELECT
ENDIF
IF VCHECK(TARGET)
	SETBIT PANT_D,17
	SIF ABL:V감각 >= 5
		PANT_B_CALC += 1
	IF ABL:V감각 >= 3
		PANT_C_CALC += 1
	;慣れてない＆濡れてないと痛い
	ELSE
		IF PALAM:윤활 < PALAMLV:2
			SETBIT PANT_D, 22
			SETBIT PANT_D, 26
		ENDIF
	ENDIF
ENDIF

;２か所以上責めるコマンドは、行為箇所による感度を半分にする
SELECTCASE SELECTCOM
	CASE 0,7,162,163,317,318,322,368,373,374,381,382,504,509,515,519
		;애무のとき、정조대がついてるとＢしか触らない
		IF SELECTCOM == 0 && TEQUIP:정조대
		ELSE
			PANT_B_CALC = PANT_B_CALC / 2
			PANT_C_CALC = PANT_C_CALC / 2
		ENDIF
ENDSELECT
PANT_B += PANT_B_CALC
PANT_C += PANT_C_CALC

;입
SELECTCASE SELECTCOM
	CASE 1,71,72,75,77,85,87,184,185,222,224,322,407,410,412,420,515,516,520,521,527
		;윤간のとき、입は使わない
		IF SELECTCOM == 322 && (TFLAG:705 & 8) == 0
		;サンドイッチ、サンドイッチさせる、助손をサンドイッチのとき、입は使わない
		ELSEIF (SELECTCOM == 515 || SELECTCOM == 520 || SELECTCOM == 521) && TFLAG:49 != 8 && TFLAG:49 != 9 && TFLAG:49 != 10
		ELSE
			SETBIT PANT_D,13
		ENDIF
ENDSELECT

;継続アイテムによる고통
;重度
IF TEQUIP:확장벌룬 || TEQUIP:요도벌룬 || BASE:뇨의 >= 90 || BASE:변의 >= 90 || TEQUIP:삼각목마
	SETBIT PANT_D, 23
;軽度
ELSEIF TEQUIP:밧줄 || TEQUIP:카테터 || TEQUIP:촉수질내산란 || TEQUIP:촉수장내산란 || TEQUIP:촉수유선삽입 || BASE:뇨의 >= 60 || BASE:변의 >= 60 || BASE:구역질 >= 60
	SETBIT PANT_D, 22
ENDIF
;----------------------------------------------------------------------
;陥落資質
;----------------------------------------------------------------------
;연모
IF TALENT:연모 == 1
	PANT_B += 1
	PANT_D |= 128
ENDIF
;친애
