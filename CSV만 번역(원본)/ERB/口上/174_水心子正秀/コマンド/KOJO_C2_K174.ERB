;ＳＭ系コマンド FIRSTTIME(240～249)
;=================================================
;-------------------------------------------------
;COM140, 스팽킹 FIRSTTIME(240)
;-------------------------------------------------
@KOJO_COM_K174_140
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;地の文呼び出し(カラー変更対応)
	CALL TRAIN_MESSAGE_COLOR
	
	;지하실
	IF TEQUIP:지하실
		;怯え
		IF (CFLAG:5005 & 64)
			SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「ひ、っ…　ご　ごめ、…なさ……っ、ごめんなさい…！　ぐっ、うぅ……」
				CASE 0
					PRINTFORML 「ぁ、あぁ！？　や、やだ…ひっ……　いっ゛　っ、う、ぅー……」
			ENDSELECT
		;반발
		ELSEIF (CFLAG:5005 & 32)
			PRINTFORML 「っ、く゛　ぅ、お…おい　やめ、っ……！　っぁ、く゛　っ…　ぅ、う……」
		ELSE
			CALL PANT_VOICE_EX
		ENDIF
	ELSE
		;怯え
		IF (CFLAG:5005 & 64)
			;PRINTFORML 
		;반발
		ELSEIF (CFLAG:5005 & 32)
			;PRINTFORML 
		;마조
		ELSEIF (CFLAG:5005 & 256)
			;PRINTFORML 
		;それ以外
		ELSE
			;PRINTFORML 
		ENDIF
	ENDIF
ENDIF
RETURN LOCAL

