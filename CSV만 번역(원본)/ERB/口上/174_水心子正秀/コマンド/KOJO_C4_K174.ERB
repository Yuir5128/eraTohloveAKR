;-------------------------------------------------
;COM320, 야외플레이 FIRSTTIME(420)
;-------------------------------------------------
@KOJO_COM_K174_320
	;-------------------------------------------------
	;移動時
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL = 1
	;-------------------------------------------------
	IF LOCAL && !TFLAG:54
		;初めて
		IF FIRSTTIME(420)
			;妄執フラグオン　맹신なし
			IF (CFLAG:5005 & 4) && TALENT:맹신 == 0
				PRINTFORML 「そ、外に行くのか？」
				PRINTFORML 「……僕を置いて行ったりしないよね」
				PRINTFORML %CALLNAME%は不安そうにこちらを見ている
				;強制的に怯えオン
				CFLAG:5005 |= 64
			;未陥落　真友ルート　굴복3
			ELSEIF !KANRAKU(TARGET) && (CFLAG:5001 & 2) && MARK:굴복각인 == 3
				PRINTFORML 「外へ行くのか？　……ああ、分かった」
				PRINTFORML 存外あっさりと命令を수入れた%CALLNAME%に戸惑っていると、ぽつりと付け발した
				PRINTFORML 「拒否したところで、どのみち無理やり連れて行くのだろう」
				;強制的に怯えオン
				CFLAG:5005 |= 64
			;それ以外
			ELSE
				PRINTFORML 「外に行くのか！？　この格好で？」
				PRINTFORML 「せ、せめて外套くらい……　あぁ、待って\@(CFLAG:5005 & 1)?くれ#よ\@！」
			ENDIF
		;2回目以降
		ELSE
			;윤간경험あり　真友ルート　未陥落
			IF EXP:윤간경험 && (CFLAG:5001 & 2) && !KANRAKU(TARGET)
				PRINTFORML 「………。」
				PRINTFORML %CALLNAME%は何か言いたげにこちらを見ていたが、やがて全てを諦めたように目を伏せた
				;強制的に怯えオン
				CFLAG:5005 |= 64
			;윤간경험あり　未陥落
			ELSEIF EXP:윤간경험 && !KANRAKU(TARGET)
				PRINTFORML 「そ、外へ行くのか？　いや、命令なら従うが……」
				PRINTFORML %CALLNAME%は以前外で遭わされた目を思い出し、身震いしている
				;強制的に怯えオン
				CFLAG:5005 |= 64
			ENDIF
		ENDIF
		
		;地の文呼び出し(カラー変更対応)
		CALL TRAIN_MESSAGE_COLOR
		
	ENDIF
	RETURN LOCAL

	;-------------------------------------------------
	;戻ってきた時
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL = 0
	;-------------------------------------------------
	IF LOCAL && TFLAG:54
		CALL TRAIN_MESSAGE_COLOR
		
		PRINTFORML 
		PRINTFORML 
	ENDIF
	RETURN LOCAL

