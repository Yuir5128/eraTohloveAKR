				PRINTFORML 「気持ちいいなら、いいのだが……」
			CASE 0
				PRINTFORML 「っ、うわ　今ビクって……！」
		ENDSELECT
		PRINTW 
		RETURN LOCAL
	ENDIF
	
	;새드　連続実行　ランダム
	IF (CFLAG:5005 & 512) && PREVCOM == 80 && RAND:2
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「そんなに、ここを蹴り潰されたいのか？」
			CASE 1
				PRINTFORML 「もっとして欲しい？　甘えるんじゃない」
			CASE 0
				PRINTFORML 「まだ발りない？　ほう……、いい度가슴だな」
		ENDSELECT
	;새드　ランダム
	ELSEIF (CFLAG:5005 & 512) && RAND:2
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 「なかなかの眺めだ」
			CASE 2
				PRINTFORML 「ほら　ここがいいのだろう？」
			CASE 1
				PRINTFORML 「そこまで期待されては、私も応えざるを得ないな」
			CASE 0
				PRINTFORML 「まったく……　清麿の趣味は理解できないな」
		ENDSELECT
	;새드　もしくは새드끼３以上
	ELSE
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 「前々から言おうと思っていたのだが、その……頭は大丈夫か？」
			CASE 2
				PRINTFORML 「ふむ……　ここがいいのか」
			CASE 1
				PRINTFORML 「分かった　では、早速……」
			CASE 0
				;신발を履いている時
				IF TEQUIP:신발 > 0
					PRINTFORML 「これで통증より快楽が勝るというのが、不思議だな」
					PRINTFORML 「……いや、痛いからこそいいのか？」
				;신발下とか스타킹
				ELSEIF TEQUIP:특수2 > 0
					PRINTFORML 「うわっ　中まで染みて……！」
				ELSE
					PRINTFORML 「脈打つ感覚が分かるというのも素발ならでは、か」
				ENDIF
		ENDSELECT
	ENDIF
	
	PRINTFORM %CALLNAME%は
				
	;신발を履いている時
	IF TEQUIP:신발
		A = 9
		CALL CLOTHES_NAME
	ELSEIF TEQUIP:특수2
		A = 2
		CALL CLOTHES_NAME
	ENDIF
	
	;새드　連続実行풋잡
	IF (CFLAG:5005 & 512) && PREVCOM == 80 && RAND:2
		SELECTCASE RAND:2
			CASE 1
				PRINTFORMW \@ TEQUIP:신발 || TEQUIP:특수2?を履いた발で#발で\@陰茎を蹴り上げた
			CASE 0
				PRINTFORMW \@ TEQUIP:신발 || TEQUIP:특수2?を履いた발で#발で\@陰茎を踏み潰した
		ENDSELECT
	;ほか
	ELSE
		SELECTCASE RAND:5
			CASE 4
				PRINTFORMW \@ TEQUIP:신발 || TEQUIP:특수2?を履いた발で#발で\@陰茎をぐりぐりと踏んでいる
			CASE 3
				PRINTFORMW \@ TEQUIP:신발 || TEQUIP:특수2?のかかとに#かかとに\@力を込め踏みつけた
			CASE 2
				PRINTFORMW \@ TEQUIP:신발 || TEQUIP:특수2?のつま先で#つま先で\@陰茎を撫で上げた
			CASE 1
				PRINTFORMW \@ TEQUIP:신발 || TEQUIP:특수2?の발裏で#발裏で\@陰茎を軽く蹴りつけた
			CASE 0
				PRINTFORMW \@ TEQUIP:신발 || TEQUIP:특수2?を履いた발で#발で\@挟み込むように陰茎を刺激した
		ENDSELECT
	ENDIF
	
ENDIF
RETURN LOCAL

