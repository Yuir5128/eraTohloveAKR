;=================================================
;基本系コマンド FIRSTTIME(100～109)
;=================================================
;-------------------------------------------------
;COM0, 애무 FIRSTTIME(100)
;-------------------------------------------------
@KOJO_COM_K174_0
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;地の文呼び出し(カラー変更対応)
	CALL TRAIN_MESSAGE_COLOR
	
	;初回
	IF FIRSTTIME(100)
		PRINTFORML 「え？　え、？　なにをしてるんだ……？」
		PRINTFORML %CALLNAME%は怪訝そうな顔で%CALLNAME:PLAYER%を見ている
	ELSE
		;未陥落　반발
		IF !KANRAKU(TARGET) || (CFLAG:5005 & 32)
			SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「ん…、…っ……　ぁっ……　んん、や…め、っ…」
				CASE 0
					CALL PANT_VOICE_EX
			ENDSELECT
			RETURN LOCAL
		ENDIF
		
		PRINTFORML %CALLNAME:PLAYER%が触れるたび、%CALLNAME%は声をもらしている
		SELECTCASE RAND:2
			CASE 1
				PRINTFORML 「は、…っ……　あっ……　ぁ、そこ…、っ…」
			CASE 0
				CALL PANT_VOICE_EX
		ENDSELECT
		
	ENDIF
ENDIF
RETURN LOCAL

