	;地の文呼び出し(カラー変更対応)
	CALL TRAIN_MESSAGE_COLOR
	
	SELECTCASE RAND:3
		CASE 2
			PRINTFORML 「あ゛っ゛…　や゛め゛……っ゛、つ゛ふ゛れ゛っ゛、え゛ぇ゛……！！」
		CASE 1
			PRINTFORML 「つ゛、ふ゛れ゛っ゛…ぁ゛、が゛っ゛……、う゛っ゛……！」
		CASEELSE
			CALL PANT_VOICE_EX
	ENDSELECT
	
	;마조
	IF (CFLAG:5005 & 256)
		PRINTFORML %CALLNAME%は苦悶の表情を浮かべながらも、声にわずかな艶を帯びさせている
	ELSE
		PRINTFORML %CALLNAME%はあまりの激痛に、全身に冷や汗を浮かべている
	ENDIF
	
ENDIF
RETURN LOCAL

