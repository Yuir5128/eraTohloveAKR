;-------------------------------------------------
;19, ビデオ売却時
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_19
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;未陥落　연모친애(思慕対象問わず)
	IF !KANRAKU(TARGET) || KANRAKU2(TARGET) == 1
		PRINTFORMW 「その『びでお』とやらはどうするつもりだ？」
		PRINTFORMW 「えぇっ、売るの！？」
		PRINTFORMW 「……もう一度考え直すつもりはないか？」
		
	;ハートフル음란
	ELSEIF ((CFLAG:5001 & 1) && TALENT:음란)
		PRINTFORMW 「その『びでお』とやらはどうするつもりだ？」
		PRINTFORML 「えぇっ、売るの！？」
		PRINTFORMW 「あぁ……　私の痴態がばら撒かれてしまうということか　それも中々……」
		PRINTFORMW %CALLNAME%は緩んだ顔で何やらブツブツ言っているがが、こちらに気づくとすぐに我に返った
		PRINTFORMW 「はっ……んんっ！　まあ小銭にはなるのではないか」
	ENDIF
	
	;地の文呼び出し(カラー変更対応)
	CALL EVENT_MESSAGE_COLOR
	
ENDIF
RETURN LOCAL

;-------------------------------------------------
;24, 비디오촬영후、売らずに保管すると言ったとき
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_24
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;未陥落　반발　ハートフル음란
	IF !KANRAKU(TARGET) || (CFLAG:5005 & 32) || ((CFLAG:5001 & 1) && TALENT:음란)
		PRINTFORML 「その『びでお』とやらは先ほどの光景が写っているんだったな」
		PRINTFORML 「変なことには使うんじゃないぞ」
		;ハートフル음란
		IF (CFLAG:5001 & 1) && TALENT:음란
			PRINTFORML 変なことって具体的には、と聞いてみた
			PRINTFORML 「え、それは……　へ、変なことは変なことだ！　それ以上でも以下でもない！」
		ENDIF
	ENDIF
	
	;地の文呼び出し(カラー変更対応)
	CALL EVENT_MESSAGE_COLOR
ENDIF
RETURN LOCAL

;-------------------------------------------------
;25, 衣装変更
;T:A = Y　　　　着脱する衣装を限定する時
;A = X　　　　　着脱部位全体を示す時
;TEQUIP:A = Y　 今何を着ているか（着てない場合は0）を組み合わせる
;脱がす、着せるに関わらず、特定の組み合わせの衣装になったときの記述例に裸신발下入れておきます
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_25
;-------------------------------------------------
;脱ぎ着にかかわらず特定の衣装組み合わせになった時例
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && !Y
	;地の文呼び出し(カラー変更対応)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;신발下or발袋だけにしたとき
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:1 && TEQUIP:0 == 0 && TEQUIP:2 == 0 && TEQUIP:3 == 0 && TEQUIP:4 == 0 && TEQUIP:5 == 0 && TEQUIP:6 == 0
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
ENDIF
;-------------------------------------------------
;着せたとき
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && Y
	;地の文呼び出し(カラー変更対応)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;정조대を付ける
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && T:2 == 5
		SIF TALENT:연모 || TALENT:친애
			PRINTFORML 「このような器具をつけなくても、\@(CFLAG:5001 &1) ?私#僕\@は清麿以外とは……」
		
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;下着を付ける
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && T:2 > 0
		
		RETURN LOCAL
	ENDIF
	
	
	;-------------------------------------------------
	;水兵服＆スカートにしたとき
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:4 == 1 && TEQUIP:5 == 2
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;女装
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP_FEMALE(TARGET) && MALE(TARGET)
		PRINTFORML 「これは……、女性の服ではないか」
		PRINTFORML %CALLNAME%は唇を尖らせている
		RETURN LOCAL
	ENDIF
	
	
ENDIF


;-------------------------------------------------
;76, 衣装変更
;T:A = Y　　　　着脱する衣装を限定する時
;A = X　　　　　着脱部位全体を示す時
;TEQUIP:A = Y　 今何を着ているか（着てない場合は0）を組み合わせる
;脱がす、着せるに関わらず、特定の組み合わせの衣装になったときの記述例に裸신발下入れておきます
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_76
;-------------------------------------------------
;脱がした時
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && Y
	;地の文呼び出し(カラー変更対応)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;정조대を外す
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && A == 2 && T:2 == 5
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;정조대以外の下着を外す
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && A == 2 && T:2 != 5
		
		IF PALAM:윤활 >= PALAMLV:4
			PRINT 脱がした
			CALL CLOTHES_NAME
			PRINTFORML からは\@VAGINA(TARGET)?愛液#先走り\@が糸のように滴った
		ELSEIF PALAM:윤활 >= PALAMLV:3
			PRINTFORM 脱がした
				CALL CLOTHES_NAME
			PRINTFORML は、\@VAGINA(TARGET)?愛液#先走り\@で湿りを帯びていた
		ENDIF
		RETURN LOCAL
	ENDIF
ENDIF

