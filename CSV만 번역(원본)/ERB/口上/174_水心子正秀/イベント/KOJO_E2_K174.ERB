;-------------------------------------------------
;8, 調教후자위
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_8
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;地の文呼び出し(カラー変更対応)
	CALL EVENT_MESSAGE_COLOR
	;정조대
	IF TEQUIP:속옷（아래） == 5
		SELECTCASE CFLAG:602
			CASE IS >= 300
				PRINTFORML 「ぁ…、ぁ……　う、ぅ…　も、やだ……　たすけ…っ…　ぅ、ぐすっ……」
				PRINTFORML あまりの下腹部の切なさに、%CALLNAME%はたまらず泣き出してしまった
			CASE IS >= 100
				PRINTFORML 「ふーっ……、…っ　ぅー……　ふー、っぅ…　う、ぅ……」
				PRINTFORML %CALLNAME%はわずかでも快感を得ようと、정조대の上から陰部をいじっていた
				PRINTFORML しかし必死の努力も空しく、身体は熱を帯びていく一方だった
			CASE IS >= 50
				PRINTFORML 「ま、待ってくれ……　っ、このままなんて…、ぁ…無理、っ…　耐えられない……」
				PRINTFORML 赤い顔の%CALLNAME%は息を荒げながら%CALLNAME:PLAYER%にすり寄って必死に懇願するが、
				PRINTFORML %CALLNAME:PLAYER%は我関せずと部屋を出て行った
			CASEELSE
				PRINTFORML 「ぅ…　っ、ぁ…ぅ、うぅ…　うー…、っ……」
				PRINTFORML じくじくと増していく下腹部の疼きに、たまらず%CALLNAME%はうめき声を上げている
		ENDSELECT
		PRINTL 
	ENDIF
ENDIF
RETURN LOCAL


