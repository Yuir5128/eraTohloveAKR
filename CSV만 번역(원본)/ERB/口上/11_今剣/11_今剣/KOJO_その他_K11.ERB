;		RETURN 0
;	ENDIF
;ENDIF
;=================================================
;調教者の사정時のテキスト
;=================================================
;-------------------------------------------------
;사정箇所変更時のテキスト　有効時はここで関数終了　CFLAG:221～230
;-------------------------------------------------
IF CFLAG:106
	CALL PALAM_MESSAGE_A
	;머리카락
	IF CFLAG:106 == 1
		PRINTFORMW 「ううー、べとべとします……」
		CFLAG:221 = 1
	;顔
	ELSEIF CFLAG:106 == 2
		PRINTFORMW 「めがあけられませんよー」
		CFLAG:222 = 1
	;가슴
	ELSEIF CFLAG:106 == 3
		CFLAG:223 = 1
	;腹
	ELSEIF CFLAG:106 == 4
		CFLAG:224 = 1
	;腋
	ELSEIF CFLAG:106 == 5
		CFLAG:225 = 1
	;太腿
	ELSEIF CFLAG:106 == 6
		CFLAG:226 = 1
	;割れ目
	ELSEIF CFLAG:106 == 7
		CFLAG:227 = 1
	;尻
	ELSEIF CFLAG:106 == 8
		CFLAG:228 = 1
	;ゴム
	ELSEIF CFLAG:106 == 9
		;セックスで사정かつ、애널セックス以外を実行時
		IF TFLAG:2 > 0 && SELECTCOM != 110 && SELECTCOM != 111 && SELECTCOM != 112 && SELECTCOM != 113 && SELECTCOM != 114 && SELECTCOM != 115 && SELECTCOM != 116
			CFLAG:229 = 1
		ELSE
			CFLAG:230 = 1
		ENDIF
	ENDIF
	RETURN 0
ENDIF

;-------------------------------------------------
;손、입以外で사정　CFLAG:231
;TFLAG:9 == １は사정、２は大量사정
;対応コマンドはキス、스마타、풋잡、애널強制奉仕、더블스마타
;-------------------------------------------------
IF TFLAG:9 == 1 || TFLAG:9 == 2
	CALL PALAM_MESSAGE_A
	CFLAG:231 = 1
ENDIF

;-------------------------------------------------
;조수를범한다で사정　CFLAG:232
;-------------------------------------------------
IF TFLAG:7 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;입で사정　CFLAG:233
;-------------------------------------------------
IF TFLAG:0 > 0
	CALL PALAM_MESSAGE_A
	;陥落済み+정액중독もしくは봉사정신
	IF (ABL:봉사정신 >= 4 || ABL:정액중독 >= 2) && KANRAKU(TARGET) && (CFLAG:233 < 3 || FLAG:7 == 2)
		CFLAG:233 = 3
	;陥落済み
	ELSEIF KANRAKU(TARGET) && (CFLAG:233 < 2 || FLAG:7 == 2)
		PRINTFORMW 「んく、はぁ……」
		PRINTFORMW %CALLNAME%はうっとりと입の端からこぼれた白濁を舌でなめとった
		CFLAG:233 = 2
	;それ以外
	ELSEIF CFLAG:233 < 1 || FLAG:7 == 2
		PRINTFORML 「うっ……ぐ、」
		PRINTFORMW 精液を吐き出そうとした%CALLNAME%の입を、%CALLNAME:PLAYER%は손で覆って無理矢理に飲みこませた
		CFLAG:233 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;손で사정　CFLAG:234
;-------------------------------------------------
IF TFLAG:1 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:234 = 1
ENDIF

;-------------------------------------------------
;가슴で사정　CFLAG:235
;-------------------------------------------------
IF TFLAG:34 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:235 = 1
ENDIF

;-------------------------------------------------
;발で사정　CFLAG:236
;-------------------------------------------------
IF TFLAG:16 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:236 = 1
ENDIF

;-------------------------------------------------
;セックスで사정　CFLAG:237　CFLAG:238
;-------------------------------------------------
IF TFLAG:2 > 0
	CALL PALAM_MESSAGE_A
	;애널セックス系
	IF SELECTCOM == 110 || SELECTCOM == 111 || SELECTCOM == 112 || SELECTCOM == 113 || SELECTCOM == 114 || SELECTCOM == 115 || SELECTCOM == 116
		IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
			PRINTFORMW 「…あ、あついのが、なかに…っ、んんっ%UNICODE(0x2661)%」
		ELSE
			PRINTFORML 「ぅ、あ……はぁ……あつい……っ」
		ENDIF
		CFLAG:237 = 1
	;セックス系
	ELSE
		IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
			IF PALAM:욕정 > 20000
				PRINTFORML 「ふああっ、あるじさまぁ%UNICODE(0x2661)%　うぅっ、ああっ、ああああっ！！」
				PRINTFORMW 「あついのが……あついのがおなかにいっぱい%UNICODE(0x2661)%　ぼくっ、あふれちゃう……はぁ、はぁぁ……%UNICODE(0x2661)%」
			ELSE
				PRINTFORMW 「あっ、あっ%UNICODE(0x2661)% 　なか、でてる……お、おくまで……ふぁっ%UNICODE(0x2661)%」
				PRINTFORMW 胎内の最奥に種付けられる感触に%CALLNAME%は全身を震わせている……
			ENDIF
		ELSE
			PRINTFORMW 「あっ、あっ%UNICODE(0x2661)% 　なか、でてる……お、おくまで……ふぁっ%UNICODE(0x2661)%」
			PRINTFORMW 胎内の最奥に種付けられる感触に%CALLNAME%は全身を震わせている……
		ENDIF
		CFLAG:238 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;逆セックスで사정させた　CFLAG:239
;-------------------------------------------------
IF (SELECTCOM == 260 || SELECTCOM == 261 || SELECTCOM == 262 || SELECTCOM == 263 || SELECTCOM == 264 || SELECTCOM == 265) && TFLAG:10 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:239 = 1
ENDIF

