;----------------------------------------------------------------------
;○○汎用喘ぎ
;----------------------------------------------------------------------
;입が塞がってる(볼개그・촉수입虐中以外)
;----------------------------------------------------------------------
@PANT_VOICE_A_KX1
;個別汎用喘ぎを使用する時は下の行を削除してください
RETURN 0
PRINT 「
REPEAT B
	P:2 = 10
	CALL VOICE_RENZOKU
	;고통がある
	IF (D & 12582912) != 0
		IF P == 0
			PRINT 
		ELSEIF P == 1
			PRINT 
		ELSEIF P == 2
			PRINT 
		ELSEIF P == 3
			PRINT 
		ELSEIF P == 4
			PRINT 
		ELSEIF P == 5
			PRINT 
		ELSEIF P == 6
			PRINT 
		ELSEIF P == 7
			PRINT 
		ELSEIF P == 8
			PRINT 
		ELSEIF P == 9
			PRINT 
		ENDIF
	;任意奉仕系
	ELSEIF (D & 16777216) != 0
		IF P == 0
			PRINT 
		ELSEIF P == 1
			PRINT 
		ELSEIF P == 2
			PRINT 
		ELSEIF P == 3
			PRINT 
		ELSEIF P == 4
			PRINT 
		ELSEIF P == 5
			PRINT 
		ELSEIF P == 6
			PRINT 
		ELSEIF P == 7
			PRINT 
		ELSEIF P == 8
			PRINT 
		ELSEIF P == 9
			PRINT 
		ENDIF
	;その他
	ELSE
		IF P == 0
			PRINT 
		ELSEIF P == 1
			PRINT 
		ELSEIF P == 2
			PRINT 
		ELSEIF P == 3
			PRINT 
		ELSEIF P == 4
			PRINT 
		ELSEIF P == 5
			PRINT 
		ELSEIF P == 6
			PRINT 
		ELSEIF P == 7
			PRINT 
		ELSEIF P == 8
			PRINT 
		ELSEIF P == 9
			PRINT 
		ENDIF
	ENDIF
	P:1 = P
REND
P = RAND:3
	IF P == 0
		PRINT 
	ELSEIF P == 1
		PRINT 
	ELSE
		PRINT 
	ENDIF
PRINTW 」

A = 0
B = 0
C = 0
D = 0
E = 0
F = 0
G = 0
RETURN 0

