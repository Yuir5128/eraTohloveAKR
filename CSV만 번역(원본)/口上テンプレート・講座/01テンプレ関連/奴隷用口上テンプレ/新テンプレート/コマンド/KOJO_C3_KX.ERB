		PRINTFORMW 
		PRINTFORMW 
	ELSE
		PRINTFORMW 
		PRINTFORMW 
	ENDIF
ENDIF
RETURN LOCAL

;-------------------------------------------------
;COM201, 선물 FIRSTTIME(301)
;何をあげてるか？は、コマンドフォルダ内선물の項目に入ってます
;自分であげるものを決めたい場合、CALL TRAIN_MESSAGE_COLORを消して、自分で書く
;-------------------------------------------------
@KOJO_COM_KX1_201
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;地の文呼び出し(カラー変更対応)
	CALL TRAIN_MESSAGE_COLOR
	
	;笑顔
	IF TFLAG:17 == 1
	;食べ物
	ELSEIF TFLAG:17 == 2
	;日用品
	ELSEIF TFLAG:17 == 3
	;악세사리
	ELSEIF TFLAG:17 == 4
	;服
	ELSEIF TFLAG:17 == 5
	;高価な物
	ELSEIF TFLAG:17 == 6
	;特別な선물（Extraモードエンディング用）
	ELSEIF TFLAG:17 == 7
		;エンゲージリング
		IF CFLAG:5100 == 1
		;オーダーメイド바이브
		ELSEIF CFLAG:5101 == 1
		;オーダーメイド首輪
		ELSEIF CFLAG:5102 == 1
		ENDIF
	;軽装
	ELSEIF TFLAG:17 == 11
	;プロミスリング
	ELSEIF TFLAG:17 == 50
	ENDIF
ENDIF
RETURN LOCAL

