	;調教対象の마조끼
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ABL:마조끼
		SELECTCASE ABL:마조끼
			;마조끼５以上
			CASE IS >= 5
				PRINTFORMW 
				PRINTFORMW 
			;마조끼３以上
			CASE IS >= 3
				PRINTFORMW 
				PRINTFORMW 
			;마조끼１以上
			CASE IS >= 1
				PRINTFORMW 
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL
	ENDIF

