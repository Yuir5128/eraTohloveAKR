;-------------------------------------------------
;촉수전립선자극
;촉수계 커맨드
;-------------------------------------------------
@COM364

PRINTL 촉수전립선자극
STR:0 = 촉수전립선자극

CALL KOJO_MESSAGE_COM

C = 1

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 50
LOSEBASE:기력 += 100

IF EXP:촉수경험 < EXPLV:1
	TIMES LOSEBASE:체력, 1.10
	TIMES LOSEBASE:기력, 2.00
ELSEIF EXP:촉수경험 < EXPLV:2
	TIMES LOSEBASE:체력, 0.90
	TIMES LOSEBASE:기력, 1.80
ELSEIF EXP:촉수경험 < EXPLV:3
	TIMES LOSEBASE:체력, 0.80
	TIMES LOSEBASE:기력, 1.50
ELSEIF EXP:촉수경험 < EXPLV:4
	TIMES LOSEBASE:체력, 0.70
	TIMES LOSEBASE:기력, 1.20
ELSEIF EXP:촉수경험 < EXPLV:5
	TIMES LOSEBASE:체력, 0.60
	TIMES LOSEBASE:기력, 1.00
ELSE
	TIMES LOSEBASE:체력, 0.50
	TIMES LOSEBASE:기력, 0.50
ENDIF

;ABL:A감각를 본다
IF ABL:A감각 == 0
	SOURCE:쾌A += 150
ELSEIF ABL:A감각 == 1
	SOURCE:쾌A += 200
ELSEIF ABL:A감각 == 2
	SOURCE:쾌A += 600
ELSEIF ABL:A감각 == 3
	SOURCE:쾌A += 2000
ELSEIF ABL:A감각 == 4
	SOURCE:쾌A += 2600
ELSE
	SOURCE:쾌A += 3400
ENDIF

;EXP:A경험를 본다
;백버진는 있을 수 없다
IF EXP:A경험 < EXPLV:2
	TIMES SOURCE:쾌A , 0.60
ELSEIF EXP:A경험 < EXPLV:3
	TIMES SOURCE:쾌A , 1.00
ELSEIF EXP:A경험 < EXPLV:4
	TIMES SOURCE:쾌A , 1.20
ELSEIF EXP:A경험 < EXPLV:5
	TIMES SOURCE:쾌A , 1.30
ELSE
	TIMES SOURCE:쾌A , 1.80
ENDIF

;PALAM:윤활를 본다
IF PALAM:윤활 < PALAMLV:1
	TIMES SOURCE:쾌A , 0.10
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES SOURCE:쾌A , 0.40
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES SOURCE:쾌A , 1.00
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES SOURCE:쾌A , 1.40
ELSE
	TIMES SOURCE:쾌A , 1.80
ENDIF

SOURCE:중독충족 += 200
SOURCE:액체 += 500
SOURCE:일탈 += 2000

;ABL:A감각를 본다
IF ABL:A감각 == 0
	SOURCE:굴복 += 100
ELSEIF ABL:A감각 == 1
	SOURCE:굴복 += 180
ELSEIF ABL:A감각 == 2
	SOURCE:굴복 += 250
ELSEIF ABL:A감각 == 3
	SOURCE:굴복 += 380
ELSEIF ABL:A감각 == 4
	SOURCE:굴복 += 500
ELSE
	SOURCE:굴복 += 800
ENDIF

;EXP:A경험를 본다
;백버진는 있을 수 없다
IF EXP:A경험 < EXPLV:2
	SOURCE:통증 += 100
ELSEIF EXP:A경험 < EXPLV:3
	SOURCE:통증 += 10
ELSEIF EXP:A경험 < EXPLV:4
	SOURCE:통증 += 0
ELSEIF EXP:A경험 < EXPLV:5
	SOURCE:통증 += 0
ELSE
	SOURCE:통증 += 0
ENDIF

;PALAM:윤활를 본다
IF PALAM:윤활 < PALAMLV:1
	SOURCE:통증 += 1000
	TIMES SOURCE:통증, 3.00
ELSEIF PALAM:윤활 < PALAMLV:2
	SOURCE:통증 += 300
	TIMES SOURCE:통증, 1.00
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES SOURCE:통증, 0.50
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES SOURCE:통증, 0.20
ELSE
	TIMES SOURCE:통증, 0.10
ENDIF

;소인 체형
SIF TALENT:소인체형
	TIMES SOURCE:통증, 2.00

;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 500

;정조관념
IF TALENT:정조관념
	TIMES SOURCE:굴복, 0.60
	SOURCE:반발 += 1000
ENDIF

;PALAM:욕정를 본다
IF PALAM:욕정 < PALAMLV:1
	TIMES SOURCE:중독충족, 0.90
	TIMES SOURCE:액체, 0.90
	TIMES SOURCE:반발, 1.80
ELSEIF PALAM:욕정 < PALAMLV:2
	TIMES SOURCE:중독충족, 1.00
	TIMES SOURCE:액체, 1.00
	TIMES SOURCE:반발, 1.50
ELSEIF PALAM:욕정 < PALAMLV:3
	TIMES SOURCE:중독충족, 1.10
	TIMES SOURCE:액체, 1.10
	TIMES SOURCE:반발, 1.00
ELSEIF PALAM:욕정 < PALAMLV:4
	TIMES SOURCE:중독충족, 1.20
	TIMES SOURCE:액체, 1.20
	TIMES SOURCE:반발, 0.60
ELSE
	TIMES SOURCE:중독충족, 1.30
	TIMES SOURCE:액체, 1.30
	TIMES SOURCE:반발, 0.30
ENDIF

;A민감, 둔감을 본다
;쾌A 자체의 체크는후로 정리해 준다
IF TALENT:A둔감
	TIMES SOURCE:통증, 1.50
	TIMES SOURCE:굴복, 1.50
	TIMES SOURCE:일탈, 1.50
ELSEIF TALENT:A민감
	TIMES SOURCE:통증, 0.60
	TIMES SOURCE:굴복, 0.60
	TIMES SOURCE:일탈, 0.60
ENDIF

SOURCE:성행동 += B/2
SOURCE:불결 += A*150
SOURCE:두려움 += B/4

IF TEQUIP:애널바이브 == 2
	C += 1
ELSEIF TEQUIP:애널바이브 == 3
	C += 2
	TEQUIP:애널바이브 = 2
ELSE
	TEQUIP:애널바이브 = 2
ENDIF

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;촉수삽입라면, 촉수체액이애널를 뒤따른다
IF TEQUIP:촉수
	STAIN:애널 |= 32
	STAIN:장내 |= 32
ENDIF

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
EXP:A경험 += C
PRINTFORML A경험＋{C}
T = 1

;촉수장착중은 의존도 벡터 강제 변경
;의존도 벡터
TFLAG:33 = 5

;의존도 보정치(복종로 1, 예속로 2 강제 변경인 것으로, 이것까지의 수치를 리셋트)
IF TALENT:복종 && ASSIPLAY == 0
	TFLAG:30 = 2
ELSEIF TALENT:예속 && ASSIPLAY == 0
	TFLAG:30 = 4
ENDIF

RETURN 1



;--------------------------------------------------
;촉수전립선 고문 고문
;--------------------------------------------------
@EQUIP_COM364
PRINTL ＜촉수전립선 꾸짖고 고문중＞

LOSEBASE:체력 += 30
LOSEBASE:기력 += 50

IF TEQUIP:애널바이브 == 3
	A = 25
ELSEIF TEQUIP:애널바이브 == 2
	A = 20
ELSE
	A = 12
ENDIF

;EXP:촉수경험를 본다
IF EXP:촉수경험 < EXPLV:1
	TIMES A , 2.00
	D = 0
ELSEIF EXP:촉수경험 < EXPLV:2
	TIMES A , 1.50
	D = 25
ELSEIF EXP:촉수경험 < EXPLV:3
	TIMES A , 1.20
	D = 60
ELSEIF EXP:촉수경험 < EXPLV:4
	TIMES A , 1.00
	D = 180
ELSEIF EXP:촉수경험 < EXPLV:5
	TIMES A , 0.80
	D = 450
ELSE
	TIMES A , 0.60
	D = 1000
ENDIF

;ABL:A감각를 본다
IF ABL:A감각 == 0
	A = 40
ELSEIF ABL:A감각 == 1
	A = 120
ELSEIF ABL:A감각 == 2
	A = 300
ELSEIF ABL:A감각 == 3
	A = 500
ELSEIF ABL:A감각 == 4
	A = 650
ELSE
	A = 850
ENDIF

;EXP:A경험를 본다
IF EXP:A경험 < EXPLV:2
	TIMES A , 0.60
	B = 150
ELSEIF EXP:A경험 < EXPLV:3
	TIMES A , 1.00
	B = 20
ELSEIF EXP:A경험 < EXPLV:4
	TIMES A , 1.20
	B = 0
ELSEIF EXP:A경험 < EXPLV:5
	TIMES A , 1.40
	B = 0
ELSE
	TIMES A , 1.60
	B = 0
ENDIF

;PALAM:윤활를 본다
IF PALAM:윤활 < PALAMLV:1
	TIMES A , 0.10
	B += 400
	TIMES B , 3.00
	C += 1000
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES A , 0.40
	B += 150
	TIMES B , 1.00
	C += 200
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES A , 1.00
	TIMES B , 0.50
	C += 80
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES A , 1.40
	TIMES B , 0.20
ELSE
	TIMES A , 1.80
	TIMES B , 0.10
ENDIF

;PALAM:욕정를 본다
IF PALAM:욕정 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:욕정 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:욕정 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:욕정 < PALAMLV:4
	TIMES A , 1.10
ELSE
	TIMES A , 1.20
ENDIF

;ABL:순종를 본다
IF ABL:순종 == 0
	TIMES A , 0.80
ELSEIF ABL:순종 == 1
	TIMES A , 0.90
ELSEIF ABL:순종 == 2
	TIMES A , 1.00
ELSEIF ABL:순종 == 3
	TIMES A , 1.10
ELSEIF ABL:순종 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;소인체형
SIF TALENT:소인체형
	TIMES B , 2.00

;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 500

SOURCE:쾌A += A/2
SOURCE:성행동 += A/5
SOURCE:통증 += B
SOURCE:불결 += D*50
SOURCE:일탈 += C/2
SOURCE:반발 += C

SIF FLAG:6
	PRINTFORML 촉수가 장벽을 더듬고 있는지, 때때로%CALLNAME%의 하복부가 툭 움직이고 있다…
;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
EXP:A경험 += 2
PRINTL 
PRINTFORML %EXPNAME:1%＋2
T += 2

RETURN 1


