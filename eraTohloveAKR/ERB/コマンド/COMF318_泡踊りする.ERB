;-------------------------------------------------
;거품비비기한다
;시추에이션별 커맨드
;-------------------------------------------------
@COM318

PRINTL 거품비비기한다
STR:0 = 거품비비기한다
CALL KOJO_MESSAGE_COM

;스타킹 벗게 한다
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TEQUIP:특수2 = 0
	TEQUIP:특수2 = 0
ENDIF

;-------------------------------------------------
;사정게이지 체크
;-------------------------------------------------
B = 200

CALL SHOOT_GAUGE_CHECK_MT

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:사정 += B

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 5
LOSEBASE:기력 += 50

SOURCE:쾌B += 100
SOURCE:쾌C += 100

SOURCE:애정 += 300

;스킨십선호라면애정의 소스 추가
SIF TALENT:스킨십선호
	SOURCE:애정 += 50

SOURCE:두려움 += 20
SOURCE:중독충족 += 100
SOURCE:욕정 += 300
SOURCE:노출 += 60
SOURCE:일탈 += 300
SOURCE:반발 += 300

;-------------------------------------------------
;특수소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 800


;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
CALL COM_STAIN

;이동시에 오줌·변존재를 클리어
TFLAG:66 = 0
TFLAG:57 = 0


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;게이경험라든지 가산
CALL COM_EXP(7)

;애정경험
IF (TALENT:연모 == 1 || TALENT:친애 == 1) && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +1
	EXP:애정경험 += 1
ENDIF

;의존도 벡터
TFLAG:33 = 1

;의존도 보정치(주인 또한연모, 음란, 복종로 1. 친애, 창부, 예속로 2)
IF (TALENT:연모 || TALENT:음란 || TALENT:복종) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:친애 || TALENT:창부 || TALENT:예속) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

