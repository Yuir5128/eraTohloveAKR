;-------------------------------------------------
;자위과시한다
;과시하고 계 커맨드
;-------------------------------------------------
@COM227

PRINTL 자위과시한다
STR:0 = 자위과시한다

CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;사정게이지 체크
;-------------------------------------------------
B = 500

CALL SHOOT_GAUGE_CHECK_MT

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:사정 += B

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 0
LOSEBASE:기력 += 50

SOURCE:액체 =50
SOURCE:욕정 += 150
SOURCE:굴복 += 750
SOURCE:일탈 += 500
SOURCE:반발 += 400

;-------------------------------------------------
;특수소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 400


;-------------------------------------------------
;사정체크
;-------------------------------------------------
CALL SAMEN_CHECK(PLAYER, 1)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 0
	;사정처 선택의 가능·불가
	;노예가공, 한편 주인이수나오메가라면 선택 불가
	IF TALENT:공 && (TALENT:PLAYER:수 || TALENT:MASTER:오메가)
		TFLAG:704 = 0
	;노예의순종와봉사정신발 해 7이상, 혹은정액중독유라면 선택가능
	ELSEIF ((ABL:순종+ABL:봉사정신) >= 7) || ABL:정액중독
		TFLAG:704 = 1
	ELSE
		TFLAG:704 = 0
	ENDIF
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;의존도 벡터
TFLAG:33 = 3

EXP:PLAYER:A경험 += 1
PRINTFORML %CALLNAME:PLAYER%의 A경험＋1


RETURN 1


