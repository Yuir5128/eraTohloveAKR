;-------------------------------------------------
;삽입 자궁입고문
;파생계 커맨드
;-------------------------------------------------
@COM508

PRINTL 삽입 자궁입고문
STR:0 =삽입 자궁입고문

SELECTCOM = 508
CALL KOJO_MESSAGE_COM


;스타킹 찌릿찌릿 플래그
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF

;-------------------------------------------------
;사정게이지 체크
;-------------------------------------------------
B = 600

CALL SHOOT_GAUGE_CHECK_MT

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:사정 += B

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 60
LOSEBASE:기력 += 120

SOURCE:쾌V += 400

SOURCE:애정 += 350

;스킨십선호라면애정의 소스 추가
SIF TALENT:스킨십선호
	SOURCE:애정 += 50
SOURCE:통증 += 400
SOURCE:중독충족 += 250
SOURCE:노출 += 120
SOURCE:반발 += 500


;-------------------------------------------------
;특수소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 500

;정조관념소유로연모, 친애 가져 이외로는 항상반발의 소스를 추가한다(섹스계만)
SIF TALENT:정조관념 && (TALENT:연모 == 0 && TALENT:친애 == 0)
	SOURCE:반발 += 1000

;V에의고통는 먼저 처리해 둔다
;EXP:V경험를 본다(처녀의반감는 별도 처리필 보고인 것으로 생략)
IF EXP:V경험 < EXPLV:1
	TIMES SOURCE:통증, 2.50
ELSEIF EXP:V경험 < EXPLV:2
	TIMES SOURCE:통증, 0.80
	TIMES SOURCE:반발, 1.20
ELSEIF EXP:V경험 < EXPLV:3
	TIMES SOURCE:통증, 0.40
ELSEIF EXP:V경험 < EXPLV:4
	TIMES SOURCE:통증, 0.10
ELSEIF EXP:V경험 < EXPLV:5
	TIMES SOURCE:통증, 0.00
ELSEIF EXP:V경험 >= EXPLV:5
	TIMES SOURCE:통증, 0.00
ENDIF

;PALAM:윤활를 본다
IF PALAM:윤활 < PALAMLV:1
	TIMES SOURCE:통증, 1.20
	TIMES SOURCE:반발, 1.50
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES SOURCE:통증, 0.70
	TIMES SOURCE:반발, 1.20
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES SOURCE:통증, 0.40
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES SOURCE:통증, 0.10
ELSEIF PALAM:윤활 >= PALAMLV:4
	TIMES SOURCE:통증, 0.10
ENDIF

;중독에 의한중독충족의 소스는 먼저 처리해 둔다
;섹스중독를 본다
IF ABL:섹스중독 == 1
	TIMES SOURCE:중독충족, 0.00
ELSEIF ABL:섹스중독 == 2
	TIMES SOURCE:중독충족, 0.30
ELSEIF ABL:섹스중독 == 3
	TIMES SOURCE:중독충족, 0.60
ELSEIF ABL:섹스중독 == 4
	TIMES SOURCE:중독충족, 0.90
ELSEIF ABL:섹스중독 == 5
	TIMES SOURCE:중독충족, 1.20
ELSEIF ABL:섹스중독 == 6
	TIMES SOURCE:중독충족, 1.50
ELSEIF ABL:섹스중독 == 7
	TIMES SOURCE:중독충족, 1.80
ELSEIF ABL:섹스중독 == 8
	TIMES SOURCE:중독충족, 2.30
ELSEIF ABL:섹스중독 == 9
	TIMES SOURCE:중독충족, 3.00
ELSEIF ABL:섹스중독 >= 10
	TIMES SOURCE:중독충족, 5.00
ENDIF


;-------------------------------------------------
;사정체크
;-------------------------------------------------
CALL SAMEN_CHECK(PLAYER, 1)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 1
	;사정처 선택의 가능·불가
	TFLAG:704 = 1
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;노예의 V와질내⇔조교자의 C의 더러움이 이동
;페니스밴드를 사용하는 경우, 더러움은 이동하지 않는다
IF PENIS(PLAYER)
	CALL COM_STAIN
ENDIF

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;V경험
EXP:V경험 += 2
PRINTL V경험＋2

;조교자가말자지로 노예가소인체형였던 경우, V확장경험
IF TALENT:소인체형 && TALENT:PLAYER:말자지
	IF EXP:V확장경험 == 0
		EXP:이상경험 += 1
		PRINTL V확장 첫경험으로이상경험＋1
	ENDIF
	EXP:V확장경험 += 1
	PRINTL V확장경험＋1
ENDIF

;성교경험
EXP:PLAYER:성교경험 += 1
EXP:성교경험 += 1
PRINTS EXPNAME:4
PRINTL +1

;삽입경험
EXP:PLAYER:삽입경험 += 1

;게이경험라든지 가산
CALL COM_EXP(4)

;애정경험
IF TALENT:연모 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +2
	EXP:애정경험 += 2
ELSEIF TALENT:친애 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +4
	EXP:애정경험 += 4
ENDIF

;의존도 벡터
TFLAG:33 = 1

;의존도 보정치(연모로 1, 친애로 2)
IF TALENT:연모 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:친애 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1







