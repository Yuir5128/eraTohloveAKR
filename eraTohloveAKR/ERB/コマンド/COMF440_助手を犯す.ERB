;-------------------------------------------------
;조수를범한다
;주인×조수계 커맨드
;-------------------------------------------------
@COM440

;-------------------------------------------------
;특수파생 처리
;-------------------------------------------------
;전회와 이번 조교자가 같음
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;양쪽 모두 삽입하고 있으면 「조수를 이륜 꽂아」
	IF PREVCOM == 430
		CALL COM_ABLE524
		IF RESULT == 1
			TFLAG:40 = 11
			TFLAG:41 = 0
			TFLAG:49 = 11
			JUMP COM524
		ENDIF
	;노예가 조수를 범하고 있을 때 「조수를 샌드위치(두 구멍 삽입)」
	ELSEIF PREVCOM == 431
		CALL COM_ABLE521
		IF RESULT == 1
			TFLAG:40 = 12
			TFLAG:41 = 0
			TFLAG:49 = 11
			JUMP COM521
		ENDIF
	;입욕중은 「조수를 샌드위치」
	ELSEIF PREVCOM == 407
		CALL COM_ABLE521
		IF RESULT == 1
			TFLAG:40 = 11
			TFLAG:41 = 0
			TFLAG:49 = 13
			JUMP COM521
		ENDIF
	;조수가 노예를 범하는 경우 「연결(노예←조수←주인)」
	ELSEIF PREVCOM == 405 || PREVCOM == 406
		CALL COM_ABLE519
		IF RESULT == 1
			IF PREVCOM == 405
				TFLAG:41 = 1
			ELSE
				TFLAG:41 = 2
			ENDIF
			TFLAG:40 = 11
			TFLAG:49 = 0
			JUMP COM519
		ENDIF
	;노예가 주인을 범하는 경우 「연결(조수←주인←노예)」
	ELSEIF PREVCOM == 261 || PREVCOM == 264
		CALL COM_ABLE519
		IF RESULT == 1
			IF PREVCOM == 261
				TFLAG:49 = 6
			ELSE
				TFLAG:49 = 7
			ENDIF
			TFLAG:40 = 11
			TFLAG:41 = 0
			JUMP COM519
		ENDIF
	;전회 커맨드가 「조수를 이륜 꽂아」 「조수를 샌드위치」 「연결」
	ELSEIF PREVCOM == 524 || PREVCOM == 521 || PREVCOM == 519
		;양쪽 모두 삽입하고 있으면 「조수를 이륜 꽂아」
		IF TFLAG:49 == 11
			CALL COM_ABLE524
			IF RESULT == 1
				TFLAG:40 = 11
				TFLAG:41 = 0
				JUMP COM524
			ENDIF
		;노예가 조수를 범하고 있을 때 「조수를 샌드위치(두 구멍 삽입)」
		ELSEIF TFLAG:49 == 12
			CALL COM_ABLE521
			IF RESULT == 1
				TFLAG:40 = 11
				TFLAG:41 = 0
				JUMP COM521
			ENDIF
		;입욕중, 구욕명령는 「조수를 샌드위치」
		ELSEIF TFLAG:49 == 13
			CALL COM_ABLE521
			IF RESULT == 1
				TFLAG:40 = 11
				TFLAG:41 = 0
				JUMP COM521
			ENDIF
		;노예가 주인을 범하는 경우 「연결(조수←주인←노예)」
		ELSEIF TFLAG:49 == 6 || TFLAG:49 == 7
			CALL COM_ABLE519
			IF RESULT == 1
				TFLAG:40 = 11
				TFLAG:41 = 0
				JUMP COM519
			ENDIF
		;조수가 노예를 범하는 경우 「연결(노예←조수←주인)」
		ELSEIF TFLAG:41 == 1 || TFLAG:41 == 2
			CALL COM_ABLE519
			IF RESULT == 1
				TFLAG:40 = 11
				TFLAG:49 = 0
				JUMP COM519
			ENDIF
		ENDIF
	ENDIF
ENDIF

;아무것도 읽지 않고 여기에 왔을 때는 플래그 해제
TFLAG:40 = 0
TFLAG:41 = 0
TFLAG:49 = 0
TFLAG:42 = 0

PRINTL 조수를범한다
STR:0 = 조수를범한다

CALL KOJO_MESSAGE_COM

;처녀상실 플래그
SIF TALENT:ASSI:처녀 || TALENT:ASSI:재생처녀
	TFLAG:19 |= 3

;조교자가동정였던 경우의 상실 플래그
SIF TALENT:PLAYER:동정
	TFLAG:160 |= 2

;삽입 개소 판정 플래그
TFLAG:705 = 0


;-------------------------------------------------
;사정게이지 체크(주인)
;-------------------------------------------------
B = 1350

CALL SHOOT_GAUGE_CHECK_MA1

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:MASTER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:MASTER:사정 += B

;-------------------------------------------------
;사정게이지 체크(조수)
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_MA2

;재장전 처리중은사정게이지는 20분의 1에
SIF CFLAG:ASSI:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:사정 += B

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 0
LOSEBASE:기력 += 80

SOURCE:액체 += 100
SOURCE:굴복 += 1500
SOURCE:일탈 += 700
SOURCE:반발 += 700

;-------------------------------------------------
;특수소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 700

;조수와 노예의 궁합에 의해, 전체의 소스가 변화
T = NO:TARGET
A = NO:ASSI
SOURCE:액체 *= RELATION:A
SOURCE:굴복 *= RELATION:A
SOURCE:울굴 *= RELATION:A
SOURCE:일탈 *= RELATION:A
SOURCE:반발 *= RELATION:A
SOURCE:액체 /= 100
SOURCE:굴복 /= 100
SOURCE:울굴 /= 100
SOURCE:일탈 /= 100
SOURCE:반발 /= 100


;-------------------------------------------------
;사정체크(주인)
;-------------------------------------------------
CALL SAMEN_CHECK(MASTER, 1)
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 1
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;조수를범한다로사정 한 플래그
SIF E == 2
	TFLAG:7 = 2
SIF E == 1
	TFLAG:7 = 1

;-------------------------------------------------
;사정체크(조수)
;-------------------------------------------------
CALL SAMEN_CHECK(ASSI)

;사정시의 처리
IF E >= 1
	;사정처 타입의 설정
	TFLAG:703 = 6
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정게이지의 재계산이나사정경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF


;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;노예의 P⇔조교자의 V의 더러움이 이동
;페니스밴드를 사용하는 경우, 더러움은 이동하지 않는다
IF PENIS(TARGET)
	CALL COM_STAIN
ENDIF

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;조수가처녀의 경우, 조수와 노예에게이상경험부가. 다만 한 번 만
IF TALENT:ASSI:처녀 == 1 && TFLAG:705 == 0
	PRINTFORML ＜%CALLNAME:ASSI%가처녀상실＞
	TALENT:ASSI:처녀 = 0
	EXP:ASSI:이상경험 += 1
	PRINTFORML %CALLNAME:ASSI%에 %EXPNAME:50%＋1
	;이상한 상태에서의처녀상실로이상경험를 가산한다
	;처녀상실의 상대나 꼬맹이의 자신
		;꼬맹이 서치
		R = NO:ASSI
		L = NO:MASTER
		CALL GET_JIBUN
		IF A
			PRINTFORML %CALLNAME:MASTER%상대에처녀상실로 %EXPNAME:50%+1
			EXP:ASSI:이상경험 += 1
		ENDIF
	;처녀상실이 촬영되었다
	IF TEQUIP:비디오카메라
		PRINTFORML 비디오촬영중에처녀상실로 %EXPNAME:50%+1
		EXP:ASSI:이상경험 += 1
	ENDIF
	IF (CFLAG:99 & 2) == 0
		IF RELATION:(NO:ASSI) > 100
			EXP:이상경험 += 1
			PRINTFORML %CALLNAME%에 %EXPNAME:50%＋1
		ENDIF
		CFLAG:99 |= 2
	ENDIF
	;거치고 싶고 실마리응용 안면 판정의 갱신
	G = (2099 + NO:MASTER)
	H = (2099 + NO:ASSI)
	CFLAG:MASTER:H |= 64
	CFLAG:ASSI:G |= 128
	;처녀상실상대의 기록
	CSTR:ASSI:10 = %CALLNAME:MASTER%
ENDIF

;조수의 V경험
EXP:ASSI:V경험 += 1

;조수가소인체형로 조교자가말자지였던 경우
IF TALENT:ASSI:소인체형 && TALENT:MASTER:말자지
	IF EXP:ASSI:V확장경험 == 0
		EXP:이상경험 += 1
		PRINTL V확장을 강요받아이상경험＋1
		EXP:ASSI:이상경험 += 1
		PRINTFORML V확장 %CALLNAME:ASSI%의이상경험＋1
	ENDIF
	EXP:ASSI:V확장경험 += 1
	PRINTFORML %CALLNAME:ASSI%의 V확장경험＋1
ENDIF

;성교경험
EXP:ASSI:성교경험 += 1
EXP:MASTER:성교경험 += 1
PRINTS EXPNAME:4
PRINTL +1

;삽입경험
EXP:MASTER:삽입경험 += 1

;게이경험라든지 가산
CALL COM_EXP(10)

;의존도 벡터
TFLAG:33 = 5

T = NO:TARGET
A = NO:ASSI
;의존도 보정치(조수와의 궁합 100↑또한음란로 1, 창부로 2)
IF TALENT:음란 && RELATION:A > 100
	TFLAG:30 += 1
ELSEIF TALENT:창부 && RELATION:A > 100
	TFLAG:30 += 2
ENDIF


RETURN 1

