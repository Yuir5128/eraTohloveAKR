;-------------------------------------------------
;전동오나홀
;P계 조교 커맨드
;-------------------------------------------------
@COM32

IF TEQUIP:촉수
	PRINTL 촉수페니스고문
	STR:0 = 촉수페니스고문
	SELECTCOM = 343
ELSE
	PRINTL 전동오나홀
	STR:0 = 전동오나홀
ENDIF

CALL KOJO_MESSAGE_COM


;착탈의 처리
IF TEQUIP:전동오나홀 == 0
	TEQUIP:전동오나홀 = 1
ELSE
	TEQUIP:전동오나홀 = 0
ENDIF

;스타킹 찌릿찌릿 플래그
IF TEQUIP:특수2 == 5 || TEQUIP:특수2 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비체력, 기력
LOSEBASE:체력 += 10
LOSEBASE:기력 += 50

SOURCE:쾌C += 200

SOURCE:노출 += 20
SOURCE:일탈 += 30
SOURCE:반발 += 30

;-------------------------------------------------
;특수소스 처리
;-------------------------------------------------
;억압, 저항소유에는, 억울소스 추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:울굴 += 30


;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;게이경험라든지 가산
CALL COM_EXP(1)

;촉수소환시, 촉수경험의 리셋트를 한다
SIF TEQUIP:촉수 == 1
	T = 0

;의존도 벡터
TFLAG:33 = 1


RETURN 1


;-------------------------------------------------
;장착 계속의 처리
;-------------------------------------------------
@EQUIP_COM32
;오나홀 장비중
IF TEQUIP:촉수
	PRINTL ＜촉수페니스고문중＞
ELSE
	PRINTL ＜오나홀 장비중＞
ENDIF

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
LOSEBASE:체력 += 5
LOSEBASE:기력 += 20

SOURCE:쾌C += 100
SOURCE:노출 += 10
SOURCE:일탈 += 15
SOURCE:반발 += 15


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;촉수경험가산
SIF TEQUIP:촉수 == 1
	T += 1


RETURN 1

