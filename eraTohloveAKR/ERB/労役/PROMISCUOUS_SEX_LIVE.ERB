	;관장플레이
	IF CFLAG:A:178
		;하도SM는 수가 증가한다
		IF CFLAG:A:171
			L = ABL:A:마조끼+ABL:A:스캇중독
			R += ABL:A:마조끼+ABL:A:스캇중독
			
			;고통, 공포의 주용
			D += L*2
		ELSE
			L = (ABL:A:마조끼+ABL:A:스캇중독)/2
			R += (ABL:A:마조끼+ABL:A:스캇중독)/2
			
			;고통, 공포의 주용
			D += L
		ENDIF
		
		EXP:A:관장경험 += L
		PRINTFORML 관장경험＋{L}
		EXP:A:배변경험 += L
		PRINTFORML 배변경험＋{L}
		
		;불쾌의 구슬
		G += (10-ABL:A:스캇중독)*2
		V += L*5
	ENDIF
	
	;음뇨플레이
	IF CFLAG:A:177
		L = (ABL:A:마조끼+ABL:A:스캇중독)/2
		R += (ABL:A:마조끼+ABL:A:스캇중독)/2
		
		EXP:A:음뇨경험 += L
		PRINTFORML 음뇨경험＋{L}
		
		;불쾌의 구슬
		G += (10-ABL:A:스캇중독)*2
	ENDIF
	
	;방뇨 감상
	IF CFLAG:A:176
		L = (ABL:A:마조끼+ABL:A:노출벽)/3
		R += (ABL:A:마조끼+ABL:A:노출벽)/3
		
		EXP:A:배뇨경험 += L
		PRINTFORML 배뇨경험＋{L}
		
		;불쾌의 구슬
		G += 10-ABL:A:스캇중독
		V += L*3
	ENDIF
	
	EXP:A:스캇경험 += R
	PRINTFORML 스캇경험＋{R}
ENDIF

;--------------------------------------------------------
;수치보정
;--------------------------------------------------------
IF B == 1
	V += 5
ELSEIF B == 2
	V += 2
ELSEIF B == 3
	V += 3
ELSEIF B == 4
	V += 5
ELSE
	V += 1
ENDIF

IF TALENT:A:부끄럼쟁이
	V *= 2
ELSEIF TALENT:A:수치심없음
	V /= 2
ENDIF
SIF V < 1
	V = 1


;--------------------------------------------------------
;구슬
;--------------------------------------------------------
IF C > 0
	JUEL:A:쾌C += C*50
	PRINTFORML 쾌C의 구슬＋{C*50}
ENDIF
IF E > 0
	JUEL:A:쾌V += E*30
	PRINTFORML 쾌V의 구슬＋{E*30}
ENDIF
IF F > 0
	JUEL:A:쾌A += F*30
	PRINTFORML 쾌A의 구슬＋{F*30}
ENDIF
IF H > 0
	JUEL:A:쾌B += H*30
	PRINTFORML 쾌B의 구슬＋{H*30}
ENDIF

JUEL:A:순종 += B*100
PRINTFORML 순종의 구슬＋{B*100}

JUEL:A:습득 += Q*35
PRINTFORML 습득의 구슬＋{Q*35}

IF V > 0
	JUEL:A:수치 += V*50
	PRINTFORML 수치의 구슬＋{V*50}
ENDIF

IF TEQUIP:A:정조대
	JUEL:A:욕정 += B*200+1000
	PRINTFORML 욕정의 구슬＋{B*200+1000}
ELSE
	JUEL:A:욕정 += B*50
	PRINTFORML 욕정의 구슬＋{B*50}
ENDIF

IF D > 0
	JUEL:A:고통 += D*30
	PRINTFORML 고통의 구슬＋{D*30}
	JUEL:A:공포 += D*15
	PRINTFORML 공포의 구슬＋{D*15}
ENDIF
IF G > 0
	JUEL:A:불쾌 += G*50
	PRINTFORML 불쾌의 구슬＋{G*50}
ENDIF

IF D > 0 || G > 0 || E > 0 || F > 0
	JUEL:A:부정 += ((D+G+E+F)*100) /ABL:A:순종
	PRINTFORML 부정의 구슬＋{((D+G+E+F)*200) /ABL:A:순종}
ENDIF


;-----------------------------
;스트레스, 금욕 포인트
Q = 0
R = 0
;-----------------------------
;스트립 쇼
IF B == 1
	BASE:A:체력 -= 300
	Q = 1
	R = 30
;난교 라이브
ELSEIF B == 2
	BASE:A:체력 -= 500
	Q = 3
	R = 50
;SM쇼
ELSEIF B == 3
	BASE:A:체력 -= 800
	Q = 5
	R = 20
;고문 쇼
ELSEIF B == 4
	BASE:A:체력 -= 1000
	Q = 8
	R = 0
;수간쇼
ELSEIF B == 5
	BASE:A:체력 -= 1200
	Q = 10
	R = 10
ENDIF

;금욕 포인트
IF TEQUIP:A:정조대
	CFLAG:A:602 += R
	SIF TALENT:PLAYER:천리안
		PRINTFORML 금욕 포인트＋{R}
ELSE
	CFLAG:A:602 = 0
ENDIF

;스트레스치의 부가
CFLAG:A:10 += Q
SIF TALENT:PLAYER:천리안
	PRINTFORML 피로도＋{Q}


;-----------------------------
;보수액의 가산
;-----------------------------
;최후에 금액의 가산
PRINTFORMW 쇼의 수익으로 해서{S}고슈금을 얻었습니다
MONEY += S

DRAWLINE

;휴게 플래그를 세운다
FLAG:0 = 1
BEGIN TURNEND






;====================================================================
;플레이 내용의 판정
;====================================================================
@SEX_LIVE_PLAY_CHECK

CFLAG:A:160 = 0
CFLAG:A:161 = 0
CFLAG:A:162 = 0
CFLAG:A:163 = 0
CFLAG:A:164 = 0
CFLAG:A:165 = 0
CFLAG:A:166 = 0
CFLAG:A:167 = 0
CFLAG:A:168 = 0
CFLAG:A:169 = 0
CFLAG:A:170 = 0
CFLAG:A:171 = 0
CFLAG:A:172 = 0
CFLAG:A:173 = 0
CFLAG:A:174 = 0
CFLAG:A:175 = 0
CFLAG:A:176 = 0
CFLAG:A:177 = 0
CFLAG:A:178 = 0
CFLAG:A:179 = 0

;노예의수(LOCAL:1)공(LOCAL:2) 보존
IF B == 5
	LOCAL:1 = 1
ELSEIF B == 4
	IF ABL:A:새드끼 > ABL:A:마조끼
		LOCAL:2 = 1
	ELSEIF ABL:A:마조끼 > ABL:A:새드끼
		LOCAL:1 = 1
	ELSE
		IF !RAND:5
			LOCAL:2 = 1
		ELSE
			LOCAL:1 = 1
		ENDIF
	ENDIF
ELSE
	IF TALENT:A:공
		LOCAL:2 = 1
	ELSEIF TALENT:A:수 || TALENT:A:오메가
		LOCAL:1 = 1
	ELSE
		IF !RAND:5
			LOCAL:2 = 1
		ELSE
			LOCAL:1 = 1
		ENDIF
	ENDIF
ENDIF

;--------------------------------
;수간
;--------------------------------
SIF B == 5
	CFLAG:A:175 += 1

;--------------------------------
;자위
;--------------------------------
SIF B == 1 && (ABL:A:노출벽 >= 4 || ABL:A:자위중독 >= 2)
	CFLAG:A:162 += 1

;--------------------------------
;복수인 플레이
;--------------------------------
;노예수
IF B == 2 && LOCAL:1
	CFLAG:A:173 += 1
;노예공
ELSEIF B == 2 && LOCAL:2
	CFLAG:A:174 += 1
ENDIF

;--------------------------------
;SM계
;--------------------------------
;고문 쇼는 절대 하도SM
IF B == 4
	;노예 S
	IF LOCAL:2
		CFLAG:A:172 += 1
	;노예 M
	ELSE
		CFLAG:A:171 += 1
	ENDIF
;SM쇼는 옵션과 노예 S의 경우 노예의 S기분으로 변동
ELSEIF B == 3
	;옵션이 없으면 절대 소프트
	IF (FLAG:160 & 8) == 0
		IF LOCAL:2
			CFLAG:A:170 += 1
		ELSE
			CFLAG:A:169 += 1
		ENDIF
	;노예 S 때
	ELSEIF LOCAL:2
		;새드끼 4 이상 있으면 하드하게
		IF ABL:A:새드끼 >= 4
			CFLAG:A:172 += 1
		ELSE
			CFLAG:A:170 += 1
		ENDIF
	;노예 M 때
	ELSE
		;마조끼 4 이상 있다, 혹은 랜덤 1/5으로 하드하게
		IF ABL:A:마조끼 >= 4 || !RAND:4
			CFLAG:A:171 += 1
		ELSE
			CFLAG:A:169 += 1
		ENDIF
	ENDIF
ENDIF

;--------------------------------
;스카계
;--------------------------------
;식분플레이는 하도SM로, 노예 M로, 옵션 있을 때만 랜덤
SIF CFLAG:A:171 && FLAG:160 & 512 && !RAND:3
	CFLAG:A:179 += 1

;관장플레이는 하도SM계라면 절대 있다. 소프트라면 랜덤
SIF (B == 4 && !RAND:3) || (B == 3 && !RAND:5)
	CFLAG:A:178 += 1

;음뇨플레이는 하도SM로, 노예 M로, 옵션 있을 때 만
SIF CFLAG:A:171 && FLAG:160 & 128 && !RAND:3
	CFLAG:A:177 += 1

;방뇨 감상은 소프트 SM 때만 랜덤(관장가 있으면 절대)
SIF CFLAG:A:178 || (B == 3 && !RAND:3)
	CFLAG:A:176 += 1


;--------------------------------
;섹스계
;--------------------------------
;난교 라이브, SM쇼, 수간쇼
IF B == 2 || B == 3 || B == 5
	;노예공
	IF LOCAL:2
		;노예동정로 옵션이 없으면 안돼
		IF TALENT:A:동정 && (FLAG:160 & 4) == 0
		ELSE
			CFLAG:A:165 += 1
			;-----------------------
			;동정 먹어
			;-----------------------
			SIF TALENT:A:동정 && (FLAG:160 & 4)
				CFLAG:A:168 += 1
		ENDIF
	;노예수
	ELSE
		;바기나 없는, 혹은 노예처녀로 옵션이 없으면 안돼
		IF !VAGINA(A) || ((TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1) == 0)
		ELSE
			CFLAG:A:163 += 1
			;-----------------------
			;처녀간통
			;-----------------------
			SIF (TALENT:A:처녀 || TALENT:A:재생처녀) && (FLAG:160 & 1)
				CFLAG:A:166 += 1
		ENDIF
		
		;노예처녀로 옵션이 없으면 안돼
		;여자라면 랜덤 3/4이지만, 관장가 있었을 경우는 실전 있어
		IF (TALENT:A:백버진 && (FLAG:160 & 2) == 0) || (!CFLAG:A:178 && (VAGINA(A) && RAND:3 > 0))
		ELSE
			CFLAG:A:164 += 1
			;-----------------------
			;백버진간통
			;-----------------------
			SIF TALENT:A:백버진 && (FLAG:160 & 2)
				CFLAG:A:167 += 1
		ENDIF
	ENDIF
ENDIF

;--------------------------------
;성적 봉사는 난교 라이브, SM쇼, 수간쇼라면 절대 있다
;--------------------------------
IF B == 2 || B == 3 || B == 5
	;노예공
	IF LOCAL:2
		CFLAG:A:161 += 1
	;노예수
	ELSE
		CFLAG:A:160 += 1
	ENDIF
ENDIF



