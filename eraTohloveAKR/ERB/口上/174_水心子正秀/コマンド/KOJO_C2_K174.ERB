;=================================================
;SM계 커맨드 FIRSTTIME(240~249)
;=================================================
;-------------------------------------------------
;COM140,스팽킹 FIRSTTIME(240)
;-------------------------------------------------
@KOJO_COM_K174_140
;-------------------------------------------------
;기입 체크(=0,비표시,1,표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;지문 호출(칼라 변경 대응)
	CALL TRAIN_MESSAGE_COLOR
	
	;지하실
	IF TEQUIP:지하실
		;두려움
		IF (CFLAG:5005 & 64)
			SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「히,읏… 오후째,… 없음……,미안해요…!긋,우우……」
				CASE 0
					PRINTFORML 「아,아아!? 시,싫다… 힛…… 있고 ″,우,우─……」
			ENDSELECT
		;반발
		ELSEIF (CFLAG:5005 & 32)
			PRINTFORML 「읏,구″,어… 어이 그만두어읏……!읏아,구″… 우,우……」
		ELSE
			CALL PANT_VOICE_EX
		ENDIF
	ELSE
		;두려움
		IF (CFLAG:5005 & 64)
			;PRINTFORML 
		;반발
		ELSEIF (CFLAG:5005 & 32)
			;PRINTFORML 
		;마조
		ELSEIF (CFLAG:5005 & 256)
			;PRINTFORML 
		;그 이외
		ELSE
			;PRINTFORML 
		ENDIF
	ENDIF
ENDIF
RETURN LOCAL

