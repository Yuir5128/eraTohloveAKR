;-------------------------------------------------
;노예의 절정
;-------------------------------------------------
;기입 체크(=0,비표시,1,표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	PRINTL
	;지문 호출(칼라 변경 대응)
	CALL PALAM_MESSAGE_B_COLOR
	
	;두려움
	IF (CFLAG:5005 & 64)
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「… 얏……,아…,읏,아,읏……,이,… 읏」
			CASE 1
				PRINTFORML 「아…,이,힛……,아,아잇,아… 아……」
			CASE 0
				PRINTFORML 「구,읏…,우…… 웃,아…,야,,… 읏,응응앗…」
		ENDSELECT
		
	;절정도 4이상 랜덤
	ELSEIF TFLAG:29 >= 4 && RAND:2
		SELECTCASE RAND:6
			CASE 5
				PRINTFORML 「아앗,괴,… 굉장한 것,와… 룻,우… 응응…,읏,앗,히,아아아아!」
			CASE 4
				PRINTFORML 「읏,또,아앗아,읏히,아아아아앗…,앗,또,아…,읏,아아앗」
			CASE 3
				PRINTFORML 「아… 에,어…,다,째…… 응…,낫,읏앗… 야,앗,읏아아,읏!」
			CASE 2
				PRINTFORML 「아… 아아,아앗…… 도… 그만두어야… 해,죽으면…,읏!」
			CASE 1
				PRINTFORML 「히,읏아아아… 감색,… 앗,아,이… 도,얏… 째……」
			CASE 0
				PRINTFORML 「앗,야…,앙…… 이,하앗,그만두고…,머리,… 오,삼가 이만 줄입니다…,응우웃」
		ENDSELECT
		PRINTFORML %CALLNAME%는 생리적인 눈물로 얼굴을 뭉글뭉글하면서,쾌감에 몸부림치고 있다…
		
	;반발 녹고 혹은 반발+강절정
	ELSEIF (CFLAG:5005 & 128) || ((CFLAG:5005 & 32) && TFLAG:29 >= 2)
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「도… 하,,…… 읏,키,엣…,응아,아……,아,아…」
			CASE 1
				PRINTFORML 「아,싫다…,그만두고……,읏,아…… 아,아아앗」
			CASE 0
				PRINTFORML 「이,읏……,아,잇,기… 싶고,…… 낫,야,읏,아앗」
		ENDSELECT
		IF TFLAG:29 >= 4
			PRINTFORML %CALLNAME%는 생리적인 눈물로 얼굴을 뭉글뭉글하면서,덮쳐 오는 쾌감의 물결에 몸부림치고 있다…
		ELSE
			PRINTFORML %CALLNAME%는 덮쳐 오는 쾌감의 물결에,열심히 참고 있다…
		ENDIF
		
	;반발
	ELSEIF (CFLAG:5005 & 32)
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「우…,… 읏응,응…… 우,응응,응…… 이……,웃,응읏……」
			CASE 1
				PRINTFORML 「응…,우……,아… 응……,응우우우,긋…,응…」
			CASE 0
				PRINTFORML 「아……,우,읏응우… 우,구,우우우……,읏,응우…」
		ENDSELECT
		PRINTFORML %CALLNAME%는 덮쳐 오는 쾌감의 물결에 이를 악물어,열심히 참고 있다…
		
	;녹고 강 혹은 강절정
	ELSEIF (CFLAG:5005 & 16) || TFLAG:29 >= 2
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「얏…,아……,앗히,응읏… 이… 읏,아… 아,아아앗」
			CASE 1
				PRINTFORML 「거기…,아,이…,있고…,아…… 도,이,우……」
			CASE 0
				PRINTFORML 「(이)다,아…,아아… 안돼…,이,라고,에… 응,아…… 하앗,읏,이…」
		ENDSELECT
		IF TFLAG:29 >= 4
			PRINTFORML %CALLNAME%는 생리적인 눈물로 얼굴을 뭉글뭉글하면서,쾌감에 몸부림치고 있다…
		ELSE
			PRINTFORML %CALLNAME%는 생리적인 눈물을 흘리면서,쾌감에 허덕이고 있다…
		ENDIF
		
	;녹고 미만 혹은 절정
	ELSEIF (CFLAG:5005 & 8) || TFLAG:29
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「도,우……,읏,아,아,얏…… 도,응,이…,라고…」
			CASE 1
				PRINTFORML 「아… 아,하… 아,아아… 도,…… 읏,이,좋다…」
			CASE 0
				PRINTFORML 「아,읏………,이,읏,…,우… 응응……」
		ENDSELECT
		PRINTFORML %CALLNAME%는 쾌감에 허덕이고 있다…
	ENDIF
	
	;발정 이벤트
	IF (CFLAG:5004 & 4)
		;페니스가 있는 경우는사정를 수반하는 경우에 금욕 포인트가 줄어든다
		;여기에서는 돌아간다
		SIF PENIS(TARGET)
			RETURN LOCAL
		
		;페니스가 없는지,아이사이즈의 경우는 C절정나 V절정로 금욕 포인트가 줄어든다
		IF (!PENIS(TARGET) || TALENT:아이사이즈) && (NOWEX:0 || NOWEX:1) && (EX:0 == NOWEX:0) && (EX:1 == NOWEX:1)
			PRINTL 
			PRINTFORML 「하아…… 하아…… 아하……」
			PRINTFORML 애타게 기다린 절정에,%CALLNAME%는 행복하게 여운에 잠겨,뺨을 느슨해지게 하고 있다…
		ENDIF
	ENDIF
	
	PRINTFORMW 
	RETURN LOCAL
ENDIF


