;19,비디오 매각시
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_19
;-------------------------------------------------
;기입 체크(=0,비표시,1,표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;미함락 연모친애(사모 대상 묻지 않고)
	IF !KANRAKU(TARGET) || KANRAKU2(TARGET) == 1
		PRINTFORMW 「그 『로』등은 어떻게 할 생각이야?」
		PRINTFORMW 「에엣,팔아!?」
		PRINTFORMW 「…… 한번 더 다시 생각할 생각은 없는가?」
		
	;하트풀음란
	ELSEIF ((CFLAG:5001 & 1) && TALENT:음란)
		PRINTFORMW 「그 『로』등은 어떻게 할 생각이야?」
		PRINTFORML 「에엣,팔아!?」
		PRINTFORMW 「아아…… 나의 치태가 흩뿌려져 버린다고 하는 일이나 그것도 꽤……」
		PRINTFORMW %CALLNAME%는 느슨해진 얼굴로 뭔가 투덜투덜 말하고 있지만이,이쪽을 눈치채자마자 제 정신이 되었다
		PRINTFORMW 「핫…… 응읏!뭐 잔돈으로는 되는 것이 아닌가」
	ENDIF
	
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
	
ENDIF
RETURN LOCAL

;-------------------------------------------------
;24,비디오촬영후,팔지 않고 보관한다고 했을 때
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_24
;-------------------------------------------------
;기입 체크(=0,비표시,1,표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;미함락 반발 하트풀음란
	IF !KANRAKU(TARGET) || (CFLAG:5005 & 32) || ((CFLAG:5001 & 1) && TALENT:음란)
		PRINTFORML 「그 『로』등은 방금전의 광경이 비치고 있었구나」
		PRINTFORML 「이상한 일에는 사용하는 것이 아니다」
		;하트풀음란
		IF (CFLAG:5001 & 1) && TALENT:음란
			PRINTFORML 이상한 일은 구체적으로는,이라고 (들)물어 보았다
			PRINTFORML 「에,그것은…… 이,이상한 일은 이상한 일이다!그 이상도 이하도 아니다!」
		ENDIF
	ENDIF
	
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
ENDIF
RETURN LOCAL

;-------------------------------------------------
;25,의상 변경
;T:A = Y　　　　착탈하는 의상을 한정할 때
;A = X　　　　　착탈 부위 전체를 나타낼 때
;TEQUIP:A = Y　 지금 무엇을 입고 있을까(입지 않은 경우는 0)를 짜맞춘다
;벗기는,덮어 씌우는 것에 관련되지 않고,특정의 편성의 의상이 되었을 때의 기술예에 알몸신발하 넣어 둡니다
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_25
;-------------------------------------------------
;벗어 벌에 관계없이 특정의 의상 편성이 되었을 때 예
;기입 체크(=0,비표시,1,표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && !Y
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;신발하or발 봉투인 만큼 했을 때
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:1 && TEQUIP:0 == 0 && TEQUIP:2 == 0 && TEQUIP:3 == 0 && TEQUIP:4 == 0 && TEQUIP:5 == 0 && TEQUIP:6 == 0
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
ENDIF
;-------------------------------------------------
;덮어 씌웠을 때
;기입 체크(=0,비표시,1,표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && Y
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;정조대를 붙인다
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && T:2 == 5
		SIF TALENT:연모 || TALENT:친애
			PRINTFORML 「이러한 기구를 붙이지 않아도,\@(CFLAG:5001 &1) ? 사#사\@는 청마의외로는……」
		
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;속옷을 붙인다
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && T:2 > 0
		
		RETURN LOCAL
	ENDIF
	
	
	;-------------------------------------------------
	;세일러 복＆스커트로 했을 때
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:4 == 1 && TEQUIP:5 == 2
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;여장
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP_FEMALE(TARGET) && MALE(TARGET)
		PRINTFORML 「이것은……,여성의 옷이 아닌가」
		PRINTFORML %CALLNAME%는 입술을 뾰족하게 하고 있다
		RETURN LOCAL
	ENDIF
	
	
ENDIF


;-------------------------------------------------
;76,의상 변경
;T:A = Y　　　　착탈하는 의상을 한정할 때
;A = X　　　　　착탈 부위 전체를 나타낼 때
;TEQUIP:A = Y　 지금 무엇을 입고 있을까(입지 않은 경우는 0)를 짜맞춘다
;벗기는,덮어 씌우는 것에 관련되지 않고,특정의 편성의 의상이 되었을 때의 기술예에 알몸신발하 넣어 둡니다
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_K174_76
;-------------------------------------------------
;벗겼을 때
;기입 체크(=0,비표시,1,표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && Y
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;정조대를 제외한다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && A == 2 && T:2 == 5
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;정조대 이외의 속옷을 제외한다
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && A == 2 && T:2!= 5
		
		IF PALAM:윤활 >= PALAMLV:4
			PRINT 벗겼다
			CALL CLOTHES_NAME
			PRINTFORML 로부터는\@VAGINA(TARGET)? 애액#앞질러 해\@가 실과 같이 방울져 떨어졌다
		ELSEIF PALAM:윤활 >= PALAMLV:3
			PRINTFORM 벗겼다
				CALL CLOTHES_NAME
			PRINTFORML 는,\@VAGINA(TARGET)? 애액#앞질러 해\@로 습기를 띠고 있었다
		ENDIF
		RETURN LOCAL
	ENDIF
ENDIF

