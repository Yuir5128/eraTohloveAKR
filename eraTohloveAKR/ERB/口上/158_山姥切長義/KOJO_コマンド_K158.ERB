
;==================================================
;@KOJO_MESSAGE_COM 관계(X를 캐릭터 번호에 치환)
;커맨드 실행시에 출력됩니다
;==================================================
@KOJO_MESSAGE_COM_K158
;조수가 조교했을 때에대사을 스킵 한다(기호에 응해 사용하는, 줄머리의 것;을 지우면 스킵 하게 된다)
;SIF ASSI >= 0 && ASSIPLAY
;	RETURN 0
;볼개그착용시에는 지문을 표시해대사을 스킵 한다
IF TEQUIP:볼개그 &&  SELECTCOM != 107
	CALL TRAIN_MESSAGE
	RETURN 0
ENDIF

;붕괴도 스킵
IF TALENT:붕괴
	CALL TRAIN_MESSAGE
	RETURN 0
ENDIF

;부르는 법 판정
;음란·창부 붕괴 미함락
IF CFLAG:40 == 0 && !TALENT:실의
	CSTR:51 = %NAME_CALL_158(PLAYER)%
;조교자가 사모·공포 대상 이외
ELSEIF KANRAKU(TARGET) == 4 || KANRAKU(TARGET) == 5
	CSTR:51 = %NAME_CALL_158(PLAYER)%
ELSE
	CSTR:51 = %CSTR:50%
ENDIF

;감사관 플레이 평정
IF CFLAG:5005 & 1
	CALL SCORING_COM
	CFLAG:5006 += RESULT
	PRINTFORML 스코아:{CFLAG:5006} 점
	PRINTL
ENDIF
