;==================================================
;특정의 조건을 채우고 있는 경우,커맨드를 실행할 때에 소스가 추가
;노출벽라든지마조끼라든지 취미에 맞은 커맨드나 상태로체력·기력회복하는 사양에.
;마조끼똥 높은데 곧기절 하는 것 어떻게든 하고 싶었던 것으로 테스트.
;==================================================
;봉사계의 커맨드 실행시에체력/uA력회복.
IF ABL:봉사정신 >= 3
	SELECTCASE SELECTCOM
		;수몸봉사는체력회복
		CASE 70,71,73 TO 80,85 TO 87,317,420,421,422
			BASE:0 += ABL:봉사정신*2
		;공봉사는기력회복
		CASE 220 TO 225,260,261,263,264,318
			BASE:1 += ABL:봉사정신*2
		;체력기력양쪽 모두 회복한다
		CASE 243 TO 245,319,503,504
			BASE:0 += ABL:봉사정신*2
			BASE:1 += ABL:봉사정신*2
	ENDSELECT
ENDIF

;옷을 입지 않았다고기력회복,노출플레이중은체력회복.
IF ABL:노출벽 >= 3
	;전라 루트
	IF !TEQUIP:전신상의 && !TEQUIP:상의 && !TEQUIP:하의 && !TEQUIP:속옷（위） && !TEQUIP:속옷（아래） && !TEQUIP:특수2 && !TEQUIP:특수1
		BASE:1 += ABL:노출벽*2
	;반나체 루트
	ELSEIF (TEQUIP:전신상의 || (TEQUIP:상의 && TEQUIP:하의)) && !TEQUIP:속옷（위） && !TEQUIP:속옷（아래）
		BASE:1 += ABL:노출벽*2
	ENDIF
	SIF TEQUIP:비디오카메라 || TEQUIP:야외플레이 || TEQUIP:수치플레이 || SELECTCOM == 3
		BASE:0 += ABL:노출벽*2
ENDIF

;구속되고 있으면기력회복.
;고통커맨드 실행시에체력회복.
IF ABL:마조끼 >= 3
	SIF TEQUIP:밧줄 || TEQUIP:아이마스크 || TEQUIP:볼개그 || TEQUIP:정조대 || TEQUIP:사정관리 || TEQUIP:전신상의 == 9
		BASE:1 += ABL:마조끼*2
	SELECTCASE SELECTCOM
		CASE 16,25,44,56,62,140 TO 155
			BASE:0 += ABL:마조끼*2
	ENDSELECT
ENDIF

;노예공커맨드 실행시에체력/uA력회복.
IF ABL:새드끼 >= 3
	SIF TEQUIP:신발 > 0 && SELECTCOM == 80
		BASE:0 += ABL:새드끼*2
	SIF ABL:PLAYER:마조끼 >= 3 && (SELECTCOM == 226 || SELECTCOM == 227)
		BASE:0 += ABL:새드끼*2
		BASE:1 += ABL:새드끼*2
	SIF SELECTCOM >= 226 && SELECTCOM <= 227
		BASE:1 += ABL:새드끼*2
ENDIF

;자위를 하면체력·기력회복.
IF ABL:자위중독 >= 3
	SELECTCASE SELECTCOM
		CASE 4,5,6,7
			BASE:1 += ABL:자위중독*2
	ENDSELECT
ENDIF

;정액 마셔체력회복.
;정액을 받을 수 있을 것 같은 커맨드 실행시는기력회복.
IF ABL:정액중독 >= 3
	SIF TFLAG:0 == 0 && TFLAG:43 == 13
		BASE:0 += ABL:정액중독*2
	SELECTCASE SELECTCOM
		CASE 70 TO 75,226,227,407,420,421,516
			BASE:1 += ABL:정액중독*2
		CASE 410,412
			BASE:0 += ABL:정액중독*2
	ENDSELECT
ENDIF

;섹스 하면체력·기력회복. 음란√라면 특수섹스에서도 회복.
IF ABL:섹스중독 >= 3
	IF TALENT:음란 || TALENT:창부
		SELECTCASE SELECTCOM
			CASE 322,381,405,406,430,431,511 TO 515,517 TO 525
				BASE:0 += ABL:섹스중독*2
				BASE:1 += ABL:섹스중독*2
		ENDSELECT
	ENDIF
	SELECTCASE SELECTCOM
		CASE 100 TO 116,260 TO 265,507 TO 510,526
			BASE:0 += ABL:섹스중독*2
			BASE:1 += ABL:섹스중독*2
	ENDSELECT
ENDIF

;스카계 커맨드 실행으로체력회복.
;변의/mA의가 있을 때기력회복.
IF ABL:스캇중독 >= 3
	SIF BASE:4 >= 60 || BASE:6 >= 60
		BASE:1 += ABL:스캇중독*2
	SELECTCASE SELECTCOM
		CASE 182 TO 185,285,286,302,304
			BASE:0 += ABL:스캇중독*2
	ENDSELECT
	;움직이는지 모르지만 일단 들어갈 수 있어 본다
	SIF TFLAG:58 == 1 || TFLAG:68 == 1
		BASE:0 += ABL:스캇중독*2
ENDIF


;==================================================
;랜덤소대사
;==================================================
;절정 회수
IF (EX:0 + EX:1 + EX:2 + EX:3 >= 20)
	PRINTFORMW 20회 이상의 절정을 거쳐,%CALLNAME%는 머리의 나사가 날아가 버린 것 같다
	PRINTFORMW 야무지지 못하게 얼굴을 느슨해지게 해 말씨가 돌지 않는입조로 멋없는 허덕임을 누설하고 있다
ELSEIF (EX:0 + EX:1 + EX:2 + EX:3 >= 10) && (EX:0 + EX:1 + EX:2 + EX:3 < 20)  && MARK:반발각인!= 3
	PRINTFORMW 절정을 몇번이나 맛보여져%CALLNAME%는 숨도 끊어질듯 끊어질듯이 허덕이고 있다
ENDIF


;-------------------------------------------------
;커맨드 실행시의 대사 CFLAG 300~500
;실행 조건은 자료 폴더내,커맨드 실행 조건 파일을 참조
;-------------------------------------------------

;=================================================
;기본계 커맨드
;=================================================
;-------------------------------------------------
;0,애무 CFLAG:300
;-------------------------------------------------
IF SELECTCOM == 0
	CALL TRAIN_MESSAGE
	;처음
	IF CFLAG:300 == 0
		IF NO:MASTER == 0 || ((ASSI > 0 && ASSIPLAY) && NO:ASSI == 0)
			PRINTFORMW 「무엇이다,대장. 간지럽지만」
		ELSE
			PRINTFORMW 「무엇이다,간지럽지만」
		ENDIF
		CFLAG:300 = 1
	;2회째 이후
	ELSE
		;함락이 끝난 상태
		IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:300 < 4 || FLAG:7 == 2)
			;연모,친애
			IF TALENT:3 || TALENT:6
				SIF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
					PRINTFORMW 「대장에게 손대어지는 것 좋아한다. 기분이 좋고 기분도 좋은%UNICODE(0x2661)%」
			;음란,창부
			ELSEIF TALENT:4 || TALENT:7
				PRINTFORMW 
			;복종,예속
			ELSEIF TALENT:5 || TALENT:8
				PRINTFORMW 
			ENDIF
			CFLAG:300 = 4
		;굴복각인Lv3
		ELSEIF MARK:2 == 3 && (CFLAG:300 < 3 || FLAG:7 == 2)
			IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
				IF RAND:2 == 0
					PRINTFORMW 「후,조금,기다려 줘나 대장. 그래,그런 곳까지 손댈까,……%UNICODE(0x2661)%」
				ELSE
					PRINTFORMW 「대장의손,기분이 좋구나」
				ENDIF
			ELSEIF ASSI > 0 && ASSIPLAY
				PRINTFORMW 「후아…… 읏!」
			ELSE
				PRINTFORMW 「하아…… 아아,아……」
			ENDIF
			CFLAG:300 = 3
		;그 이외
		ELSEIF CFLAG:300 < 2 || FLAG:7 == 2
			PRINTFORMW 「그만두어라고,간질여」
		CFLAG:300 = 2
		ENDIF
	ENDIF
ENDIF

;-------------------------------------------------
;1,키스한다 CFLAG:301
;-------------------------------------------------
IF SELECTCOM == 1
	CALL TRAIN_MESSAGE
	;처음
	IF CFLAG:301 == 0
		PRINTFORMW 「응,구…… 후핫. 이봐,입안까지 빤다 라고어떨까라고 생각하겠어」
		CFLAG:301 = 1
	;2회째 이후
	ELSE
		;굴복각인Lv3
		IF MARK:2 == 3 && (CFLAG:301 < 3 || FLAG:7 == 2)
			IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
				PRINTFORMW 「%UNICODE(0x2661)% 도미 실마리%UNICODE(0x2661)%%UNICODE(0x2661)%」
			ELSE
				PRINTFORMW %CALLNAME%는부족해 있고 산소를 보충하도록(듯이) 어깨로 숨을 쉬고 있다……
			ENDIF
		ELSEIF CFLAG:301 < 2 || FLAG:7 == 2
			PRINTFORMW 「응우……」
		ENDIF
		CFLAG:301 = 2
	ENDIF
ENDIF

;-------------------------------------------------
;2,아무것도안한다 CFLAG:302
;-------------------------------------------------
IF SELECTCOM == 2
	CALL TRAIN_MESSAGE
	IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
		IF PALAM:욕정 > 30000
			PRINTFORMW 「도미 실마리,부탁하겠어,그만두면 싫다……!」
			PRINTFORMW %CALLNAME%는욕정로 상기 한 얼굴을 %CALLNAME:PLAYER%에 향하여\@KANRAKU(TARGET) ? 행위 # 조교\@의 계속을 간원 하고 있다……
		ELSE
			PRINTFORMW 「어이쿠,휴게인가. 백탕이나 차라도 준비할까?」
		ENDIF
	ELSE
		PRINTFORMW 「휴게인가」
	ENDIF
ENDIF

;-------------------------------------------------
;3,비패개장 CFLAG:303
;-------------------------------------------------
IF SELECTCOM == 3
	CALL TRAIN_MESSAGE
	;처음
	IF CFLAG:303 == 0
		;심신자
		IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
			;연모,친애
			IF TALENT:3 || TALENT:6
				;처녀
				IF TALENT:0
					PRINTFORML 「뭐,미토루 아가씨이니까,예쁜 것일 것이다」
					PRINTFORM %CALLNAME%는 어린 균열을 열어 미개발의 질입를
						IF TALENT:161 == 0 && ABL:A감각 > 3
						PRINTFORML 쬐었다.
						PRINTFORMW 「…… 여기가 이러하지 미토루 아가씨라는 것도 뭐 하지만…… 후뿐이 아니고 코코에도 대장의 모양 기억하게 해 주어라」
						PRINTFORM 더욱 편손를 늘려,아누스에도 손가락을 반입 양쪽 모두의 구멍을
						ENDIF
					PRINTFORMW 넓혀 잘 보이도록 허리를 내민다.
					PRINTFORMW 「이봐,이봐요. 재빨리 가라앉혀 주어라,도미 실마리%UNICODE(0x2661)%」
				;비처녀
				ELSE
					;나카타씨가 끝난 상태
					IF CFLAG:TARGET:102 > 0
						PRINTFORML 「이런,넓히면 대장의 종 흘러넘쳐 버린다……」
						PRINTFORML 「후,그 만큼 충분히 따라 줄 것이다? ,도미 실마리%UNICODE(0x2661)%」
					ELSE
						PRINTFORMW 「나는 어디에서라도 대장의 것이니까,좋아할 뿐(만큼) 봐 주어도 좋아」
						PRINTFORMW 「…… 하지만,보는 것만으로 끝이라는 일은 없구나?」
					ENDIF
				ENDIF
			;연모,친애 이외
			ELSE
				PRINTFORMW 
			ENDIF
		;심신자 이외
		ELSE
			PRINTFORMW 
		ENDIF
		CFLAG:303 = 1
	;2회째 이후
	ELSE
		;함락이 끝난 상태
		IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:303 < 4 || FLAG:7 == 2)
			;연모,친애
			IF TALENT:3 || TALENT:6
				IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
					;처녀
					IF TALENT:0
						IF TEQUIP:로터삽입
							PRINTFORMW %CALLNAME%가 균열을 넓히면,떨리는로터를 들여다 보게 한 작은 질입로부터 녹진녹진 꿀이 흘러넘쳤다.
							PRINT 「이봐,이런 질척질척 이다.
								SIF (TALENT:MASTER:거근 && EXP:V경험 >= 120) || (TALENT:MASTER:말자지 && EXP:V경험 >= 180)
								PRINT 이제(벌써) 대장의 큰 것 돌진해도 괜찮다고.
							PRINTFORMW 보고 있는 것만으로 좋은 것인지,대장?」
						ELSE
							IF RAND:2 == 0
								PRINTFORMW 「대장,슬슬 나치노처녀 먹어 주지 않을까?」
								PRINTFORMW 닫은 음순을 나누어 펼쳐,비부[秘部]를 쬐면서%CALLNAME%는 불만스러운 듯이입를 날카롭게 되어지고 있다.
							ELSE
								PRINTFORMW 「도미 실마리,보류가 지나면 여기로부터 덮쳐 버리겠어?」
								PRINTFORMW 발를 M자에 열려,가랑이의 상하로부터 양손로 균열을 넓히면서,%CALLNAME%는 권하도록(듯이) 추잡하게 허리를 흔들고 있다……
							ENDIF
						ENDIF
					ELSE
						;나카타씨가 끝난 상태
						IF CFLAG:TARGET:102 > 0
							PRINTFORML 「응……%UNICODE(0x2661)% 도미 실마리의,흘러넘쳐들%UNICODE(0x2661)%」
							PRINTFORML 「아하,추잡해? 흥분해? 그러면,좀 더 충분히 안에 대장의 자식 주어라%UNICODE(0x2661)%」
						ELSE
							PRINTFORMW 「응? 코코는 뭐든지? 대장 기분 좋게 해 나도 기분 좋아지는 구멍이겠지%UNICODE(0x2661)%」
							PRINTFORMW 「보는 것만으로 좋은 것인지,도미 실마리%UNICODE(0x2661)% 여기라도 기다리기 어려워 하고 있는 것이다?」
						ENDIF
					ENDIF
				ELSE
					PRINTFORMW 
				ENDIF
			;음란,창부
			ELSEIF TALENT:4 || TALENT:7
				PRINTFORMW 
			;복종,예속
			ELSEIF TALENT:5 || TALENT:8
				PRINTFORMW 
			ENDIF
		CFLAG:303 = 4
		;굴복각인Lv3
		ELSEIF MARK:2 == 3 && (CFLAG:303 < 3 || FLAG:7 == 2)
			PRINTFORMW 
		CFLAG:303 = 3
		;그 이외
		ELSEIF CFLAG:303 < 2 || FLAG:7 == 2
			PRINTFORMW 
		ENDIF
		CFLAG:303 = 2
	ENDIF
ENDIF

;-------------------------------------------------
;4,자위 CFLAG:304
;-------------------------------------------------
IF SELECTCOM == 4
	CALL TRAIN_MESSAGE
	;처음
	IF CFLAG:304 == 0
		PRINTFORMW 「내가 혼자서 하는 곳 보여라라는? 좋은 취미 하고 있구나?」
		CFLAG:304 = 1
	;2회째 이후
	ELSE
		;함락이 끝난 상태
		IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:304 < 4 || FLAG:7 == 2)
			;연모,친애
			IF TALENT:3 || TALENT:6
				IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
					;애널바이브나바이브＋A감각LV3 이상이나 V감각LV2
					IF (TEQUIP:애널바이브 || TEQUIP:바이브) && (ABL:A감각 >= 3 || ABL:V감각 >= 2)
						PRINTFORMW 「아아…%UNICODE(0x2661)% (이)나,대장,나,완구로 가는 것,도,싫다고……!」
						PRINTFORMW %CALLNAME%는 그렇게 애원 하면서도 허리를 흔들어,바이브를 빼고 꽂기 이바지하는손는 멈출 수 없는 같다……
					ELSEIF TEQUIP:로터삽입 || TEQUIP:로터A삽입
						;처녀
						IF TALENT:0
							IF RAND:2 == 0
								PRINTFORMW 「후하,도미 실마리,이것으로 좋은가?」
								PRINTFORMW %CALLNAME%는 명해지는 대로,반 뽑아낸로터를손를 사용하지 않고 다시 삼켜 보였다
							ELSE
								PRINTFORMW 「아%UNICODE(0x2661)% 아,야,너무 보지마,대장%UNICODE(0x2661)%」
								PRINTFORMW 크게 가랑이를 열게 하고 있기 (위해)때문에,%CALLNAME%가 물소리를 세워 만지작거리는\@TALENT:남자? 음경 # 좋은 곳\@도,
								PRINTFORMW 흠칫 몸이 떨릴 때에 문로터가 튀어나오지 않는 모양 큐우큐우 잡고 있는\@TEQUIP:로터삽입? 질입 # 국혈\@도 자주(잘) 보인다……
							ENDIF
						;비처녀
