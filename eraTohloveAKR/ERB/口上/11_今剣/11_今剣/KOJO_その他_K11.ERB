;-------------------------------------------------
;사정개소 변경시의 텍스트 유효시는 여기서 함수 종료 CFLAG:221~230
;-------------------------------------------------
IF CFLAG:106
	CALL PALAM_MESSAGE_A
	;머리카락
	IF CFLAG:106 == 1
		PRINTFORMW 「우우─,끈적끈적 합니다……」
		CFLAG:221 = 1
	;안
	ELSEIF CFLAG:106 == 2
		PRINTFORMW 「째를 열 수 없어요─」
		CFLAG:222 = 1
	;가슴
	ELSEIF CFLAG:106 == 3
		CFLAG:223 = 1
	;복
	ELSEIF CFLAG:106 == 4
		CFLAG:224 = 1
	;액
	ELSEIF CFLAG:106 == 5
		CFLAG:225 = 1
	;허벅지
	ELSEIF CFLAG:106 == 6
		CFLAG:226 = 1
	;균열
	ELSEIF CFLAG:106 == 7
		CFLAG:227 = 1
	;고
	ELSEIF CFLAG:106 == 8
		CFLAG:228 = 1
	;고무
	ELSEIF CFLAG:106 == 9
		;섹스로사정 한편,애널섹스 이외를 실행시
		IF TFLAG:2 > 0 && SELECTCOM != 110 && SELECTCOM != 111 && SELECTCOM != 112 && SELECTCOM != 113 && SELECTCOM != 114 && SELECTCOM != 115 && SELECTCOM != 116
			CFLAG:229 = 1
		ELSE
			CFLAG:230 = 1
		ENDIF
	ENDIF
	RETURN 0
ENDIF

;-------------------------------------------------
;손,입 이외로사정 CFLAG:231
;TFLAG:9 == 1은사정,2는 대량사정
;대응 커맨드는 키스,스마타,풋잡,애널강제 봉사,더블스마타
;-------------------------------------------------
IF TFLAG:9 == 1 || TFLAG:9 == 2
	CALL PALAM_MESSAGE_A
	CFLAG:231 = 1
ENDIF

;-------------------------------------------------
;조수를범한다로사정 CFLAG:232
;-------------------------------------------------
IF TFLAG:7 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;입로사정 CFLAG:233
;-------------------------------------------------
IF TFLAG:0 > 0
	CALL PALAM_MESSAGE_A
	;함락이 끝난+정액중독 혹은봉사정신
	IF (ABL:봉사정신 >= 4 || ABL:정액중독 >= 2) && KANRAKU(TARGET) && (CFLAG:233 < 3 || FLAG:7 == 2)
		CFLAG:233 = 3
	;함락이 끝난 상태
	ELSEIF KANRAKU(TARGET) && (CFLAG:233 < 2 || FLAG:7 == 2)
		PRINTFORMW 「응쿠,하아……」
		PRINTFORMW %CALLNAME%는 넋을 잃고입의 구석으로부터 흘러넘친 백탁을 혀가 아닌 잡았다
		CFLAG:233 = 2
	;그 이외
	ELSEIF CFLAG:233 < 1 || FLAG:7 == 2
		PRINTFORML 「웃…… 구,」
		PRINTFORMW 정액을 토해내려고 한%CALLNAME%의입를,%CALLNAME:PLAYER%는손로 가려 무리하게에 삼키게 했다
		CFLAG:233 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;손로사정 CFLAG:234
;-------------------------------------------------
IF TFLAG:1 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:234 = 1
ENDIF

;-------------------------------------------------
;가슴로사정 CFLAG:235
;-------------------------------------------------
IF TFLAG:34 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:235 = 1
ENDIF

;-------------------------------------------------
;발로사정 CFLAG:236
;-------------------------------------------------
IF TFLAG:16 > 0
	CALL PALAM_MESSAGE_A
	CFLAG:236 = 1
ENDIF

;-------------------------------------------------
;섹스로사정 CFLAG:237 CFLAG:238
;-------------------------------------------------
IF TFLAG:2 > 0
	CALL PALAM_MESSAGE_A
	;애널섹스계
	IF SELECTCOM == 110 || SELECTCOM == 111 || SELECTCOM == 112 || SELECTCOM == 113 || SELECTCOM == 114 || SELECTCOM == 115 || SELECTCOM == 116
		IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
			PRINTFORMW 「… 뜨,뜨거운 것이,(안)중에…,응응%UNICODE(0x2661)%」
		ELSE
			PRINTFORML 「우,아…… 하아…… 뜨겁다……」
		ENDIF
		CFLAG:237 = 1
	;섹스계
	ELSE
		IF NO:MASTER == 0 || (ASSI > 0 && NO:ASSI == 0)
			IF PALAM:욕정 > 20000
				PRINTFORML 「후아앗,주인님%UNICODE(0x2661)% ,아앗,아아아앗!!」
				PRINTFORMW 「뜨거운 것이…… 뜨거운 것이 배에 가득%UNICODE(0x2661)% 나,넘쳐 버린다…… 하아,하아……%UNICODE(0x2661)%」
			ELSE
				PRINTFORMW 「아,아%UNICODE(0x2661)% 　중,나오고 있다…… 두,둘 때까지……%UNICODE(0x2661)%」
				PRINTFORMW 태내의 최안쪽에 교배감촉에%CALLNAME%는 전신을 진동시키고 있다……
			ENDIF
		ELSE
			PRINTFORMW 「아,아%UNICODE(0x2661)% 　중,나오고 있다…… 두,둘 때까지……%UNICODE(0x2661)%」
			PRINTFORMW 태내의 최안쪽에 교배감촉에%CALLNAME%는 전신을 진동시키고 있다……
		ENDIF
		CFLAG:238 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;역섹스로사정 시킨 CFLAG:239
;-------------------------------------------------
