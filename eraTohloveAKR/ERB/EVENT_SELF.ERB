;-----------------------------------------------
;아침풋잡
;-----------------------------------------------
@SELF_MORNING_5
DRAWLINE
A = 0

;새드끼로 가산 Lv3로 1
;원래 코코에 오는 시점에서 3있으므로 최저 1
A += (ABL:새드끼 - 2)
;도착적
SIF TALENT:도착적
	A += 1
;새드
SIF TALENT:새드
	A += 2
;소악마
SIF TALENT:소악마
	A += 1
;주인이【마조】
SIF  TALENT:MASTER:마조 == 1
	A += 5
;연모
SIF TALENT:연모 || TALENT:친애
	A += 1
;음란
SIF TALENT:음란
	A += 2
;창부
SIF TALENT:창부
	A += 4
;반발는 3있으면 안돼
SIF MARK:반발각인 >= 3
	RETURN 0
;조건을 채우지 않았다
SIF A < 1
	RETURN 0

DRAWLINE


;대사의 호출
TFLAG:13 = 30
CALL KOJO_MESSAGE_EVENT

;남자의 경우
IF MALE(TARGET)
	EXP:가학쾌락경험 += A
	PRINTFORML %CALLNAME%의 %EXPNAME:32%＋{A}
	;주인도 남자라면, 게이경험가산
	IF MALE(TARGET) && MALE(MASTER)
		EXP:게이경험 += A
		PRINTFORML %EXPNAME:41%＋{A}
	ENDIF
	;주인이아이사이즈가 아닐 때, 조교 대상으로정액경험, 주인에게사정경험추가
	;주인에게고통쾌락경험추가
	;습득, 욕정의 주획득
	;아이사이즈가 아니다
	IF TALENT:MASTER:아이사이즈 == 0
		EXP:정액경험 += A/2
		EXP:MASTER:사정경험 += A/2
		;금욕 포인트 줄인다
		CFLAG:MASTER:602 = 0
		PRINTFORML %CALLNAME%의 %EXPNAME:20%＋{A/2}
		PRINTFORML %CALLNAME:MASTER%의 %EXPNAME:3%＋{A/2}
	ENDIF
	EXP:MASTER:고통쾌락경험 += A/4
	JUEL:욕정 += A*20
	JUEL:습득 += A*70
	PRINTFORML %CALLNAME:MASTER%의 %EXPNAME:30%＋{A/4}
	PRINTFORML %PALAMNAME:6%의 구슬＋{A*20}
	PRINTFORML %PALAMNAME:8%의 구슬＋{A*70}
	DRAWLINE
ELSE
	EXP:가학쾌락경험 += A
	PRINTFORML %CALLNAME%의 %EXPNAME:32%＋{A}
	;주인도 여자라면, 레즈경험가산
	IF  MALE(TARGET) == 0 && MALE(MASTER) == 0
		EXP:레즈경험 += A
		PRINTFORML %EXPNAME:40%＋{A}
	ENDIF
	;주인에게고통쾌락경험추가
	;습득, 욕정의 주획득
	EXP:MASTER:고통쾌락경험 += A/4
	JUEL:욕정 += A*20
	JUEL:습득 += A*70
	PRINTFORML %CALLNAME:MASTER%의 %EXPNAME:30%＋{A/4}
	PRINTFORML %PALAMNAME:6%의 구슬＋{A*20}
	PRINTFORML %PALAMNAME:8%의 구슬＋{A*70}
	DRAWLINE
ENDIF
RETURN 0


