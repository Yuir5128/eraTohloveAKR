@SELF_MORNING_1
DRAWLINE
A = 0
TFLAG:13 = 1
CALL KOJO_MESSAGE_EVENT

;정액중독
IF ABL:정액중독 == 0
	A = 1
ELSEIF ABL:정액중독 == 1
	A = 2
ELSEIF ABL:정액중독 == 2
	A = 3
ELSEIF ABL:정액중독 == 3
	A = 4
ELSEIF ABL:정액중독 == 4
	A = 5
ELSEIF ABL:정액중독 >= 5
	A = 6
ENDIF

;악취둔감 더러움무시
SIF TALENT:악취둔감 || TALENT:더러움무시
	A += 1
;악취민감
SIF TALENT:악취민감
	A -= 1
;헌신적
SIF TALENT:헌신적
	A += 1
;발정기
SIF TALENT:발정기
	A += 1
;Pavlov 카운터
CALL SEIIN_CHECK
;정액절정경험
SIF EXP:정음절정경험 > P
	A += 3

SIF A == 0
	A = 1

EXP:펠라경험 += A
;아이사이즈가 아니다
SIF TALENT:MASTER:아이사이즈 == 0
	EXP:정액경험 += A/2
JUEL:순종 += A*100
JUEL:굴복 += A*30
JUEL:습득 += A*40
PRINTFORML %EXPNAME:22%＋{A}
;아이사이즈가 아니다
SIF TALENT:MASTER:아이사이즈 == 0
	PRINTFORML %EXPNAME:20%＋{A/2}
;정액절정경험
IF EXP:정음절정경험 > P
	PRINTFORML %EXPNAME:24%＋1
	EXP:정음절정경험 += 1
ENDIF
PRINTFORML %PALAMNAME:5%의 구슬＋{A*100}
PRINTFORML %PALAMNAME:7%의 구슬＋{A*30}
PRINTFORMW %PALAMNAME:8%의 구슬＋{A*40}
DRAWLINE

RETURN 0


