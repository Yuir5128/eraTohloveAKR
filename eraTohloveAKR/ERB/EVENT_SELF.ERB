;SELF.ERB, 노예로부터의 봉사 따위의 이벤트
;-------------------------------------------------
;아침에 일어나는 이벤트
;-------------------------------------------------
@EVENT_NEWDAY_SELF
;조교 대상이 없는 경우는 종료
SIF TARGET < 0
	RETURN 0
;전체 판정, 함락 소질이 없는 경우 혹은반발각인가 있을 때는 함수 종료
SIF (TALENT:연모 == 0 && TALENT:음란 == 0 && TALENT:복종 == 0 && TALENT:친애 == 0 && TALENT:창부 == 0 && TALENT:예속 == 0) || MARK:반발각인
	RETURN 0
;기절 하고 있으면 함수 종료
SIF TALENT:기절
	RETURN 0

;아침 이벤트 캔슬 플래그가 서있는 경우는 종료
SIF CFLAG:42
	RETURN 0

VARSET LOCAL
LOCAL:1 = 10
;이벤트 결정
FOR LOCAL, 1, 10
	CALLFORM SELF_MORNING_ABLE{LOCAL}
	IF RESULT
		LOCAL:(LOCAL:1) = LOCAL
		LOCAL:1 += 1
	ENDIF
NEXT
LOCAL:1 -= 10
SIF LOCAL:1 == 0
	RETURN 0
LOCAL:2 = LOCAL:(RAND:(LOCAL:1) + 10)
IF LOCAL:2 == 7 || LOCAL:2 == 8
	CALLFORM MORNING_EVENT_{LOCAL:2}
ELSE
	CALLFORM SELF_MORNING_{LOCAL:2}
ENDIF

