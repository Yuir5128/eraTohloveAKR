;-----------------------------------------------
;아침기승위
;-----------------------------------------------
@SELF_MORNING_3
DRAWLINE
A = 0
TFLAG:13 = 3

CALL KOJO_MESSAGE_EVENT

;원이 남자, 혹은 V보다 A가 높으면 A우선
IF MALE2(TARGET) || ABL:V감각 < ABL:A감각
	;다만, A를 사용할 수 없는 경우는 V의 사용 가부를 본다
	IF ABL:A감각 < 3 || TALENT:백버진
		;V를 사용할 수 있으면 V를 사용하려고 한다
		IF VAGINA(TARGET) && !TALENT:처녀 && !TALENT:재생처녀 && ABL:V감각 >= 3
			;그 위에서 V도정조대로 메워지고 있으면정조대대사을 불러낸다
			IF TEQUIP:속옷（아래） == 5
				TFLAG:67 = 0
			;메워지지 않으면 V를 사용한다
			ELSE
				TFLAG:67 = 1
			ENDIF
		ENDIF
	;A를 사용할 수 있으면정조대 관계없이 A를 사용한다
	ELSE
		TFLAG:67 = 2
	ENDIF
;원이 여자라면 V우선
ELSE
	;정조대가 있는 경우, A의 사용 가부를 본다
	IF TEQUIP:속옷（아래） == 5
		;A도 사용할 수 없으면, 정조대대사을 불러낸다
		IF ABL:A감각 < 3 || TALENT:백버진
			TFLAG:67 = 0
		;A를 사용할 수 있으면 A를 사용한다
		ELSE
			TFLAG:67 = 2
		ENDIF
	;원래 V를 사용할 수 없는 경우는 A의 사용 가부를 본다
	ELSEIF TALENT:처녀 || TALENT:재생처녀 || ABL:V감각 < 3
		;A를 사용할 수 있으면 A를 사용한다
		SIF ABL:A감각 >= 3 && !TALENT:백버진
			TFLAG:67 = 2
	;V를 사용할 수 있으므로 V를 사용한다
	ELSE
		TFLAG:67 = 1
	ENDIF
ENDIF

;정조대의 경우, 이 이후의 부분은 읽지 않는다
SIF TFLAG:67 == 0
	RETURN 0

;정액중독
IF ABL:정액중독 == 0
	A = 1
ELSEIF ABL:정액중독 == 1
	A = 1
ELSEIF ABL:정액중독 == 2
	A = 2
ELSEIF ABL:정액중독 == 3
	A = 2
ELSEIF ABL:정액중독 == 4
	A = 3
ELSEIF ABL:정액중독 >= 5
	A = 3
ENDIF

;섹스중독
IF ABL:섹스중독 == 0
	A += 1
ELSEIF ABL:섹스중독 == 1
	A += 2
ELSEIF ABL:섹스중독 == 2
	A += 3
ELSEIF ABL:섹스중독 == 3
	A += 4
ELSEIF ABL:섹스중독 == 4
	A += 5
ELSEIF ABL:섹스중독 >= 5
	A += 6
ENDIF

;음란구멍
SIF TALENT:음란구멍
	A += 2
;항문광
SIF TALENT:항문광
	A += 2
;섹스광
SIF TALENT:섹스광
	A += 3
;헌신적
SIF TALENT:헌신적
	A += 1
;발정기
SIF TALENT:발정기
	A += 1

SIF A == 0
	A = 1

;A사용의 경우
IF TFLAG:67 == 2
	EXP:A경험 += A
	EXP:성교경험 += A
	;아이사이즈가 아니다
	SIF TALENT:MASTER:아이사이즈 == 0
		EXP:정액경험 += A/2
	IF CFLAG:108 == 0
		EXP:장내사정경험 += A/2
		CFLAG:702 += A/2
	ENDIF
	JUEL:쾌A += A*100
	JUEL:순종 += A*100
	JUEL:굴복 += A*30
	JUEL:습득 += A*40
	PRINTFORML %EXPNAME:1%＋{A}
	PRINTFORML %EXPNAME:4%＋{A}
	;아이사이즈가 아니다
	SIF TALENT:MASTER:아이사이즈 == 0
		PRINTFORML %EXPNAME:20%＋{A/2}
	SIF CFLAG:702 > 0 && CFLAG:108 == 0
		PRINTFORML %EXPNAME:14%+{A/2}
	PRINTFORML %PALAMNAME:2%의 구슬＋{A*20}
	PRINTFORML %PALAMNAME:5%의 구슬＋{A*100}
	PRINTFORML %PALAMNAME:7%의 구슬＋{A*30}
	PRINTFORMW %PALAMNAME:8%의 구슬＋{A*40}
	DRAWLINE
ELSEIF TFLAG:67 == 1
	EXP:V경험 += A
	EXP:성교경험 += A
	;아이사이즈가 아니다
	SIF TALENT:MASTER:아이사이즈 == 0
		EXP:정액경험 += A/2
	IF CFLAG:108 == 0
		EXP:질내사정경험 += A/2
		CFLAG:102 += A/2
	ENDIF
	JUEL:쾌V += A*100
	JUEL:순종 += A*100
	JUEL:굴복 += A*30
	JUEL:습득 += A*40
	PRINTFORML %EXPNAME:0%＋{A}
	PRINTFORML %EXPNAME:4%＋{A}
	;아이사이즈가 아니다
	SIF TALENT:MASTER:아이사이즈 == 0
		PRINTFORML %EXPNAME:20%＋{A/2}
	SIF CFLAG:102 > 0 && CFLAG:108 == 0
		PRINTFORML %EXPNAME:5%＋{A/2}
	PRINTFORML %PALAMNAME:1%의 구슬＋{A*20}
	PRINTFORML %PALAMNAME:5%의 구슬＋{A*100}
	PRINTFORML %PALAMNAME:7%의 구슬＋{A*30}
	PRINTFORMW %PALAMNAME:8%의 구슬＋{A*40}
	DRAWLINE
ENDIF

EXP:MASTER:성교경험 += A
EXP:MASTER:삽입경험 += A


RETURN 0

