;-------------------------------------------------
;COM201,선물 FIRSTTIME(301)
;무엇을 주고 있을까? 하,커맨드 폴더내선물의 항목에 들어가 있습니다
;스스로 주는 것을 결정하고 싶은 경우,CALL TRAIN_MESSAGE_COLOR를 지워,스스로 쓴다
;-------------------------------------------------
@KOJO_COM_KX1_201
;-------------------------------------------------
;기입 체크(=0,비표시,1,표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;지문 호출(칼라 변경 대응)
	CALL TRAIN_MESSAGE_COLOR
	
	;웃는 얼굴
	IF TFLAG:17 == 1
	;음식
	ELSEIF TFLAG:17 == 2
	;일용품
	ELSEIF TFLAG:17 == 3
	;악세사리
	ELSEIF TFLAG:17 == 4
	;복
	ELSEIF TFLAG:17 == 5
	;고가의 물건
	ELSEIF TFLAG:17 == 6
	;특별한선물(Extra 모드 엔딩용)
	ELSEIF TFLAG:17 == 7
		;인게이지 링
		IF CFLAG:5100 == 1
		;오더 메이드바이브
		ELSEIF CFLAG:5101 == 1
		;오더 메이드 목걸이
		ELSEIF CFLAG:5102 == 1
		ENDIF
	;경장
	ELSEIF TFLAG:17 == 11
	;프로미스 링
	ELSEIF TFLAG:17 == 50
	ENDIF
ENDIF
RETURN LOCAL

