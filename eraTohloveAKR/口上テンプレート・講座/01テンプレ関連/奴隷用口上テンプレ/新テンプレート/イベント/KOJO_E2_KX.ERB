;-------------------------------------------------
;25,의상 변경
;T:A = Y　　　　착탈하는 의상을 한정할 때
;A = X　　　　　착탈 부위 전체를 나타낼 때
;TEQUIP:A = Y　 지금 무엇을 입고 있을까(입지 않은 경우는 0)를 짜맞춘다
;벗기는,덮어 씌우는 것에 관련되지 않고,특정의 편성의 의상이 되었을 때의 기술예에 알몸신발하 넣어 둡니다
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_KX1_25
;-------------------------------------------------
;벗어 벌에 관계없이 특정의 의상 편성이 되었을 때 예
;기입 체크(=0,비표시,1,표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && !Y
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;신발하or발 봉투인 만큼 했을 때
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:1 && TEQUIP:0 == 0 && TEQUIP:2 == 0 && TEQUIP:3 == 0 && TEQUIP:4 == 0 && TEQUIP:5 == 0 && TEQUIP:6 == 0
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
ENDIF
;-------------------------------------------------
;덮어 씌웠을 때
;기입 체크(=0,비표시,1,표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && Y
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;정조대를 붙인다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && T:2 == 5
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;세일러 복＆스커트로 했을 때
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:4 == 1 && TEQUIP:5 == 2
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
ENDIF


;-------------------------------------------------
;76,의상 변경
;T:A = Y　　　　착탈하는 의상을 한정할 때
;A = X　　　　　착탈 부위 전체를 나타낼 때
;TEQUIP:A = Y　 지금 무엇을 입고 있을까(입지 않은 경우는 0)를 짜맞춘다
;벗기는,덮어 씌우는 것에 관련되지 않고,특정의 편성의 의상이 되었을 때의 기술예에 알몸신발하 넣어 둡니다
;-------------------------------------------------
@KOJO_MESSAGE_EVENT_KX1_76
;-------------------------------------------------
;벗겼을 때
;기입 체크(=0,비표시,1,표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && !Y
	;지문 호출(칼라 변경 대응)
	CALL EVENT_MESSAGE_COLOR
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;정조대를 제외한다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && A == 2 && T:2 == 5
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;정조대 이외의 속옷을 제외한다
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && A == 2 && T:2!= 5
		PRINTFORMW 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
ENDIF

